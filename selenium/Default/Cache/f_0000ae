};\n\n\n\n// WEBPACK FOOTER //\n// ./src/packages/address-typeahead/src/components/SuggestionList.js","import React from 'react';\nimport PropTypes from 'prop-types';\nimport classnames from 'classnames';\n\nexport default function SearchInput(props) {\n  const {\n    error,\n    onSearchInputChange,\n    inputOverrides,\n    label,\n    labelOverrides,\n    onSearchBlur,\n    onSearchFocus,\n    preventAutoComplete,\n    searchInputText,\n    touched,\n  } = props;\n\n\n  return (\n    <div\n      className={\n        classnames(\n          'no-overflow clearfix ncss-input-container',\n          'animatedForm ncss-col-md-6 ncss-col-sm-12',\n          'mod-ncss-input-container mb2-sm mb4-md va-sm-t mb3-sm'\n        )}\n    >\n      <div\n        className={\n          classnames(\n            'no-overflow clearfix ncss-input-container animatedForm',\n            touched && error ? 'error' : '',\n            'mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t',\n            'mb3-sm',\n          )}\n      >\n        <input\n          aria-invalid={!!error || undefined}\n          aria-label={error || undefined}\n          autoCapitalize={'false'}\n          autoComplete={preventAutoComplete ? 'off' : 'address-line1'}\n          className={'ncss-input pt2-sm pr4-sm pb2-sm pl4-sm fs-exclude'}\n          id={'search-address-input'}\n          maxLength={250}\n          onBlur={onSearchBlur}\n          onChange={onSearchInputChange}\n          onFocus={onSearchFocus}\n          required\n          type={'text'}\n          value={searchInputText}\n          {...inputOverrides}\n        />\n\n        <label\n          id={'search-input-label'}\n          htmlFor={'search-address-input'}\n          className=\"ncss-label ncss-label-text pr6-sm\"\n          {...labelOverrides}\n        >\n          <i className={'glyph g72-search'} />\n          {' '}\n          {label}\n        </label>\n        <span className={'search-address-input-error ncss-error-msg'}>\n          {error}\n        </span>\n      </div>\n    </div>\n  );\n}\n\nSearchInput.defaultProps = {\n  preventAutoComplete: false,\n};\n\nSearchInput.propTypes = {\n  id: PropTypes.string,\n  error: PropTypes.bool,\n  onSearchInputChange: PropTypes.func,\n  inputOverrides: PropTypes.shape({}),\n  label: PropTypes.string,\n  labelOverrides: PropTypes.shape({}),\n  onSearchBlur: PropTypes.func,\n  onSearchFocus: PropTypes.func,\n  preventAutoComplete: PropTypes.bool,\n  searchInputText: PropTypes.string,\n  touched: PropTypes.bool,\n};\n\n\n\n// WEBPACK FOOTER //\n// ./src/packages/address-typeahead/src/components/SearchInput.js","export const LOQATE_BASE = 'https://api.addressy.com';\nexport const INTERACTIVE_FIND = '/Capture/Interactive/Find/v1.10/json3.ws';\nexport const INTERACTIVE_RETRIEVE = '/Capture/Interactive/Retrieve/v1.00/json3ex.ws';\n\nexport const pageActionName = 'tread-address-typeahead-event';\nexport const FIND_ERROR_FALLBACK_MESSAGE = 'Error during FIND/address';\n\nexport const DEFAULT_ADDRESS_API_KEY = 'ZP52-HY96-TA14-YD58';\nexport const DEFAULT_COUNTRIES = ['US'];\nexport const DEFAULT_LIMIT = 5;\nexport const DEFAULT_LANGUAGE = 'en';\n\n\n\n// WEBPACK FOOTER //\n// ./src/packages/ux-tread-address-locate/src/constants.js","export const DEFAULT_MESSAGES = {\n  label: 'Start typing the first line of your address',\n  invalidAddress: 'Please enter a valid address.',\n  incompleteSubmission: 'Please complete address selection or enter an address manually.',\n  enterAddressManually: 'Enter address manually',\n  keepTypingAddress: 'Keep typing your address to display more results',\n};\n\nexport const COUNTRY_FORM_MAP = {\n  type2: [\n    'DE',\n    'FR',\n    'PT',\n    'GR',\n    'LU',\n    'CZ',\n    'HU',\n    'SI',\n    'DK',\n    'FI',\n    'SE',\n    'AT',\n    'PL',\n    'CH',\n    'NO',\n    'ES',\n    'IT',\n    'NL',\n    'BE',\n  ],\n};\n\nexport const DEFAULT_SEARCH_INPUT_ID = 'search-address-input';\n\n\n\n// WEBPACK FOOTER //\n// ./src/packages/address-typeahead/src/constants.js","import React, { Component } from 'react';\nimport PropTypes from 'prop-types';\nimport throttle from 'lodash.throttle';\nimport get from 'lodash.get';\nimport { findAddresses, retrieveAddress } from '../../../ux-tread-address-locate/src';\nimport normalizeFindAddressResponse from '../normalizers/findAddressNormalizer';\nimport normalizeRetrieveAddressResponse from '../normalizers/retrieveAddressNormalizer';\nimport SafeAnchor from '../../../../checkout/view/components/SafeAnchor';\nimport SuggestionList from './SuggestionList';\nimport { DEFAULT_MESSAGES, DEFAULT_SEARCH_INPUT_ID } from '../constants';\nimport SearchInput from './SearchInput';\n\nexport default class AddressSuggestComponent extends Component {\n  constructor(props, context) {\n    super(props, context);\n\n    this.componentRef = React.createRef(); // Detects user leaving component\n    this.listRef = React.createRef(); // Used to scroll list to first entry after a tiered search\n\n    this.state = {\n      componentHasFocus: false, // component is in use or did user click away\n      searchInputActive: false, // search focus flag\n      searchInputText: '',\n      showSuggestComponent: true, // true-using suggestions, false-manual entry/clicked suggestion\n      suggestions: [],\n      touchmove: false,\n    };\n\n    this.catchMouseDown = this.catchMouseDown.bind(this);\n    this.catchScreenScroll = this.catchScreenScroll.bind(this);\n    this.catchTouchend = this.catchTouchend.bind(this);\n    this.closeSuggestionInput = this.closeSuggestionInput.bind(this);\n    this.handleOptOutClick = this.handleOptOutClick.bind(this);\n    this.onBlur = this.onBlur.bind(this);\n    this.onSearchFocus = this.onSearchFocus.bind(this);\n    this.onSearchInputChange = this.onSearchInputChange.bind(this);\n    this.onSelectSuggestion = this.onSelectSuggestion.bind(this);\n    this.requestSuggestions = this.requestSuggestions.bind(this);\n\n    /*\n     Using throttle here instead of debounce since, imo, the consumer\n     expectation is that the results should update while they type and\n     a tightly configured debounce might create an experience where a\n     faster typer would not see results until they stop typing. Throttle gives\n     us a middle ground where they still have the ui changing as they type\n     but reduce the total number of api calls made.\n    */\n    this.throttledRequestSuggestions = throttle(this.requestSuggestions, 700);\n  }\n\n  componentDidMount() {\n    if (this.props.isTouchDevice) {\n      document.body.addEventListener('touchmove', this.catchScreenScroll);\n      document.body.addEventListener('touchend', this.catchTouchend);\n    } else {\n      document.body.addEventListener('mousedown', this.catchMouseDown);\n    }\n  }\n\n  componentWillUnmount() {\n    this.removeListeners();\n  }\n\n  /*\n  onBlur currently handles\n    1) Set error state flag: touched, empty search input blurred\n    2) Override errors flag true: search blurred but user moved focus to a dropdown element\n    3) Override errors flag false: user blurred component\n  */\n\n  onBlur(event) {\n    const state = {};\n    const { componentHasFocus, searchInputText } = this.state;\n    const searchInputBlurred = event.currentTarget.id === this.props.searchInputId;\n    // Check for blurred touched empty search input and set error flag\n    if (searchInputBlurred) {\n      state.invalid = !searchInputText;\n      state.searchInputActive = false;\n    }\n\n    // Override errors if search was blurred but user moved focus to a suggestion element\n    if (searchInputBlurred && componentHasFocus) {\n      state.suppressError = true;\n    }\n\n    // If component lost focus then remove any error overrides\n    if (!componentHasFocus) {\n      state.suppressError = false;\n    }\n\n    this.setState(state);\n  }\n\n  onSearchFocus() {\n    this.setState({\n      componentHasFocus: true,\n      invalid: false,\n      searchInputActive: true,\n      touched: true,\n    });\n\n    /*\n    On mobile, scroll search input to top so that suggestion list can be fully viewed.\n    Appears to be working correctly on iOS.\n    NOTE: For the future, an animated scroll would be kinder to the user.\n    */\n    if (this.props.isMobile) {\n      const ref = this.componentRef.current;\n      // Gets document.body scroll position which ranges from 0 to a negative number\n      const bodyTopLeft = document.body.getBoundingClientRect().top * -1;\n      // Gets distance from top of device screen to top of component\n      const componentTopFromViewPort = ref && ref.getBoundingClientRect().top;\n      // Adds a little whitespace at the top\n      const scrollTo = (bodyTopLeft + componentTopFromViewPort) - 10;\n      window.scrollTo(0, scrollTo);\n    }\n  }\n\n  /**\n   * Handles clicking/touching suggestions,\n   * also handles calling the next level down if the address selected is multi-level\n   * @param {object} e - Event object\n   */\n  onSelectSuggestion(e = { currentTarget: {}, preventDefault: () => {} }) {\n    e.preventDefault();\n    const id = e.currentTarget.value;\n    const type = e.currentTarget.getAttribute('data-suggestion-type');\n    // Triggering Tiered Search\n    if (type !== 'Address') {\n      this.props.onSelectViewMoreAddresses();\n      this.throttledRequestSuggestions(\n        this.state.searchInputText, id, true, this.props.country, this.props.language\n      );\n    // Selecting Address\n    } else {\n      this.setState({\n        componentHasFocus: false,\n      });\n\n      retrieveAddress({\n        id,\n      }).then((response) => {\n        const normalizedResponse = normalizeRetrieveAddressResponse(response);\n        this.setSuggestedAddress(normalizedResponse);\n      });\n    }\n  }\n\n\n  onSearchInputChange(e) {\n    const searchInputText = e.target.value;\n    const { searchInputText: prevSearchText, suggestions } = this.state;\n    const { country, language } = this.props;\n\n    // User changes search input text - hit api\n    if (searchInputText && searchInputText !== prevSearchText) {\n      this.setState(() => ({\n        searchInputText,\n      }));\n      const trimmedSearchText = searchInputText.trim();\n      this.throttledRequestSuggestions(trimmedSearchText, null, false, country, language);\n\n      // User cleared search input of previous value - clear state\n    } else if (!searchInputText && (suggestions.length || prevSearchText)) {\n      this.setState({\n        suggestions: [],\n        searchInputText: '',\n      });\n    }\n  }\n\n  /**\n   * Calls handleAddressSelected Prop\n   * Closes the suggestion form\n   * @param {object} address - normalized address from loqate\n   */\n  setSuggestedAddress(address) {\n    this.props.handleAddressSelected(address);\n    this.closeSuggestionInput();\n  }\n\n  removeListeners() {\n    if (this.props.isTouchDevice) {\n      document.body.removeEventListener('touchmove', this.catchScreenScroll);\n      document.body.removeEventListener('touchend', this.catchTouchend);\n    } else {\n      document.body.removeEventListener('mousedown', this.catchMouseDown);\n    }\n  }\n\n  // This catches the destination of the users mouse click on desktop and\n  // determines if the component is losing focus\n  catchMouseDown(event) {\n    const componentHasFocus =\n      event.target.id !== 'addressSuggestionOptOut' &&\n      this.componentRef.current.contains(event.target);\n    if (componentHasFocus !== this.state.componentHasFocus) {\n      this.setState({ componentHasFocus });\n    }\n  }\n\n  /*\n  Used to differentiate scrolling the screen vs tapping on it.\n\n  If you are testing this in iOS simulator you will sometimes see\n  that blurring the component via tap fails to close the search list.\n  This occurs because the user moves the mouse an imperceptible amount,\n  enough to fire a touchmove and have the logic consider this a screen scroll.\n\n  This could possibly be addressed by using touchstart to record initial\n  viewport positioning and then touchend for end viewport position and get\n  rid of the touchmove event listener.\n  */\n  catchScreenScroll() {\n    if (!this.state.touchmove) {\n      this.setState({ touchmove: true });\n    }\n  }\n\n  /*\n  Used to track where the user lifts their finger from the screen. In combination\n  with tracking touchmove events we can tell if the user tapped the screen or\n  merely scrolled it.\n  */\n  catchTouchend(event) {\n    // If we previously trapped a move event then the user was sliding their finger\n    // and this is not a focus change and we can ignore.\n    const state = { touchmove: false }; // Reset touchmove flag for next track\n    const isTap = !this.state.touchmove;\n\n    if (isTap) {\n      state.componentHasFocus = this.componentRef.current.contains(event.target);\n    }\n\n    if (Object.keys(state).some(key => state[key] !== this.state[key])) {\n      this.setState(state);\n    }\n  }\n\n  requestSuggestions(text, containerId = null, scrollToTop = false, country, language) {\n    findAddresses({\n      text,\n      container: containerId,\n      countries: country,\n      language,\n    }).then((response) => {\n      // normalize\n      const normalizedAddresses = normalizeFindAddressResponse(response);\n      this.setState(\n        () => ({ suggestions: normalizedAddresses.Items }),\n        // Scroll to top of list (if this is a tiered search the list scroll position\n        // will remain unchanged where the user had scrolled to before clicking)\n        () => {\n          if (scrollToTop) {\n            get(this, 'listRef.current.childNodes[0].childNodes[0]', {}).scrollTop = 0;\n          }\n        },\n      );\n    });\n  }\n\n  /**\n   * Wrapper for clicking opt out to close the component\n   *\n   */\n  handleOptOutClick() {\n    this.props.handleOptOut();\n    this.closeSuggestionInput();\n  }\n\n  closeSuggestionInput() {\n    this.setState({\n      showSuggestComponent: false,\n    });\n    this.removeListeners();\n  }\n\n  render() {\n    const {\n      componentHasFocus,\n      invalid,\n      searchInputActive,\n      searchInputText,\n      showSuggestComponent,\n      suggestions,\n      suppressError,\n      touched,\n    } = this.state;\n\n    const {\n      allowOptOut,\n      inputOverrides,\n      label,\n      labelOverrides,\n      messages,\n      searchInputId,\n      submitFailed,\n    } = this.props;\n\n    // Blurred input with no entry\n    const blankInputError = !suppressError && invalid && touched;\n\n    //  Form submitted with no search input entry\n    const blankSubmitError =\n      !suppressError && submitFailed && !searchInputActive && !searchInputText;\n\n    //  Form submitted with search text but no selection\n    const noSelectError = !suppressError && submitFailed && !searchInputActive && searchInputText;\n\n    // Checking searchInputText length here prevents user from repeatedly deleting chars quickly,\n    // triggering a search as the input value changes but clearing the input totally before the api\n    // returns a response and leaving the component with no search input text but a array of\n    // address suggestions\n    const showSuggestionsList = componentHasFocus && suggestions.length && searchInputText.length;\n\n    let error;\n    if (blankInputError || blankSubmitError) {\n      error = messages.invalidAddress;\n    } else if (noSelectError) {\n      error = messages.incompleteSubmission;\n    }\n    return (\n      <div ref={this.componentRef}>\n        <div className={`${showSuggestComponent ? '' : 'd-sm-h'} ncss-col-sm-12 autoSuggestOuter`}>\n          <div\n            className={`\n              autoSuggestSearchInput\n              ${componentHasFocus ? 'forceBorder' : ''}\n              ${blankInputError || blankSubmitError ? 'invalid' : ''}\n              ${noSelectError ? 'incomplete' : ''}`\n            }\n          >\n            <SearchInput\n              error={error}\n              inputOverrides={inputOverrides}\n              label={label}\n              labelOverrides={labelOverrides}\n              onSearchBlur={this.onBlur}\n              onSearchFocus={this.onSearchFocus}\n              onSearchInputChange={this.onSearchInputChange}\n              searchInputId={searchInputId}\n              searchInputText={searchInputText}\n              touched={touched}\n            />\n          </div>\n\n          <div className={`${showSuggestionsList ? '' : 'd-sm-h'} suggestionListWrapper`}>\n            <div className=\"suggestionList\" ref={this.listRef}>\n              <SuggestionList\n                suggestions={suggestions}\n                onSuggestionClick={this.onSelectSuggestion}\n                onSuggestionBlur={this.onBlur}\n                onClose={this.handleOptOutClick}\n                messages={messages}\n                allowOptOut={allowOptOut}\n              />\n            </div>\n          </div>\n          { allowOptOut &&\n            <div className=\"mt1-sm mb2-sm mb4-md va-sm-t pl4-sm\">\n              <SafeAnchor\n                role=\"button\"\n                id=\"addressSuggestionOptOut\"\n                aria-label={messages.enterAddressManually}\n                className=\"ncss-btn text-color-grey ncss-base p0-sm u-underline\"\n                onClick={this.handleOptOutClick}\n              >\n                {messages.enterAddressManually}\n              </SafeAnchor>\n            </div>\n          }\n        </div>\n      </div>\n    );\n  }\n}\nAddressSuggestComponent.defaultProps = {\n  allowOptOut: false,\n  label: DEFAULT_MESSAGES.label,\n  searchInputId: DEFAULT_SEARCH_INPUT_ID,\n  messages: { ...DEFAULT_MESSAGES },\n  handleOptOut: () => {},\n  handleAddressSelected: () => {},\n  onSelectViewMoreAddresses: () => {},\n};\n\nAddressSuggestComponent.propTypes = {\n  /**\n   * Boolean to allow users to enter address manually (Shows Message, adds handler)\n   * defaults to false\n   */\n  allowOptOut: PropTypes.bool,\n  /**\n   * Country to default search to address (area), also defines response structure\n   */\n  country: PropTypes.string,\n  /**\n   * function that will be called when a user selects an address\n   */\n  handleAddressSelected: PropTypes.func,\n  /**\n   * function that will be called if a user opts out\n   */\n  handleOptOut: PropTypes.func,\n  /**\n   * Is the device a mobile device (helps with touch/scroll events)\n   */\n  isMobile: PropTypes.bool,\n  /**\n   * Same as the check for mobile (helps with touch/scroll events)\n   */\n  isTouchDevice: PropTypes.bool,\n  /**\n   * Search input label, defaults to DEFAULT_MESSAGES.label\n   */\n  label: PropTypes.string,\n  /**\n   * Language that the results for address should be returned in\n   */\n  language: PropTypes.string,\n  /**\n   * Messages, an object with keys that should be overriden based on the language presented to the\n   * user, defaults to matching keys in DEFAULT_MESSAGES\n   */\n  messages: PropTypes.shape({\n    enterAddressManually: PropTypes.string,\n    incompleteSubmission: PropTypes.string,\n    invalidAddress: PropTypes.string,\n  }),\n  /**\n   * A function that allows consuming applications to customize what happens when this is clicked\n   */\n  onSelectViewMoreAddresses: PropTypes.func,\n  /**\n   * Add to or override the search <input /> element's attributes\n   */\n  inputOverrides: PropTypes.shape({}),\n  /**\n   * Add to or override the search <label /> element's attributes\n   */\n  labelOverrides: PropTypes.shape({}),\n  /**\n   * Search Input Id to allow for multiples on a page you can override this here\n   * (that way events fire correctly)\n   */\n  searchInputId: PropTypes.string,\n  /**\n   * Handle if a full form had a submission failure\n   */\n  submitFailed: PropTypes.bool,\n};\n\n\n\n// WEBPACK FOOTER //\n// ./src/packages/address-typeahead/src/components/AddressSuggest.js","import PropTypes from 'prop-types';\nimport React from 'react';\nimport { connect } from 'react-redux';\nimport {\n  getShippingMode,\n  getPaymentMode,\n  getProductsMode,\n  getPricingMode,\n  getOrderReviewMode,\n  getOrderConfirmationMode,\n  getPostDataMode,\n  getTunnelMode,\n  getCurrentView,\n} from '../../selectors';\nimport getCookieValue from '../../helpers/getCookieValue';\n\nexport function CurrentModesComponent(props) {\n  const {\n    shippingMode,\n    paymentMode,\n    pricingMode,\n    productsMode,\n    orderReviewMode,\n    orderConfirmationMode,\n    postDataMode,\n    tunnelMode,\n    currentView,\n  } = props;\n  if (getCookieValue('viewModes') !== '1') { return null; }\n  const views = [\n    { name: 'Shipping', mode: shippingMode, key: 'SHIPPING' },\n    { name: 'Payment', mode: paymentMode, key: 'PAYMENT' },\n    { name: 'Review', mode: orderReviewMode, key: 'ORDER_REVIEW' },\n    { name: 'Confirm', mode: orderConfirmationMode, key: 'ORDER_CONFIRMATION' },\n    { name: 'Pricing', mode: pricingMode, key: '' },\n    { name: 'Products', mode: productsMode, key: '' },\n    { name: 'PostData', mode: postDataMode, key: 'POST_DATA' },\n    { name: 'Tunnel', mode: tunnelMode, key: 'TUNNEL' },\n  ];\n  return (\n    <div\n      className=\"bg-white\"\n      style={{\n        position: 'fixed',\n        right: 0,\n        bottom: 0,\n        zIndex: 2147483647,\n        cellPadding: 2,\n      }}\n    >\n      <table className=\"border-medium-grey\">\n        <tbody>\n          { views.map(view =>\n            <tr\n              className={currentView === view.key ? 'u-bold' : ''}\n              key={`current-modes-${view.name}`}\n            >\n              <td className=\"ta-sm-r pl1-sm pr1-sm\">{ view.name }</td>\n              <td className=\"pl1-sm pr1-sm\">{ view.mode }</td>\n            </tr>\n          )}\n        </tbody>\n      </table>\n    </div>\n  );\n}\n\nCurrentModesComponent.propTypes = {\n  shippingMode: PropTypes.string,\n  paymentMode: PropTypes.string,\n  pricingMode: PropTypes.string,\n  productsMode: PropTypes.string,\n  orderReviewMode: PropTypes.string,\n  orderConfirmationMode: PropTypes.string,\n  postDataMode: PropTypes.string,\n  tunnelMode: PropTypes.string,\n  currentView: PropTypes.string,\n};\n\nexport function mapStateToProps(state) {\n  return {\n    shippingMode: getShippingMode(state),\n    paymentMode: getPaymentMode(state),\n    pricingMode: getPricingMode(state),\n    productsMode: getProductsMode(state),\n    orderReviewMode: getOrderReviewMode(state),\n    orderConfirmationMode: getOrderConfirmationMode(state),\n    postDataMode: getPostDataMode(state),\n    tunnelMode: getTunnelMode(state),\n    currentView: getCurrentView(state),\n  };\n}\n\nexport default connect(mapStateToProps)(CurrentModesComponent);\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/containers/CurrentModes.js","module.exports = [\n  {\n    name: 'Alabama',\n    label: 'Alabama',\n    abbreviation: 'AL',\n  },\n  {\n    name: 'Alaska',\n    label: 'Alaska',\n    abbreviation: 'AK',\n  },\n  {\n    name: 'Arizona',\n    label: 'Arizona',\n    abbreviation: 'AZ',\n  },\n  {\n    name: 'Arkansas',\n    label: 'Arkansas',\n    abbreviation: 'AR',\n  },\n  {\n    name: 'California',\n    label: 'California',\n    abbreviation: 'CA',\n  },\n  {\n    name: 'Colorado',\n    label: 'Colorado',\n    abbreviation: 'CO',\n  },\n  {\n    name: 'Connecticut',\n    label: 'Connecticut',\n    abbreviation: 'CT',\n  },\n  {\n    name: 'Delaware',\n    label: 'Delaware',\n    abbreviation: 'DE',\n  },\n  {\n    name: 'District of Columbia',\n    label: 'District of Columbia',\n    abbreviation: 'DC',\n  },\n  {\n    name: 'Florida',\n    label: 'Florida',\n    abbreviation: 'FL',\n  },\n  {\n    name: 'Georgia',\n    label: 'Georgia',\n    abbreviation: 'GA',\n  },\n  {\n    name: 'Hawaii',\n    label: 'Hawaii',\n    abbreviation: 'HI',\n  },\n  {\n    name: 'Idaho',\n    label: 'Idaho',\n    abbreviation: 'ID',\n  },\n  {\n    name: 'Illinois',\n    label: 'Illinois',\n    abbreviatio