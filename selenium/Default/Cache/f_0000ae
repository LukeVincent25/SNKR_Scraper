scription: 'Label for credit card radio button',\n    defaultMessage: 'Credit or Debit Card',\n  },\n  payPalButtonLabel: {\n    id: 'checkout.payment.addPaypal.button.label',\n    description: 'Label for adding paypal',\n    defaultMessage: 'PayPal',\n  },\n  saveButtonLabel: {\n    id: 'checkout.payment.saveButton.label',\n    description: 'continue To Order Review Button button label',\n    defaultMessage: 'Continue To Order Review',\n  },\n  noPaymentAdded: {\n    id: 'C06E97B8',\n    description: '/buy/checkouts/v2/PAYMENT_INVALID',\n    defaultMessage: 'The payment method is invalid. Please try again. If you keep having problems, try a different payment method or contact your card issuer for help.',\n  },\n  billingCountryFormTooltip: {\n    id: 'checkout.payment.billingCountryForm.tooltip',\n    description: 'Tooltip explanation of the purpose of the billing country label',\n    defaultMessage: 'This is the country/region where your payment method is registered.',\n  },\n  placeOrderCopy: {\n    id: 'checkout.OrderReview.secondary.copy',\n    description: 'Copy for order and review header section',\n    defaultMessage: 'Before you place your order, please take a moment to review your shipping information, billing information, and cart summary.',\n  },\n  placeOrderLabel: {\n    id: 'checkout.OrderReview.button.label.placeOrderButton',\n    description: 'Label for \"place order\" button',\n    defaultMessage: 'Place Order',\n  },\n});\n\nexport const IFRAME_DOMAIN = 'https://paymentcc.nike.com';\nexport const IFRAME_ELEMENT = 'credit-card-iframe';\nexport const IFRAME_MSG = 'validate';\nexport const PAYMENT_TYPE_CREDIT_CARD = 'CreditCard';\nexport const IFRAME_DOMAIN_WHITELIST = /(nike|nikecloud)\\.com/;\n\n\nexport class PaymentComponent extends Component {\n  constructor(props) {\n    super(props);\n\n    this.togglePrepaidForm = this.togglePrepaidForm.bind(this);\n    this.toggleBillingCountryFormVisibility = this.toggleBillingCountryFormVisibility.bind(this);\n    this.setBillingCountryModalVisibility = this.setBillingCountryModalVisibility.bind(this);\n    this.acceptBillingCountryModal = this.acceptBillingCountryModal.bind(this);\n    this.cancelAndCloseBillingCountryModal = this.cancelAndCloseBillingCountryModal.bind(this);\n    this.continueToReview = this.continueToReview.bind(this);\n    this.handleABOrderClick = this.handleABOrderClick.bind(this);\n    this.togglePaymentOptions = this.togglePaymentOptions.bind(this);\n    this.toggleCNPaymentOptions = this.toggleCNPaymentOptions.bind(this);\n    this.showDeleteConfirmModal = this.showDeleteConfirmModal.bind(this);\n    this.hideDeleteConfirmModal = this.hideDeleteConfirmModal.bind(this);\n    this.handleDeletePayment = this.handleDeletePayment.bind(this);\n    this.getPaymentOptions = this.getPaymentOptions.bind(this);\n    this.cancelEdit = this.cancelEdit.bind(this);\n    this.handleCreditCardPostMessage = this.handleCreditCardPostMessage.bind(this);\n    this.toggleSetSameAsShipping = this.toggleSetSameAsShipping.bind(this);\n    this.handleUseCard = handleUseCard.bind(this);\n    this.toggleSavePayment = this.toggleSavePayment.bind(this);\n    this.forceHideKeyboard = this.forceHideKeyboard.bind(this);\n    this.onHiddenInputFocus = this.onHiddenInputFocus.bind(this);\n    this.handleKlarnaV2Submit = this.handleKlarnaV2Submit.bind(this);\n    this.handleKlarnaAuthorization = this.handleKlarnaAuthorization.bind(this);\n    this.handleSofortSubmit = this.handleSofortSubmit.bind(this);\n    this.handleIdealSubmit = this.handleIdealSubmit.bind(this);\n    this.handleCodSubmit = this.handleCodSubmit.bind(this);\n    this.handleApplePaySubmit = this.handleApplePaySubmit.bind(this);\n    this.handleKonbiniSubmit = this.handleKonbiniSubmit.bind(this);\n    this.handleBankSelect = this.handleBankSelect.bind(this);\n    this.validateIdealForm = this.validateIdealForm.bind(this);\n    this.handlePlaceOrderBtnClick = this.handlePlaceOrderBtnClick.bind(this);\n    this.toggleShouldShowBankTransfer = this.toggleShouldShowBankTransfer.bind(this);\n    this.handleKonbiniStoreChange = this.handleKonbiniStoreChange.bind(this);\n    this.handleUpdateKlarnaSession = this.handleUpdateKlarnaSession.bind(this);\n    this.loadKlarnaWidget = this.loadKlarnaWidget.bind(this);\n    this.state = {\n      isKlarnaLoading: true,\n    };\n  }\n\n  // Have to add this because of 2 step checkout\n  // TODO https://jira.nike.com/browse/CCC-5034\n  // eslint-disable-next-line camelcase\n  UNSAFE_componentWillMount() {\n    loadIovation();\n  }\n\n  componentDidMount() {\n    const {\n      dispatch,\n      isUsingWechatBrowser,\n      klarnaSessionToken,\n      isDeliveryTypePickup,\n    } = this.props;\n\n    window.addEventListener('message', this.handleCreditCardPostMessage, false);\n    dispatch({\n      type: SET_GUID,\n      payload: v4(),\n    });\n\n    if (isUsingWechatBrowser === null || isUsingWechatBrowser === undefined) {\n      this.props.setUsingWechatBrowser(getIsUsingWechatBrowser());\n    }\n\n    if (klarnaSessionToken) {\n      this.loadKlarnaWidget();\n    }\n\n    if (isDeliveryTypePickup) {\n      this.initializeBopisBillingForm();\n    }\n  }\n\n  componentDidUpdate(prevProps) {\n    const {\n      canUseApplePay,\n      contactFormValues,\n      contactStateValues,\n      dispatch,\n      hasKlarna,\n      isCod,\n      isContactFormInvalid,\n      isKonbini,\n      isPayPal,\n      klarnaSessionToken,\n      paymentToken,\n      selectedPaymentMethod,\n      shouldContinueToOrderReview,\n      shouldFetchKlarnaToken,\n      shouldShowContactForm,\n      showModal,\n      totalsAfterPrepaid,\n    } = this.props;\n\n    if (shouldFetchKlarnaToken) {\n      dispatch(fetchKlarnaToken())\n        // eslint-disable-next-line no-console\n        .catch((e) => { console.log('Error caught, already fetching klarna token', e); });\n    }\n    // handle case where klarna session loads after payment component mounts.\n    if (hasKlarna && klarnaSessionToken && !prevProps.klarnaSessionToken) {\n      this.loadKlarnaWidget();\n    }\n    // Set the payment method to be GiftCard if total amount is covered\n    // by any prepaid methods for China.\n    if (totalsAfterPrepaid === 0 && selectedPaymentMethod !== 'GiftCard' && getLocale() === 'zh-CN') {\n      dispatch({\n        type: SET_SELECTED_PAYMENT_METHOD,\n        payload: 'GiftCard', // this really meant either gift card, promo code or both\n      });\n    }\n\n    // add contact info to state\n    if (shouldShowContactForm && !isContactFormInvalid &&\n        contactFormValues && contactStateValues !== contactFormValues.values) {\n      dispatch({\n        type: SET_CONTACT_INFO,\n        payload: contactFormValues.values,\n      });\n    }\n\n    if (shouldContinueToOrderReview) {\n      dispatch({\n        type: SET_CONTINUE_TO_ORDER_REVIEW,\n        payload: true,\n      });\n      if (!paymentToken && !isPayPal && !isCod && !isKonbini) {\n        dispatch(postPaymentPreview(true));\n      }\n    }\n\n    if (canUseApplePay && !get(prevProps, 'wasApplePayDefaulted') && selectedPaymentMethod !== APPLE_PAY) {\n      dispatch({\n        type: SET_SELECTED_PAYMENT_METHOD,\n        payload: APPLE_PAY,\n      });\n      dispatch({\n        type: SET_APPLEPAY_DEFAULTED,\n        payload: true,\n      });\n    }\n\n    // set focus to modal - accessibility\n    if (showModal) {\n      this.modalElement.focus();\n    }\n  }\n\n  componentWillUnmount() {\n    this.props.dispatch({\n      type: SET_CONTINUE_TO_ORDER_REVIEW,\n      payload: false,\n    });\n    window.removeEventListener('message', this.handleCreditCardPostMessage, false);\n  }\n\n  onHiddenInputFocus() {\n    this.hiddenInput.blur();\n  }\n\n  setBillingCountryModalVisibility(displayCountry = null) {\n    const { dispatch } = this.props;\n    dispatch({\n      type: SET_BILLING_COUNTRY_MODAL_COUNTRY,\n      payload: displayCountry,\n    });\n  }\n\n  getPaymentContent() {\n    const { inExperimentAB4790A } = this.props;\n\n    switch (true) {\n    case this.props.isCreditDebit:\n      return (\n        <CreditDebit\n          cancelEdit={this.cancelEdit}\n          data={this.data}\n          handleUseCard={this.handleUseCard}\n          addCardAndPlaceOrder={this.handleABOrderClick}\n          selected={this.sameAsShipping}\n          showDeleteConfirmModal={this.showDeleteConfirmModal}\n          syncWithSelected\n          togglePaymentOptions={this.togglePaymentOptions}\n          toggleSavePayment={this.toggleSavePayment}\n          toggleSetSameAsShipping={this.toggleSetSameAsShipping}\n          {...this.props}\n        />\n      );\n    case this.props.isPayPal:\n      return (<PayPal {...this.props} />);\n    case this.props.isApplePay:\n      return (<ApplePay\n        handleApplePaySubmit={this.handleApplePaySubmit}\n        {...this.props}\n      />);\n    case this.props.isCod:\n      return (<Cod\n        inExperimentAB4790A={inExperimentAB4790A}\n        handleCodSubmit={this.handleCodSubmit}\n        {...this.props}\n      />);\n    case this.props.isSofort:\n      return (\n        <Sofort\n          handleSofortSubmit={this.handleSofortSubmit}\n          toggleSetSameAsShipping={this.toggleSetSameAsShipping}\n          inExperimentAB4790A={inExperimentAB4790A}\n          {...this.props}\n        />\n      );\n    case this.props.isIdeal:\n      return (\n        <Ideal\n          handleIdealSubmit={this.handleIdealSubmit}\n          toggleSetSameAsShipping={this.toggleSetSameAsShipping}\n          idealBanks={this.props.idealBanks}\n          handleBankSelect={this.handleBankSelect}\n          selectedBank={this.props.selectedBank}\n          showIdealFieldErrors={this.props.showIdealFieldErrors}\n          validateIdealForm={this.validateIdealForm}\n          inExperimentAB4790A={inExperimentAB4790A}\n          {...this.props}\n        />\n      );\n    case this.props.isKonbini:\n      return (\n        <Konbini\n          options={this.props.availableKonbiniStores}\n          selectedOption={this.props.selectedKonbiniStore}\n          toggleSetSameAsShipping={this.toggleSetSameAsShipping}\n          onChange={this.handleKonbiniStoreChange}\n          handleKonbiniSubmit={this.handleKonbiniSubmit}\n          {...this.props}\n        />\n      );\n    default:\n      return null;\n    }\n  }\n\n  getPaymentOptions() {\n    const {\n      canUseApplePay,\n      hasPrepaidMethods,\n      availablePaymentOptions,\n      selectedPaymentMethod,\n      disabledPaymentOptions,\n      isUsingWechatBrowser,\n      allPaymentOptionsCN,\n    } = this.props;\n\n    const hiddenOptions = [];\n    if (!canUseApplePay) {\n      hiddenOptions.push(APPLE_PAY);\n    }\n\n    return getLocale() === 'zh-CN' ?\n      (\n        <PaymentOptionsCNComponent\n          options={availablePaymentOptions}\n          disabledOptions={disabledPaymentOptions}\n          selectedOption={selectedPaymentMethod || ALIPAY}\n          onChange={this.toggleCNPaymentOptions}\n          isWechatBrowser={isUsingWechatBrowser}\n          allPaymentOptionsCN={allPaymentOptionsCN}\n        />\n      ) :\n      (\n        <div className=\"prl5-sm pt5-sm border-top-light-grey\">\n          <h3 className=\"ncss-brand fs16-sm mb4-sm u-uppercase\">{formatMessage(messages.summaryTitle)}</h3>\n          <PaymentOptionsComponent\n            hasPrepaidMethods={hasPrepaidMethods}\n            onChangeHandler={this.togglePaymentOptions}\n            options={availablePaymentOptions}\n            selectedOption={selectedPaymentMethod}\n            disabledOptions={disabledPaymentOptions}\n            hiddenOptions={hiddenOptions}\n          />\n\n          {/*\n            This invisible element is focused and unfocused to force removal\n            of the soft keyboard on mobile devices. This is a work around for\n            issues caused by credit card fields inside the iframe not losing\n            focus before they are removed from the DOM. See CCC-1296 for more\n            info.\n          */}\n          <input\n            aria-hidden=\"true\"\n            className=\"hidden-input\"\n            type=\"tel\"\n            ref={(input) => { this.hiddenInput = input; }}\n            onFocus={this.onHiddenInputFocus}\n          />\n        </div>\n      );\n  }\n\n  /**\n   * Bopis billing form requires first and\n   * last names to be disabled and use the state names from contact form\n   */\n  initializeBopisBillingForm() {\n    const {\n      recipient,\n      dispatch,\n    } = this.props;\n    const { firstName, lastName } = recipient;\n    const type = '@@redux-form/CHANGE';\n    dispatch({\n      type,\n      meta: {\n        form: 'genericAddressForm',\n        field: 'address.firstName',\n        touch: false,\n        persistentSubmitErrors: false,\n      },\n      payload: firstName,\n    });\n    dispatch({\n      type,\n      meta: {\n        form: 'genericAddressForm',\n        field: 'address.lastName',\n        touch: false,\n        persistentSubmitErrors: false,\n      },\n      payload: lastName,\n    });\n  }\n\n  loadKlarnaWidget() {\n    const {\n      klarnaPaymentCategories,\n    } = this.props;\n    loadKlarnaUI({\n      klarnaPaymentCategories,\n      loadCallback: () => {\n        this.setState({\n          isKlarnaLoading: false,\n        });\n      },\n    });\n  }\n\n  handleCreditCardPostMessage(e) {\n    // only handle message if base path url of the origin is from nike payment API\n    if (IFRAME_DOMAIN_WHITELIST.test(e.origin)) {\n      if (e.data) {\n        if (Object.prototype.hasOwnProperty.call(e.data, ['isValid'])) {\n          this.data = e.data;\n          this.props.dispatch({\n            type: SET_CREDIT_CARD_VALID,\n            payload: !!e.data.isValid,\n          });\n        }\n      }\n    }\n  }\n\n  handleSofortSubmit() {\n    const {\n      dispatch,\n      thirdPartyPaymentBillingAddress,\n    } = this.props;\n    if (!this.validateBillingAddress()) {\n      return;\n    }\n    dispatch({\n      type: ADD_CLICK_TRACKER,\n      payload: {\n        clickedOrderReviewButton: {\n          type: CONTINUE_TO_ORDER_REVIEW,\n        },\n      },\n    });\n    dispatch(sofortPaymentPreview(thirdPartyPaymentBillingAddress));\n  }\n\n  handleIdealSubmit() {\n    const {\n      dispatch,\n      selectedBank,\n      thirdPartyPaymentBillingAddress,\n    } = this.props;\n\n    if (!selectedBank || !this.validateBillingAddress()) {\n      return;\n    }\n\n    dispatch({\n      type: ADD_CLICK_TRACKER,\n      payload: {\n        clickedOrderReviewButton: {\n          type: CONTINUE_TO_ORDER_REVIEW,\n        },\n      },\n    });\n    dispatch(idealPaymentPreview(thirdPartyPaymentBillingAddress, selectedBank));\n  }\n\n  validateBillingAddress() {\n    const {\n      dispatch,\n      isBillingDataInvalid,\n      isClickAndCollect,\n      isStoredPaymentSelected,\n      sameAsShipping,\n    } = this.props;\n    if (!isStoredPaymentSelected && (!sameAsShipping || isClickAndCollect) &&\n      isBillingDataInvalid) {\n      // validate\n      dispatch(touch('genericAddressForm', 'address.firstName',\n        'address.lastName', 'address.altFirstName', 'address.altLastName',\n        'address.postalCode', 'address.address1', 'address.address3', 'address.city', 'address.state'));\n      return false;\n    }\n    return true;\n  }\n\n  validateIdealForm() {\n    this.props.dispatch({\n      type: TOGGLE_SHOW_IDEAL_FIELD_ERRORS,\n      payload: true,\n    });\n  }\n\n  handleBankSelect(event) {\n    const bank = get(event, 'currentTarget.value');\n    this.props.dispatch({\n      type: SET_SELECTED_BANK,\n      payload: bank,\n    });\n  }\n\n  handleCodSubmit() {\n    const {\n      dispatch,\n      address,\n    } = this.props;\n    dispatch({\n      type: ADD_CLICK_TRACKER,\n      payload: {\n        clickedOrderReviewButton: {\n          type: CONTINUE_TO_ORDER_REVIEW,\n        },\n      },\n    });\n    dispatch(codPaymentPreview(address));\n  }\n\n  handleApplePaySubmit() {\n    const { dispatch } = this.props;\n    dispatch({\n      type: ADD_CLICK_TRACKER,\n      payload: {\n        clickedOrderReviewButton: {\n          type: CONTINUE_TO_ORDER_REVIEW,\n        },\n      },\n    });\n    dispatch(applePayPaymentPreview());\n  }\n\n  toggleBillingCountryFormVisibility(e) {\n    const { dispatch } = this.props;\n    const country = e.currentTarget && e.currentTarget.value;\n    dispatch({\n      type: SHOW_BILLING_COUNTRY_FORM,\n      payload: !this.props.showBillingCountryForm,\n    });\n    // analytics\n    dispatch({\n      type: SHOW_BILLING_FORM,\n      payload: !this.props.showBillingCountryForm,\n    });\n    if (country) {\n      this.setBillingCountryModalVisibility(country);\n      dispatch(fetchPendingPaymentOptions());\n    }\n  }\n\n  forceHideKeyboard() {\n    if (this.hiddenInput) {\n      this.hiddenInput.focus();\n    }\n  }\n\n  handleKonbiniStoreChange(id) {\n    this.props.dispatch({\n      type: SET_SELECTED_KONBINI_STORE,\n      payload: id,\n    });\n  }\n\n  handleUpdateKlarnaSession() {\n    const {\n      dispatch,\n    } = this.props;\n    // if needed, conditions for updating klarna session go here\n    return dispatch(updateKlarnaSession());\n  }\n\n  handleKonbiniSubmit() {\n    if (this.props.selectedKonbiniStore && this.validateBillingAddress()) {\n      const {\n        dispatch,\n        selectedPaymentBillingAddress,\n      } = this.props;\n      dispatch({\n        type: ADD_CLICK_TRACKER,\n        payload: {\n          clickedOrderReviewButton: {\n            type: CONTINUE_TO_ORDER_REVIEW,\n          },\n        },\n      });\n      dispatch(konbiniPaymentPreview(selectedPaymentBillingAddress));\n    }\n  }\n\n  handleKlarnaV2Submit() {\n    const {\n      dispatch,\n      isKlarnaBillingSameAsShipping,\n      isKlarnaFormValid,\n      klarnaBillingAddress,\n      klarnaShippingAddress,\n      klarnaChecked,\n    } = this.props;\n    if (!klarnaChecked) {\n      return;\n    }\n    if (isKlarnaFormValid && this.validateBillingAddress()) {\n      this.handleUpdateKlarnaSession().then(() => {\n        authorizeKlarna({\n          shippingAddress: klarnaShippingAddress,\n          billingAddress: klarnaBillingAddress,\n          authCallback: this.handleKlarnaAuthorization,\n          sameAsShipping: isKlarnaBillingSameAsShipping,\n        });\n      });\n    } else {\n      dispatch(touch('klarnaForm', 'email', 'phoneNumber', 'dateOfBirth', 'gender', 'personalId'));\n    }\n  }\n\n  handleKlarnaAuthorization(res) {\n    const {\n      dispatch,\n      klarnaValues,\n      thirdPartyPaymentBillingAddress,\n    } = this.props;\n    const {\n      approved,\n      authorization_token, // eslint-disable-line\n      finalize_required, // eslint-disable-line\n      show_form, // eslint-disable-line\n    } = res;\n\n    if (approved) {\n      const authResponse = {\n        authorization_token,\n        finalize_required,\n        approved,\n      };\n      const klarnaPreview = {\n        ...klarnaValues,\n        authorization_token,\n        approved,\n      };\n      dispatch({\n        type: ADD_CLICK_TRACKER,\n        payload: {\n          clickedOrderReviewButton: {\n            type: CONTINUE_TO_ORDER_REVIEW,\n          },\n        },\n      });\n      dispatch({\n        type: SET_BILLING_ADDRESS,\n        payload: thirdPartyPaymentBillingAddress,\n      });\n      dispatch(klarnaPaymentPreview(klarnaPreview, authResponse));\n    // eslint-disable-next-line camelcase\n    } else if (!show_form) {\n      // If we get a rejected status from klarna auth, dispatch here\n      // to indicate that we should disable the klarna option.\n      dispatch({\n        type: KLARNA_AUTH_REJECTED,\n      });\n    }\n  }\n\n  continueToReview() {\n    const {\n      dispatch,\n      isBillingDataInvalid,\n      isCreditDebit,\n      isGuest,\n      isPaymentReadyToPost,\n      paymentMethod,\n      paymentMethods,\n      selectedPaymentBillingAddress,\n      shouldHandleUseCard,\n      showNewCard,\n      totalsCoveredByPrepaid,\n      validMemberCC,\n    } = this.props;\n\n    this.forceHideKeyboard();\n    if (!this.validateBillingAddress()) {\n      return Promise.resolve();\n    }\n\n    if (shouldHandleUseCard) {\n      this.handleUseCard(true, false);\n      if (showNewCard) {\n        dispatch({\n          type: UNEDIT_PAYMENT_SECTION,\n        });\n        dispatch({\n          type: ADD_CLICK_TRACKER,\n          payload: {\n            clickedOrderReviewButton: {\n              type: CONTINUE_TO_ORDER_REVIEW,\n            },\n          },\n        });\n        return Promise.resolve();\n      }\n    } else if (totalsCoveredByPrepaid) {\n      if (isBillingDataInvalid) {\n        dispatch({\n          error: true,\n          type: SET_ERRORS,\n          payload: [{\n            crcCode: 'E4C78047',\n          }],\n        });\n        return Promise.resolve();\n      }\n    }\n    dispatch({\n      type: SET_BILLING_ADDRESS,\n      payload: selectedPaymentBillingAddress,\n    });\n\n    if (isPaymentReadyToPost) {\n      let updatingExpiry = false;\n      const updatePaymentMethod = {\n        ...paymentMethod,\n        billingAddress: { ...selectedPaymentBillingAddress },\n      };\n      if (!totalsCoveredByPrepaid) {\n        // setting flag for dispatch, so we can reassign isExpired\n        if (updatePaymentMethod.isExpired) {\n          updatingExpiry = true;\n          updatePaymentMethod.expiryYear = get(this, 'data.ed.y');\n          updatePaymentMethod.expiryMonth = get(this, 'data.ed.m');\n          updatePaymentMethod.isExpired = false;\n        }\n        // Since this is a creditcard we'll remove the third party payment here\n        delete updatePaymentMethod.thirdPartyPayment;\n\n        // Updating creditcard to no longer need to validate the cvv if the user returns to payment\n        if (validMemberCC) {\n          updatePaymentMethod.validateCVV = false;\n          dispatch({\n            type: SET_VALIDATE_PAYMENT_CVV,\n            payload: updatePaymentMethod,\n          });\n        }\n        // $SR Will need to revisit - Next line -\n        const duplicateId = getDuplicateStoredId(paymentMethod, paymentMethods);\n        if (duplicateId && !isGuest) {\n          updatePaymentMethod.id = duplicateId;\n          dispatch({\n            type: UPDATE_PAYMENT_METHOD,\n            payload: updatePaymentMethod,\n          });\n        }\n      }\n\n      if (updatingExpiry) {\n        dispatch(updateStoredPayment(updatePaymentMethod));\n      }\n\n      return Promise.all([\n        dispatch(postPaymentPreview()),\n        dispatch({ type: UNEDIT_PAYMENT_SECTION }),\n        dispatch({\n          type: ADD_CLICK_TRACKER,\n          payload: {\n            clickedOrderReviewButton: {\n              type: CONTINUE_TO_ORDER_REVIEW,\n            },\n          },\n        }),\n        this.props.dispatch({\n          type: SET_INITIAL_LOADED,\n          payload: false,\n        }),\n      ]);\n    } else if (isCreditDebit) {\n      // trigger validation of payment iframe fields\n      return postMessageToChildFrame(IFRAME_MSG, IFRAME_DOMAIN, IFRAME_ELEMENT);\n    }\n    return dispatch({\n      error: true,\n      type: SET_ERRORS,\n      payload: [{\n        crcCode: 'B5013F9F',\n      }],\n    });\n  }\n\n  handleABOrderClick() {\n    const {\n      dispatch,\n      totalsCoveredByPrepaid,\n      isBillingDataInvalid,\n    } = this.props;\n    this.forceHideKeyboard();\n\n    if (!this.validateBillingAddress()) {\n      return false;\n    }\n\n    if (totalsCoveredByPrepaid && isBillingDataInvalid) {\n      return dispatch({\n        error: true,\n        type: SET_ERRORS,\n        payload: [{\n          crcCode: 'E4C78047',\n        }],\n      });\n    }\n    // This flow is abstracted into two functions because processNewPaymentMethod may modify the\n    // current payment method in state and we need placeOrderAB to pick that up.\n    return this.processNewPaymentMethod().then(() => {\n      return this.placeOrderAB();\n    });\n  }\n\n  processNewPaymentMethod() {\n    const { showNewCard, isCCValid, totalsCoveredByPrepaid } = this.props;\n    // Note: showNewCard is still true if the user has added a GC that covers total\n    // so we need to also evaluate totalsCoveredByPrepaid\n    if (showNewCard && isCCValid && !totalsCoveredByPrepaid) {\n      return this.handleUseCard(!totalsCoveredByPrepaid, false, true);\n    }\n    return Promise.resolve();\n  }\n\n  placeOrderAB() {\n    const {\n      selectedPaymentBillingAddress,\n      dispatch,\n      isCCValid,\n      isCreditDebit,\n      isGuest,\n      isPaymentReadyToPost,\n      paymentMethod,\n      paymentMethods,\n      validMemberCC,\n      showNewCard,\n      totalsCoveredByPrepaid,\n    } = this.props;\n    return new Promise((resolve) => {\n      dispatch({\n        type: SET_BILLING_ADDRESS,\n        payload: selectedPaymentBillingAddress,\n      });\n\n      if (isPaymentReadyToPost) {\n        let updatingExpiry = false;\n        const updatePaymentMethod = {\n          ...paymentMethod,\n          billingAddress: { ...selectedPaymentBillingAddress },\n        };\n        if (!totalsCoveredByPrepaid) {\n          // setting flag for dispatch, so we can reassign isExpired\n          if (updatePaymentMethod.isExpired) {\n            updatingExpiry = true;\n            updatePaymentMethod.expiryYear = get(this, 'data.ed.y');\n            updatePaymentMethod.expiryMonth = get(this, 'data.ed.m');\n            updatePaymentMethod.isExpired = false;\n          }\n          // Since this is a creditcard we'll remove the third party payment here\n          delete updatePaymentMethod.thirdPartyPayment;\n\n          // Updating creditcard - no longer need to validate the cvv\n          // if the user returns to payment\n          if (validMemberCC) {\n            updatePaymentMethod.validateCVV = false;\n            dispatch({\n              type: SET_VALIDATE_PAYMENT_CVV,\n              payload: updatePaymentMethod,\n            });\n          }\n          // $SR Will need to revisit - Next line -\n          const duplicateId = getDuplicateStoredId(paymentMethod, paymentMethods);\n          if (duplicateId && !isGuest) {\n            updatePaymentMethod.id = duplicateId;\n            dispatch({\n              type: UPDATE_PAYMENT_METHOD,\n              payload: updatePaymentMethod,\n            });\n          }\n        }\n        if (updatingExpiry) {\n          dispatch(updateStoredPayment(updatePaymentMethod)).then(result => resolve(result));\n        } else {\n          resolve(true);\n        }\n      } else if (isCreditDebit) {\n        // trigger validation of payment iframe fields\n        postMessageToChildFrame(IFRAME_MSG, IFRAME_DOMAIN, IFRAME_ELEMENT);\n        resolve(false);\n      } else {\n        this.props.dispatch({\n          error: true,\n          type: SET_ERRORS,\n          payload: [{\n            crcCode: 'B5013F9F', // Credit card information is invalid\n          }],\n        });\n        resolve(false);\n      }\n    }).then((shouldPlaceOrder) => {\n      // We need to explicitly evaluate false because a possible return from updateStoredPayment\n      // is null on success\n      if (shouldPlaceOrder !== false) {\n        if (showNewCard) {\n          // isCCValid only is updates by a new card or a cvv needs validation stored payment\n          // selecting a valid stored card after canceling out of invalid new card or stored\n          // needs validation card will leave this prop out of date\n          if (isCCValid || totalsCoveredByPrepaid) {\n            this.props.placeOrder({ position: 'bottom' });\n          }\n        } else {\n          this.props.placeOrder({ position: 'bottom' });\n        }\n      }\n    });\n  }\n\n  acceptBillingCountryModal() {\n    const { dispatch, billingCountry, billingCountryModalCountry } = this.props;\n    dispatch({\n      type: SET_BILLING_ADDRESS,\n      payload: { country: billingCountryModalCountry },\n    });\n    dispatch({\n      type: CHANGED_BILLING_COUNTRY,\n    });\n    this.toggleSetSameAsShipping(billingCountry === billingCountryModalCountry);\n    this.cancelAndCloseBillingCountryModal();\n    dispatch({\n      type: CONTINUE_CHANGE_BILLING_ADDRESS,\n    });\n    // reset to default payment option\n    dispatch({\n      type: SET_SELECTED_PAYMENT_METHOD,\n      payload: CREDIT_DEBIT,\n    });\n\n    dispatch(fetchPaymentOptions());\n  }\n\n  cancelAndCloseBillingCountryModal() {\n    const { dispatch } = this.props;\n    dispatch({ type: CLEAR_PENDING_PAYMENT_OPTIONS });\n    this.setBillingCountryModalVisibility();\n    dispatch({\n      type: CANCEL_CHANGE_BILLING_ADDRESS,\n    });\n  }\n\n  togglePrepaidForm(state) {\n    if (this.props) {\n      this.props.dispatch({\n        type: TOGGLE_PREPAID_FORM,\n        payload: { open: state },\n      });\n    }\n  }\n\n  togglePaymentOptions(event) {\n    const value = get(event, 'target.value', '');\n    const name = get(event, 'target.name');\n\n    const { dispatch, isPaypalExpress } = this.props;\n\n    // When abandoning PPE and changing to another payment type\n    if (isPaypalExpress) {\n      dispatch(convertPaypalExpressToPaypalMark());\n    }\n\n    if (name === 'newCard') {\n      dispatch({ type: SET_SELECTED_PAYMENT_METHOD, payload: CREDIT_DEBIT });\n    } else {\n      dispatch({ type: SET_SELECTED_PAYMENT_METHOD, payload: value || CREDIT_DEBIT });\n    }\n    dispatch({\n      type: SET_GUID,\n      payload: v4(),\n    });\n    const eventMap = {\n      paymentOptions: TOGGLE_PAYMENT_OPTIONS,\n      newCard: ADD_NEW_CARD,\n      cancel: CANCEL_NEW_CARD,\n    };\n    if (eventMap[name]) {\n      const val = value.toLowerCase();\n      dispatch({\n        type: eventMap[name],\n        payload: {\n          [PAYPAL]: val === PAYPAL,\n          [CREDIT_DEBIT]: val === CREDIT_DEBIT.toLowerCase(),\n          [KLARNA]: val === KLARNA,\n          [SOFORT]: val === SOFORT,\n          [COD]: val === COD.toLowerCase(),\n          [IDEAL]: val === IDEAL,\n          [KONBINI]: val === KONBINI,\n          [APPLE_PAY]: val === APPLE_PAY,\n        },\n      });\n    }\n    const thirdPartyPayment = this.props.thirdPartyPayment;\n    if (thirdPartyPayment === 'paypal') {\n      dispatch(fetchPaymentOptions());\n      dispatch({\n        type: SET_PAYMENT_METHOD,\n        payload: this.props.paymentMethods[Object.keys(this.props.paymentMethods)[0]] || {},\n      });\n    }\n  }\n\n  toggleCNPaymentOptions(optionId) {\n    const { selectedPaymentMethod, dispatch, availableCNBankOptions } = this.props;\n    if (selectedPaymentMethod !== optionId) { // should we respect otherwise?\n      dispatch({\n        type: SET_SELECTED_PAYMENT_METHOD, payload: optionId,\n      });\n      dispatch({\n        type: TOGGLE_PAYMENT_OPTIONS,\n        payload: {\n          [WECHAT]: optionId === WECHAT,\n          [ALIPAY]: optionId === ALIPAY,\n          [UNION_PAY]: optionId === UNION_PAY,\n          [COD]: optionId === COD,\n          [CN_BANK]: availableCNBankOptions.map(bank => bank.id).includes(optionId),\n        },\n      });\n    }\n  }\n\n  toggleShouldShowBankTransfer(value) {\n    const { dispatch } = this.props;\n    dispatch({\n      type: SET_SHOW_BANKS, payload: value,\n    });\n    if (value) {\n      dispatch({\n        type: OPEN_CN_BANK_TRANSFER,\n      });\n    }\n  }\n\n  toggleSetSameAsShipping(sameAsShipping) {\n    if (sameAsShipping) {\n      this.props.dispatch({\n        type: SET_BILLING_ADDRESS,\n        payload: { country: get(this.props, 'address.country') },\n      });\n      this.props.dispatch(reset('genericAddressForm'));\n    }\n    this.props.dispatch({\n      type: TOGGLE_SET_SAME_AS_SHIPPING,\n      payload: { sameAsShipping },\n    });\n  }\n\n  toggleSavePayment(savePayment) {\n    if (get(this, 'props.dispatch')) {\n      this.props.dispatch({\n        type: TOGGLE_SAVE_PAYMENT,\n        payload: { savePayment },\n      });\n    }\n  }\n\n  showDeleteConfirmModal(e, paymentId) {\n    if (e) { e.preventDefault(); }\n    const { dispatch } = this.props;\n    dispatch({\n      type: SET_SHOW_DELETE_MODAL,\n      payload: true,\n    });\n\n    dispatch({\n      type: SET_PAYMENT_TO_DELETE,\n      payload: paymentId,\n    });\n\n    dispatch({\n      type: DELETE_STORED_PAYMENT_START,\n    });\n  }\n\n  hideDeleteConfirmModal(e) {\n    if (e) { e.preventDefault(); }\n    const { dispatch } = this.props;\n    dispatch({\n      type: SET_SHOW_DELETE_MODAL,\n      payload: false,\n    });\n\n    dispatch({\n      type: SET_PAYMENT_TO_DELETE,\n      payload: '',\n    });\n\n    dispatch({\n      type: DELETE_STORED_PAYMENT_END,\n      payload: 'cancel',\n    });\n  }\n\n  handleDeletePayment(e, id) {\n    if (e) { e.preventDefault(); }\n    const { dispatch } = this.props;\n    dispatch({\n      type: SET_SHOW_DELETE_MODAL,\n      payload: false,\n    });\n\n    dispatch({\n      type: SET_PAYMENT_TO_DELETE,\n      payload: '',\n    });\n\n    dispatch({\n      type: DELETE_STORED_PAYMENT_END,\n      payload: 'delete',\n    });\n\n    this.props.dispatch(deletePaymentMethod(id));\n  }\n\n  cancelEdit(e) {\n    this.forceHideKeyboard();\n    this.props.dispatch(reset('genericAddressForm'));\n    this.togglePaymentOptions(e);\n  }\n\n\n  handlePlaceOrderBtnClick(position) {\n    if (this.props.invoiceInfoFormData.withFapiao && this.props.isInvoiceInfoInvalid) {\n      return;\n    }\n    this.props.setInvoiceInfo(formatInvoiceInfoFormToState(this.props.invoiceInfoFormData));\n    const paymentMethod = {\n      id: v4(),\n      type: this.props.selectedPaymentMethod,\n    };\n\n    if (this.props.isCNBankSelected) {\n      paymentMethod.type = ALIPAY;\n      paymentMethod.bankName = this.props.selectedPaymentMethod;\n    }\n\n    this.props.dispatch({\n      type: SET_PAYMENT_METHOD,\n      payload: paymentMethod,\n    });\n\n    this.props.placeOrder({ position });\n  }\n\n\n  render() {\n    const {\n      address,\n      allPaymentOptionsCN,\n      availableCNBankOptions,\n      billingCountry,\n      billingCountryModalCountry,\n      containsOnlyDigitalGiftCard,\n      didKlarnaCreateSessionFail,\n      dispatch,\n      inExperimentAB4790A,\n      isClickAndCollect,\n      isCreditDebit,\n      isKlarna,\n      isKlarnaAuthRejected,\n      isKlarnaBillingSameAsShipping,\n      isProcessing,\n      isShowBanks,\n      isSwoosh,\n      isCCValid,\n      paymentToDelete,\n      pendingPaymentOptions,\n      prepaidFormVisible,\n      sameAsShipping,\n      selectedPaymentMethod,\n      shoppingCountry,\n      shouldShowContactForm,\n      showChangeBillingCountryView,\n      showContinueToOrderReview,\n      showPaymentPreview,\n      storedShippingAddresses,\n      totalsAfterPrepaid,\n      isEU,\n      totalsCoveredByPrepaid,\n    } = this.props;\n\n    const { isKlarnaLoading } = this.state;\n\n    const paypalPng = require('../../../../assets/img/payment/logo_paypal2x.png'); //eslint-disable-line\n    // Payment Loading\n    if (isProcessing) {\n      return (\n        <React.Fragment>\n\n          <div className=\"ncss-row\">\n            <div className=\"ncss-col-sm-12 pb12-sm\">\n              <Loading />\n            </div>\n          </div>\n          <KlarnaWidget\n            address={address}\n            billingCountry={billingCountry}\n            didKlarnaCreateSessionFail={didKlarnaCreateSessionFail}\n            handleKlarnaV2Submit={this.handleKlarnaV2Submit}\n            isKlarna={isKlarna}\n            isKlarnaAuthRejected={isKlarnaAuthRejected}\n            hideKlarna\n          />\n        </React.Fragment>\n      );\n    }\n\n    // Payment Preview\n    if (getLocale() !== 'zh-CN' && showPaymentPreview) {\n      return (\n        <React.Fragment>\n          <div>\n            <div className=\"ncss-row\">\n              <div className=\"ncss-col-sm-12 prl5-sm pt5-sm pb5-sm\">\n                <PaymentPreview {...this.props} />\n              </div>\n            </div>\n          </div>\n          <KlarnaWidget\n            address={address}\n            billingCountry={billingCountry}\n            didKlarnaCreateSessionFail={didKlarnaCreateSessionFail}\n            handleKlarnaV2Submit={this.handleKlarnaV2Submit}\n            isKlarna={isKlarna}\n            isKlarnaAuthRejected={isKlarnaAuthRejected}\n            hideKlarna\n          />\n        </React.Fragment>\n      );\n    }\n\n    const placeOrderLogoClassName = totalsAfterPrepaid === 0 ? '' :\n      `${selectedPaymentMethod ? selectedPaymentMethod.toLowerCase() : ''}-logo-white`;\n\n    const isDesktop = getDeviceName() === 'Desktop';\n    // Content\n    return (\n      <React.Fragment>\n\n        <div className=\"ncss-row\">\n          <div className=\"ncss-col-sm-12\">\n            <Modal\n              {...this.props}\n              handleOK={this.handleDeletePayment}\n              handleCancel={this.hideDeleteConfirmModal}\n              modalRef={(el) => { this.modalElement = el; }}\n              actionData={paymentToDelete}\n            />\n          </div>\n          {billingCountryModalCountry &&\n          <div className=\"ncss-col-sm-12\">\n            <BillingCountryModal\n              country={billingCountryModalCountry}\n              isOpen\n              onAccept={this.acceptBillingCountryModal}\n              onCancel={this.cancelAndCloseBillingCountryModal}\n              paymentTypes={pendingPaymentOptions.items}\n            />\n          </div>}\n          {/*\n          GIFT CARDS / PROMOCODES\n        */}\n          <div className=\"ncss-col-sm-12\">\n            {showChangeBillingCountryView &&\n            <div className=\"ncss-col-sm-12 p5-sm\">\n              <BillingCountryFormView\n                billingCountry={billingCountry}\n                shoppingCountry={shoppingCountry}\n                showForm={this.props.showBillingCountryForm}\n                onChangeHandler={this.toggleBillingCountryFormVisibility}\n                toolTipText={formatMessage(messages.billingCountryFormTooltip)}\n              />\n            </div>}\n            <div className=\"prl5-sm pb5-sm\">\n              <CheckboxToggler\n                id=\"prepaidCard\"\n                label={isSwoosh ?\n                  formatMessage(messages.prepaidCardSwooshToggleLabel) :\n                  formatMessage(messages.prepaidCardToggleLabel)}\n                name=\"prepaidCard\"\n                labelStyle=\"pl7-sm u-full-width\"\n                onSelected={toggled => this.togglePrepaidForm(toggled)}\n                selected={prepaidFormVisible}\n                displayContentOnChecked\n              />\n              <PrepaidCard\n                toggleSetSameAsShipping={this.toggleSetSameAsShipping}\n                {...this.props}\n              />\n            </div>\n            {/*\n            Payment Options\n            */}\n            {(totalsAfterPrepaid !== 0) && this.getPaymentOptions()}\n\n            {getIsShowBanksFeatureOn() && isDesktop && getLocale() === 'zh-CN' &&\n            <BankTransferOptionsComponent\n              options={availableCNBankOptions}\n              disabledOptions={this.props.disabledPaymentOptions}\n              selectedOption={selectedPaymentMethod}\n              onChange={this.toggleCNPaymentOptions}\n              toggleIsShowBanks={this.toggleShouldShowBankTransfer}\n              isShowBanks={isShowBanks}\n              allPaymentOptionsCN={allPaymentOptionsCN}\n            />}\n\n            {getLocale() === 'zh-CN' &&\n              <section className=\"ncss-row u-sm-b p5-sm prl5-sm with-top-box-shadow\">\n                <InvoiceInfoForm />\n              </section>\n            }\n\n            {(totalsAfterPrepaid !== 0) && getLocale() !== 'zh-CN' && this.getPaymentContent()}\n\n            {(containsOnlyDigitalGiftCard && shouldShowContactForm) &&\n              <ContactForm\n                country={billingCountry}\n              />\n            }\n            {showContinueToOrderReview && getLocale() !== 'zh-CN' && (inExperimentAB4790A || isEU) &&\n            <div className=\"ncss-col-sm-12 pb5-sm prl5-sm va-sm-t ta-sm-r\">\n              <Button\n                onClick={() => {\n                  this.continueToReview();\n                }}\n                data-attr=\"continueToOrderReviewBtn\"\n                disabled={!(isCreditDebit && isCCValid) && !totalsCoveredByPrepaid}\n                className={`ncss-btn-accent continueOrderReviewBtn mod-button-width ncss-brand\n                pt3-sm prl5-sm pb3-sm pt2-lg pb2-lg d-sm-b d-md-ib u-uppercase u-rounded fs14-sm`}\n              >\n                {formatMessage(messages.saveButtonLabel)}\n              </Button>\n            </div>\n            }\n\n            {getLocale() === 'zh-CN' &&\n            <div className=\"ncss-container cn-place-order-section-border\">\n              <section className=\"ncss-row d-sm-h d-lg-b\">\n                <div data-attr=\"cn-desktop-legal-links-copy\" className=\"ncss-col-sm-12 pt5-lg pb5-sm prl5-sm cn-place-order-copy-text-color\">\n                  <div className=\"fs12-sm lh12-sm\">\n                    {renderCopy(dispatch)}\n                  </div>\n                </div>\n              </section>\n              <section className=\"ncss-row d-sm-h d-lg-b\">\n                <div className=\"ncss-col-sm-12 pb5-sm prl5-sm u-va-t ta-lg-r\">\n                  <PaymentButton\n                    text={formatMessage(messages.placeOrderLabel)}\n                    iconClassName={placeOrderLogoClassName}\n                    onClick={() => this.handlePlaceOrderBtnClick('bottom')}\n                  />\n                </div>\n              </section>\n              <section\n                className={'sticky-place-order fix d-sm-b d-lg-h ncss-container bg-white z1'}\n              >\n                <div className=\"ncss-row\">\n                  <div className={'ncss-col-sm-12 p4-sm'}>\n                    <PaymentButton\n                      text={formatMessage(messages.placeOrderLabel)}\n                      iconClassName={placeOrderLogoClassName}\n                      onClick={() => this.handlePlaceOrderBtnClick('bottom')}\n                    />\n                  </div>\n                </div>\n              </section>\n            </div>\n            }\n          </div>\n        </div>\n\n        {/*\n          Container for Klarna needs to go outside of the conditional rendered payment options\n          so it doesn't get destroyed on re-render.\n        */}\n        <KlarnaWidget\n          address={address}\n          billingCountry={billingCountry}\n          didKlarnaCreateSessionFail={didKlarnaCreateSessionFail}\n          handleKlarnaV2Submit={this.handleKlarnaV2Submit}\n          inExperimentAB4790A={inExperimentAB4790A}\n          isClickAndCollect={isClickAndCollect}\n          isKlarna={isKlarna}\n          isKlarnaAuthRejected={isKlarnaAuthRejected}\n          isKlarnaBillingSameAsShipping={isKlarnaBillingSameAsShipping}\n          isKlarnaLoading={isKlarnaLoading}\n          sameAsShipping={sameAsShipping}\n          saveButtonLabel={formatMessage(messages.saveButtonLabel)}\n          storedShippingAddresses={storedShippingAddresses}\n          toggleSetSameAsShipping={this.toggleSetSameAsShipping}\n        />\n      </React.Fragment>\n    );\n  }\n}\n\nconst mapDispatchToProps = {\n  placeOrder,\n  setUsingWechatBrowser,\n  setInvoiceInfo,\n};\n\nexport function mapStateToProps(state) {\n  const {\n    checkout,\n    app: {\n      ui,\n    },\n    userProfile: {\n      storedShippingAddresses,\n    },\n    form: {\n      genericAddressForm,\n      klarnaForm,\n      prepaidCardForm,\n      promoCodeForm,\n    },\n  } = state;\n\n  const {\n    guid,\n    paymentToDelete,\n    prepaidFormVisible,\n    sameAsShipping,\n    savePayment,\n    showBillingCountryForm,\n    showBillingCountryModal,\n    showModal,\n  } = ui.payment;\n\n  const prepaidSelector = formValueSelector('prepaidCardForm');\n  const prepaidCardNumber = prepaidSelector(state, 'prepaidCardNumber');\n  const promoSelector = formValueSelector('promoCodeForm');\n  const promotionCode = promoSelector(state, 'promoCode');\n  const pinNumber = prepaidSelector(state, 'pinNumber');\n  const prepaidData = prepaidCardForm;\n  const promoData = promoCodeForm;\n  const contactInfoData = getContactInfoData(state);\n  const paymentMethods = getFilteredStoredPayments(state);\n  const paymentMethod = checkout.paymentMethod;\n  const billingCountryModalCountry = getBillingCountryModalCountry(state);\n  const pendingPaymentOptions = get(ui, 'payment.pendingPaymentOptions');\n  const language = getLanguage();\n\n  const invoiceInfoFormSelector = formValueSelector(INVOICE_INFO_FORM_NAME);\n  const invoiceInfoFormData = {\n    withFapiao: !!invoiceInfoFormSelector(state, 'withFapiao'),\n    fapiaoType: invoiceInfoFormSelector(state, 'fapiaoType'),\n    taxId: invoiceInfoFormSelector(state, 'taxId'),\n    individualName: invoiceInfoFormSelector(state, 'individualName'),\n    corporationName: invoiceInfoFormSelector(state, 'corporationName'),\n  };\n  return {\n    address: getAddress(state),\n    addressData: genericAddressForm,\n    addressFields: genericAddressForm && genericAddressForm.values,\n    allPaymentOptionsCN: getAllPaymentOptionsCN(state),\n    availableCNBankOptions: getCNBankOptions(state),\n    availableKonbiniStores: getKonbiniStores(state),\n    availablePaymentOptions: getAvailablePaymentOptions(state),\n    billingAddress: getBillingAddress(state),\n    billingCountry: getBillingCountry(state),\n    billingCountryModalCountry,\n    billingFormValues: getBillingFormValues(state),\n    canUseApplePay: getCanUseApplePay(state),\n    contactFormValues: getContactForm(state),\n    contactInfoFields: contactInfoData,\n    contactStateValues: getContactInfo(state),\n    containsGiftCard: getContainsGiftCard(state),\n    containsOnlyDigitalGiftCard: getContainsOnlyDigitalGiftCard(state),\n    currency: getCurrency(state),\n    didKlarnaCreateSessionFail: getDidKlarnaCreateSessionFail(state),\n    disabledPaymentOptions: getDisabledPaymentOptions(state),\n    displayBillingForm: getDisplayBillingForm(state),\n    duplicatePrepaidMethods: getDuplicatePrepaidMethods(state),\n    guid,\n    hasKlarna: getHasKlarna(state),\n    hasPrepaidMethods: getHasPrepaidMethods(state),\n    idealBanks: getIdealBanks(state),\n    invoiceInfoFormData,\n    isApplePay: getIsApplePaySelected(state),\n    isApoFpo: getIsApoFpo(state),\n    isBillingDataInvalid: getIsBillingDataInvalid(state),\n    isCCValid: getIsCreditCardValid(state),\n    isBopis: getIsBopisSelected(state),\n    isClickAndCollect: getIsClickAndCollectSelected(state),\n    isCNBankSelected: getIsCNBankSelected(state),\n    isCod: getIsCodSelected(state),\n    isContactFormInvalid: getIsContactFormInvalid(state),\n    isCreditDebit: getIsCreditDebitSelected(state),\n    isDeliveryTypePickup: getIsDeliveryTypePickup(state),\n    isExpired: getIsPaymentExpired(state),\n    isFauxpis: getIsPickupLocationTypeBopis(state),\n    isGiftCardPaymentAllowed: getIsGiftCardPaymentAllowed(state),\n    isGuest: getIsGuestUser(state),\n    isIdeal: getIsIdealSelected(state),\n    isInvoiceInfoInvalid: getIsInvoiceInfoInvalid(state),\n    isKlarna: getIsKlarnaSelected(state),\n    isKlarnaAuthRejected: getIsKlarnaAuthRejected(state),\n    isKlarnaBillingSameAsShipping: getIsKlarnaBillingSameAsShipping(state),\n    isKlarnaFormValid: isValid('klarnaForm')(state),\n    isKonbini: getIsKonbiniSelected(state),\n    isPaymentReadyToPost: getIsPaymentReadyToPost(state),\n    isPayPal: getIsPayPalSelected(state),\n    isPaypalExpress: getIsPayPalExpress(state),\n    isPinRequired: getIsPinRequired(state),\n    isProcessing: getIsProcessing(state),\n    isShowBanks: getIsShowBanks(state),\n    isSofort: g