Manually),\n          incompleteSubmission: formatMessage(addressSuggestionMessages.incompleteSubmission),\n          keepTypingAddress: formatMessage(addressSuggestionMessages.keepTypingAddress),\n        }}\n        onSelectViewMoreAddresses={selectViewMoreAddressesEventHandler}\n      />}\n\n      <div className={showAddressSuggest ? 'hideForAVS' : ''}>\n        <FieldWrapper\n          wrapperStyle=\"mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t\"\n          label={formatMessage(messages[country].address1)}\n          id={'address1'}\n          type={'input'}\n          validate={inlineValidator('address1')}\n          disabled={disabled}\n          animateInputLabels\n          showPositiveValidation\n          validationMarker={validationMarker}\n          autoComplete={'street-address'}\n        />\n        {layout === 'DESKTOP' ?\n          <InputAddressLine2Component\n            initialStateOpen={address2Open}\n          />\n          :\n          <FieldWrapper\n            wrapperStyle=\"mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t\"\n            id={'address2'}\n            type={'input'}\n            validate={inlineValidator('address2')}\n            disabled={disabled}\n            animateInputLabels\n            showPositiveValidation\n            validationMarker={validationMarker}\n            placeholder={formatMessage(messages[country].address2)}\n          />\n        }\n        <FieldWrapper\n          wrapperStyle={'ncss-col-md-4 mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t'}\n          label={formatMessage(messages.postCode)}\n          id={'postalCode'}\n          type={'input'}\n          validate={inlineValidator('postalCode')}\n          disabled={disabled}\n          animateInputLabels\n          showPositiveValidation\n          validationMarker={validationMarker}\n          autoComplete={'postal-code'}\n        />\n        <FieldWrapper\n          wrapperStyle={'ncss-col-md-4 mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t'}\n          label={formatMessage(messages[country].city)}\n          id={'city'}\n          type={'input'}\n          validate={inlineValidator('city')}\n          disabled={disabled}\n          animateInputLabels\n          showPositiveValidation\n          validationMarker={validationMarker}\n          autoComplete={'address-level2'}\n          autoCapitalize={'words'}\n        />\n        <FieldWrapper\n          wrapperStyle={`ncss-col-md-4 mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t ${animatedLabelCountryContainerClasses}`}\n          label={formatMessage(messages.country)}\n          id={'country'}\n          tooltipDisabled\n          tooltipContent={[{ type: 'message', text: formatMessage(messages.countryTooltip), style: 'tip-left' }]}\n          tooltipId={'countryTooltip'}\n          placeholder={formatMessage(messages.country)}\n          disabled\n          animateInputLabels\n        />\n      </div>\n\n      <div className=\"pb4-sm\">\n        <FieldWrapper\n          wrapperStyle={'ncss-col-sm-12 ncss-col-md-6  ncss-input-container mod-ncss-input-container mb2-sm mb0-md va-sm-t'}\n          label={formatMessage(messages.email)}\n          id={'email'}\n          type={'input'}\n          inputType=\"email\"\n          validate={!props.emailDisabled && inlineValidator('email')}\n          warn={emailWarn}\n          emailInUse={emailInUse}\n          disabled={props.emailDisabled || disabled}\n          animateInputLabels\n          showPositiveValidation\n          validationMarker={validationMarker}\n          isRequired\n          showIcon\n          trackTooltip={() => { dispatch({ type: TOGGLE_TOOLTIP, payload: 'email' }); }}\n          autoComplete={'email'}\n          /* if swoosh user, email field is disabled. this populates it with account email */\n          {...(props.input ? { ...{ input: props.input } } : {})}\n        />\n        {showLoginSection &&\n          <div className=\"d-md-h ncss-col-sm-12 mb2-sm mb4-md va-sm-t\">\n            { loginSection }\n          </div>\n        }\n        <FieldWrapper\n          wrapperStyle={'ncss-col-sm-12 ncss-col-md-6 ncss-input-container mod-ncss-input-container va-sm-t'}\n          label={formatMessage(messages.phoneNumber)}\n          id={'phoneNumber'}\n          type={'input'}\n          inputType=\"tel\"\n          maxLength=\"18\"\n          validate={inlineValidator('phoneNumber')}\n          disabled={disabled}\n          animateInputLabels\n          showPositiveValidation\n          validationMarker={validationMarker}\n          isRequired\n          showIcon\n          trackTooltip={() => { dispatch({ type: TOGGLE_TOOLTIP, payload: 'phoneNumber' }); }}\n          autoComplete={'tel'}\n        />\n        {showLoginSection &&\n          <div className=\"d-sm-h d-md-b ncss-col-md-12 mb2-sm mb4-md va-sm-t\">\n            { loginSection }\n          </div>\n        }\n      </div>\n    </div>\n  );\n}\n\nAddressFormAB4114Type2Component.propTypes = {\n  address2Open: PropTypes.bool,\n  addressSuggestionLanguage: PropTypes.string,\n  allowNickName: PropTypes.bool,\n  animateInputLabels: PropTypes.bool,\n  country: PropTypes.string,\n  disabled: PropTypes.bool,\n  dispatch: PropTypes.func,\n  emailDisabled: PropTypes.bool,\n  emailInUse: PropTypes.bool,\n  emailWarn: PropTypes.func,\n  formName: PropTypes.string,\n  handleAddressSuggestOptOut: PropTypes.func,\n  handleLoginClick: PropTypes.func,\n  inExperimentAB4114b: PropTypes.bool,\n  initialValues: PropTypes.shape({}),\n  input: PropTypes.shape({}),\n  isMobile: PropTypes.bool,\n  isTouchDevice: PropTypes.bool,\n  setSuggestedAddress: PropTypes.func,\n  shippingAddress: PropTypes.bool,\n  showAddressSuggest: PropTypes.bool,\n  showCountry: PropTypes.bool,\n  showLoginSection: PropTypes.bool,\n  showPositiveValidation: PropTypes.bool,\n  validationMarker: PropTypes.string,\n};\n\nexport default AddressFormAB4114Type2Component;\n\n\