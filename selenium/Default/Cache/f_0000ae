    onJoinSuccess: this.handleJoinSuccess,\n        onLoginSuccess: once(this.props.handleLoginSuccess),\n      });\n    }\n  }\n\n  componentDidMount() {\n    const {\n      country,\n      currency,\n      locale,\n      setApplePayAvailable,\n      setCurrency,\n      setDeviceType,\n      setLayout,\n      uniteInitialized,\n    } = this.props;\n\n    setCurrency({\n      country,\n      currency,\n      locale,\n    });\n\n    setDeviceType(isTouchDevice());\n    setLayout(getLayout());\n    setApplePayAvailable();\n\n    // Coming from tunnel for PayPal Express these could be set already\n    if (uniteInitialized) {\n      this.props.kickoffApp();\n    }\n  }\n\n  // TODO https://jira.nike.com/browse/CCC-5034\n  // eslint-disable-next-line camelcase\n  UNSAFE_componentWillReceiveProps(nextProps) {\n    const { uniteInitialized } = nextProps;\n    if (uniteInitialized) {\n      this.props.kickoffApp();\n    }\n  }\n\n  handleJoinSuccess() {\n    // eslint-disable-next-line no-shadow\n    const { initialSelectedAddressId, fetchNewUserProfile } = this.props;\n    return fetchNewUserProfile(initialSelectedAddressId, true);\n  }\n\n  render() {\n    const {\n      uniteInitialized,\n    } = this.props;\n    return (\n      <ErrorBoundaryComponent>\n        <ExperimentsProvider>\n          { uniteInitialized ?\n            <Main neoDelay={2000} />\n            :\n            <section style={{ height: '100%' }}>\n              <section\n                className={'loading-fade ncss-col-sm-12 bg-white z9'}\n              >\n                <Loading />\n              </section>\n            </section>\n          }\n        </ExperimentsProvider>\n      </ErrorBoundaryComponent>\n    );\n  }\n}\n\nexport function asyncActions(dispatch) {\n  return function dispatchMetaTags() {\n    dispatch(\n      updatePageTitle('Checkout')\n    );\n  };\n}\n\nexport function mapStateToProps(state) {\n  const country = getCountry();\n  const currency = getCurrency();\n  const locale =