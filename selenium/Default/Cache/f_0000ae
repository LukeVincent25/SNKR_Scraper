SummaryAndDetails from './main/CartSummaryAndDetails';\nimport CNMobileOrderReview from './main/CNMobileOrderReview';\nimport OrderConfirmation from './main/OrderConfirmation';\nimport OrderReview from './main/OrderReview';\nimport OrderReviewHeader from './main/OrderReviewHeader';\nimport Payment from './main/Payment';\nimport PlaceOrderAB3576Component from './main/PlaceOrderAB3576';\nimport Shipping from './main/Shipping';\nimport { CLEAR_PAYMENT_METHOD } from '../../state/checkout';\n\nconst messages = defineMessages({\n  shippingTitle: {\n    id: 'checkout.section.title.deliveryOptions',\n    description: 'Delivery Options Section Title',\n    defaultMessage: '1. Delivery Options',\n  },\n  pickupTitle: {\n    id: 'checkout.section.title.pickup',\n    description: 'Shipping section title changed to Pick up',\n    defaultMessage: '1. Pickup',\n  },\n  paymentTitle: {\n    id: 'checkout.section.title.payment',\n    description: 'Payment Section Title',\n    defaultMessage: '2. Payment',\n  },\n  orderReview: {\n    id: 'checkout.section.title.orderReview',\n    description: 'Summary Section Title',\n    defaultMessage: '3. Order Review',\n  },\n  orderReviewSecondary: {\n    id: 'checkout.section.title.secondary.orderReview',\n    description: 'Secondary Order and Review Section Title',\n    defaultMessage: 'Is all your information correct?',\n  },\n  editButtonLabel: {\n    id: 'checkout.section.button.label.edit',\n    description: 'Edit Button label',\n    defaultMessage: 'Edit',\n  },\n  editShippingButtonLabelDescription: {\n    id: 'checkout.section.button.label.edit.shipping.description',\n    description: 'Edit Button label description',\n    defaultMessage: 'Edit Shipping',\n  },\n  editPaymentButtonLabelDescription: {\n    id: 'checkout.section.button.label.edit.payment.description',\n    description: 'Edit Button label description',\n    defaultMessage: 'Edit Payment',\n  },\n\n  // AB Test Strings for 4790\n  reviewYourOrderTitle: {\n    id: 'checkout.section.title.reviewyourorder',\n    description: 'Section title',\n    defaultMessage: 'Review Your Order',\n  },\n\n  // this one is used else where\n  reviewYourOrderCompletedTitle: {\n    id: 'checkout.summary.title',\n    description: 'Title for the right column Summary Section',\n    defaultMessage: 'SUMMARY',\n  },\n  // for Experiment 4790\n  inYoCart: {\n    id: 'checkout.cartdetails.title',\n    description: 'Title for the right column Cart Preview Section',\n    defaultMessage: 'IN YOUR CART',\n  },\n\n});\n\n\nexport function MainSectionComponent(props) {\n  const {\n    dispatch,\n    displayOrderReviewSection,\n    displayPaymentSection,\n    fixCtaButton,\n    hasKlarna,\n    inDeliverySection,\n    isApoFpo,\n    isPickupDeliveryType,\n    isCod,\n    isPaypalExpress,\n    layout,\n    orderComplete,\n    overrideShippingEditButton,\n    paymentCompleted,\n    paymentUserSelected,\n    shippingCompleted,\n    shippingUserSelected,\n    showPaymentEditButton,\n    showShippingEditButton,\n    thirdPartyPayment,\n    totalsCoveredByPrepaid,\n    inExperimentAB4790A,\n    isEU,\n  } = props;\n  // only show section edit button if in PREVIEW MODE (complete and not in progress)\n  const paymentEditButton = showPaymentEditButton ? {\n    title: formatMessage(messages.editButtonLabel),\n    label: formatMessage(messages.editPaymentButtonLabelDescription),\n    onClick: () => {\n      if (isCod) {\n        dispatch(deleteAllPaymentMethods());\n      }\n      dispatch({\n        type: SET_CONTINUE_TO_ORDER_REVIEW,\n        payload: false,\n      });\n      dispatch({\n        type: EDIT_PAYMENT_SECTION,\n        payload: 'edit payment',\n      });\n      dispatch({\n        type: SET_GUID,\n        payload: v4(),\n      });\n    },\n  } : null;\n  const shippingEditButton = showShippingEditButton ? {\n    title: formatMessage(messages.editButtonLabel),\n    label: formatMessage(messages.editShippingButtonLabelDescription),\n    onClick: () => {\n      // Editing shipping will void a PayPalExpress and switch the user to PayPal Mark\n      if (isPaypalExpress) {\n        dispatch(convertPaypalExpressToPaypalMark());\n      }\n      dispatch({\n        type: EDIT_SHIPPING_SECTION,\n        payload: 'edit shipping',\n      });\n      dispatch({\n        type: CLEAR_PAYMENT_METHOD,\n      });\n      dispatch({\n        type: SET_STORED_ADDRESSES_VIEW,\n        payload: (shippingCompleted && !isApoFpo && !isPickupDeliveryType),\n      });\n      dispatch({\n        type: VIEW_SHIPPING_METHODS,\n      });\n    },\n  } : null;\n  const showOrderReview = inExperimentAB4790A || isEU;\n\n  if (hasKlarna) {\n    loadKlarnaScript();\n  }\n\n  if (!orderComplete) {\n    return (\n      <main className={'ncss-col-sm-12 ncss-col-lg-8 va-sm-t'}>\n        {displayOrderReviewSection &&\n          showOrderReview &&\n          <OrderReviewHeader\n            title={formatMessage(messages.orderReviewSecondary)}\n            thirdPartyPayment={thirdPartyPayment}\n            totalsCoveredByPrepaid={totalsCoveredByPrepaid}\n          />\n        }\n        <CartSummaryAndDetails\n          dispatch={dispatch}\n          isMobile\n          title={formatMessage(messages.inYoCart)}\n        />\n\n        {\n          // for CN users, need to display some payment warning here,\n          // warning message is hard coded for now.\n        }\n        {getLocale() === 'zh-CN' && <PaymentNotification />}\n\n        <CheckoutSection\n          title={\n            isPickupDeliveryType\n              ? formatMessage(messages.pickupTitle)\n              : formatMessage(messages.shippingTitle)\n          }\n          theme=\"dark\"\n          displayContent\n          actionButton={shippingEditButton}\n          completed={shippingCompleted && !shippingUserSelected && showShippingEditButton}\n          sectionId=\"shipping\"\n          href=\"#shipping\"\n          overrideEditButton={overrideShippingEditButton}\n        >\n          <Shipping {...props} />\n        </CheckoutSection>\n\n        <CheckoutSection\n          displayContent={displayPaymentSection}\n          title={formatMessage(messages.paymentTitle)}\n          theme=\"dark\"\n          actionButton={paymentEditButton}\n          completed={paymentCompleted && !paymentUserSelected}\n          sectionId=\"payment\"\n          href=\"#payment\"\n        >\n          <Payment {...props} />\n          <CNMobileOrderReview />\n        </CheckoutSection>\n\n        {getLocale() !== 'zh-CN' &&\n          showOrderReview &&\n          <CheckoutSection\n            title={formatMessage(messages.orderReview)}\n            theme=\"dark\"\n            displayContent={displayOrderReviewSection}\n            completed={orderComplete}\n            sectionId=\"orderreview\"\n          >\n            <OrderReview\n              thirdPartyPayment={thirdPartyPayment}\n              fixCtaButton={fixCtaButton}\n              totalsCoveredByPrepaid={totalsCoveredByPrepaid}\n              layout={layout}\n            />\n          </CheckoutSection>\n        }\n\n        {!showOrderReview && !inDeliverySection &&\n          <PlaceOrderAB3576Component\n            dispatch={dispatch}\n            fixCtaButton={fixCtaButton}\n          />\n        }\n      </main>\n    );\n  }\n  return (\n    <main className=\"ncss-col-sm-12 ncss-col-lg-offset-2 ncss-col-lg-8 va-sm-t\">\n      <div className=\"ncss-container\">\n        <OrderConfirmation />\n      </div>\n    </main>\n  );\n}\n\nMainSectionComponent.propTypes = {\n  dispatch: PropTypes.func,\n  displayOrderReviewSection: PropTypes.bool,\n  displayPaymentSection: PropTypes.bool,\n  fixCtaButton: PropTypes.bool,\n  inDeliverySection: PropTypes.bool,\n  isApoFpo: PropTypes.bool,\n  isCod: PropTypes.bool,\n  isPaypalExpress: PropTypes.bool,\n  isPickupDeliveryType: PropTypes.bool,\n  layout: PropTypes.string,\n  orderComplete: PropTypes.bool,\n  overrideShippingEditButton: PropTypes.bool,\n  paymentCompleted: PropTypes.bool,\n  paymentUserSelected: PropTypes.bool,\n  shippingCompleted: PropTypes.bool,\n  shippingUserSelected: PropTypes.bool,\n  showPaymentEditButton: PropTypes.bool,\n  showShippingEditButton: PropTypes.bool,\n  thirdPartyPayment: PropTypes.string,\n  totalsCoveredByPrepaid: PropTypes.bool,\n  hasKlarna: PropTypes.bool,\n  inExperimentAB4790A: PropTypes.bool,\n  isEU: PropTypes.bool,\n};\n\nexport function mapStateToProps(state) {\n  return {\n    overrideShippingEditButton: getContainsOnlyDigitalGiftCard(state),\n    shippingCompleted: getIsDeliverySectionComplete(state),\n    shippingUserSelected: getIsShippingUserSelected(state),\n    paymentCompleted: getIsPaymentComplete(state),\n    thirdPartyPayment: getThirdPartyPayment(state),\n    isPickupDeliveryType: getIsPickupDeliveryType(state),\n    isCod: getIsCodSelected(state),\n    paymentUserSelected: getIsPaymentUserSelected(state),\n    shippingAddress: getShippingAddress(state),\n    contactInfo: getContactInfo(state),\n    recipient: getRecipient(state),\n    orderComplete: getIsOrderComplete(state),\n    inDeliverySection: getIsInDeliverySection(state),\n    isApoFpo: getIsApoFpo(state),\n    displayPaymentSection: getDisplayPaymentSection(state),\n    displayOrderReviewSection: getDisplayOrderReviewSection(state),\n    showPaymentEditButton: getShowPaymentEditButton(state),\n    showShippingEditButton: !getIsInDeliverySection(state),\n    totalsCoveredByPrepaid: getIsTotalsCoveredByPrepaid(state),\n    isPaypalExpress: getIsPayPalExpress(state),\n    hasKlarna: getHasKlarna(state),\n    isEU: getIsEU(state),\n  };\n}\n\nexport default connect(mapStateToProps)(MainSectionComponent);\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/containers/MainSection.js","import { withExperiments } from '@nike/nr-vamp-ab';\nimport { isSwooshUser } from '@nike/ux-tread-unite-redux';\nimport get from 'lodash.get';\nimport throttle from 'lodash.throttle';\nimport PropTypes from 'prop-types';\nimport React from 'react';\nimport { connect } from 'react-redux';\nimport { v4 } from 'uuid';\nimport { loadNeoScript } from '../../helpers/analyticsNeoMiddleware';\nimport {\n  hasNoItems,\n  isProductNotBuyable,\n  isProductOutOfStock,\n  isTimeoutError,\n} from '../../helpers/errorUtils';\nimport { isEU as getIsEU } from '../../helpers/getGlobals';\nimport {\n  getBopisPickupLocation,\n  getCountry,\n  getDisplayOrderReviewSection,\n  getErrors,\n  getHasKlarna,\n  getInitialLoaded,\n  getIsGuestUser,\n  getIsMainReady,\n  getIsPickupLocationTypeBopis,\n  getIsShippingReady,\n  getIsSingleItem,\n  getLayout,\n  getLocale,\n  getLocalizedCartHref,\n  getLocalizedCheckoutUrl,\n  getOneItem,\n  getPaypalMismatch,\n  getShouldDirectToPayment,\n  getShouldResetPaymentGuid,\n  getHasShippingFieldError,\n  getHasPaymentFieldError,\n} from '../../selectors';\nimport {\n  CHECKOUT_ABANDONED,\n  CLEAR_ERRORS,\n  EDIT_PAYMENT_SECTION,\n  getIsPaypalProcessing,\n  getNonFilteredAvailablePaymentOptions,\n  getOrderOrPaypalProcessing,\n  SET_CONTINUE_TO_ORDER_REVIEW,\n  SET_GUID, SET_INITIAL_LOADED,\n  SET_USER_SELECTED_SHIPPING_OPTIONS,\n  openAddressForm,\n} from '../../state/app';\nimport { ExperimentConstants } from '../components/abTests/ExperimentUtils';\nimport ErrorModal from '../components/ErrorModal';\nimport Loading from '../components/Loading';\nimport OrderProcessing from '../components/ProcessingOrder';\nimport AdyenWrapper from './AdyenWrapper';\nimport AsideSection from './AsideSection';\nimport CheckoutBanner from './CheckoutBanner';\nimport CurrentModes from './CurrentModes';\nimport DynamicDeferredPayment from './main/DynamicDeferredPayment';\nimport PaypalExpressError from './main/PaypalExpressError';\nimport MainSection from './MainSection';\n\nexport class MainComponent extends React.Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      fixCtaButton: true,\n    };\n    this.usingMouse = false;\n    this.onModalClose = this.onModalClose.bind(this);\n    this.handleScroll = this.handleScroll.bind(this);\n    this.beforeUnload = this.beforeUnload.bind(this);\n  }\n\n  // Combined strategy from https://jaketrent.com/post/update-body-class-react/ & https://github.com/ten1seven/track-focus/blob/master/dest/javascripts/track-focus.js\n  componentDidMount() {\n    document.body.classList.toggle('keyboard-focus', true);\n    document.body.addEventListener('keydown', this.preFocus);\n    document.body.addEventListener('mousedown', this.preFocus);\n    document.body.addEventListener('focusin', this.addFocus);\n    document.body.addEventListener('focusout', this.removeFocus);\n    if (this.props.layout === 'MOBILE') {\n      this.orderReviewSection = document.getElementById('orderreview');\n      this.checkoutWrapper = document.getElementById('checkout-wrapper');\n      window.addEventListener('scroll', throttle(this.handleScroll, 300));\n    }\n    window.addEventListener('beforeunload', this.beforeUnload);\n  }\n\n  // TODO https://jira.nike.com/browse/CCC-5034\n  // eslint-disable-next-line camelcase\n  UNSAFE_componentWillReceiveProps(nextProps) {\n    if ((nextProps.shippingReady || nextProps.errors.length > 0) &&\n      !this.props.initialLoaded && !nextProps.initialLoaded) {\n      this.props.dispatch({\n        type: SET_INITIAL_LOADED,\n        payload: true,\n      });\n    }\n  }\n\n  componentWillUnmount() {\n    window.removeEventListener('scroll', this.handleScroll);\n  }\n\n  onModalClose() {\n    const {\n      errors,\n      cartHref,\n      dispatch,\n      isFauxpis,\n      shouldDirectToPayment,\n      shouldResetPaymentGuid,\n      hasShippingFieldError,\n      hasPaymentFieldError,\n    } = this.props;\n    // if we ever need more than just these cases we should add another\n    // file/middleware/something to handle other errors\n    if (isProductNotBuyable(errors) || isTimeoutError(errors) || isProductOutOfStock(errors) || hasNoItems(errors)) { // eslint-disable-line max-len\n      window.location = cartHref;\n    } else if (shouldResetPaymentGuid) {\n      dispatch({\n        type: SET_GUID,\n        payload: v4(),\n      });\n      dispatch({\n        type: CLEAR_ERRORS,\n      });\n    } else if (isFauxpis) {\n      dispatch({\n        type: SET_USER_SELECTED_SHIPPING_OPTIONS,\n        payload: false,\n      });\n      dispatch({\n        type: CLEAR_ERRORS,\n      });\n    } else if (shouldDirectToPayment || hasPaymentFieldError) {\n      dispatch({\n        type: SET_GUID,\n        payload: v4(),\n      });\n      dispatch({\n        type: EDIT_PAYMENT_SECTION,\n      });\n      dispatch({\n        type: SET_CONTINUE_TO_ORDER_REVIEW,\n        payload: false,\n      });\n      dispatch({\n        type: CLEAR_ERRORS,\n      });\n    } else if (hasShippingFieldError) {\n      dispatch(openAddressForm());\n    } else {\n      dispatch({\n        type: CLEAR_ERRORS,\n      });\n    }\n  }\n\n  preFocus(e) {\n    this.usingMouse = e.type === 'mousedown';\n  }\n\n  addFocus() {\n    document.body.classList.toggle('mouse-focus', this.usingMouse);\n    document.body.classList.toggle('keyboard-focus', !this.usingMouse);\n  }\n\n  removeFocus() { // eslint-disable-line class-methods-use-this\n    document.body.classList.toggle('mouse-focus', false);\n    document.body.classList.toggle('keyboard-focus', true);\n  }\n\n  handleScroll() {\n    let orderReviewHeight;\n\n    if (this.props.displayOrderReviewSection && this.orderReviewSection) {\n      if (this.state.fixCtaButton) {\n        // if cta is floating get ORS height as if cta was inside so that we don't\n        // de-float until we have enough room to show it all onscreen\n        orderReviewHeight = this.orderReviewSection.offsetHeight + 88;\n      } else {\n        orderReviewHeight = this.orderReviewSection.offsetHeight - 44;\n      }\n\n      const orderReviewDistFromTop =\n        this.orderReviewSection.offsetTop + this.checkoutWrapper.offsetTop;\n      const distanceScrolled =\n        window.pageYOffset ||\n        document.documentElement.scrollTop ||\n        document.body.scrollTop ||\n        0;\n      const bottomOfPage = distanceScrolled + window.innerHeight;\n      const footerTop = orderReviewHeight + orderReviewDistFromTop;\n\n      this.setState({ fixCtaButton: bottomOfPage < footerTop });\n    } else if (document.getElementById('placeorderAB4790')) {\n      if (this.state.fixCtaButton) {\n        orderReviewHeight = document.getElementById('placeorderAB4790').offsetHeight + 88;\n      } else {\n        orderReviewHeight = document.getElementById('placeorderAB4790').offsetHeight - 44;\n      }\n\n      const orderReviewDistFromTop =\n        document.getElementById('placeorderAB4790').offsetTop + document.getElementById('checkout-wrapper').offsetTop;\n      const distanceScrolled =\n        window.pageYOffset ||\n        document.documentElement.scrollTop ||\n        document.body.scrollTop ||\n        0;\n      const bottomOfPage = distanceScrolled + window.innerHeight;\n      const footerTop = orderReviewHeight + orderReviewDistFromTop;\n\n      this.setState({ fixCtaButton: bottomOfPage < footerTop });\n    }\n  }\n\n  beforeUnload() {\n    if (this.props.orderOrPaypalProcessing) {\n      this.props.dispatch({\n        type: CHECKOUT_ABANDONED,\n        payload: {\n          isPaypal: !!this.props.paypalProcessing,\n        },\n      });\n    }\n  }\n\n  render() {\n    const {\n      checkoutUrl,\n      country,\n      dispatch,\n      errors,\n      isFauxpis,\n      isGuest,\n      isMainReady,\n      isSingleItem,\n      isSwoosh,\n      layout,\n      locale,\n      neoDelay,\n      paymentOptions,\n      paypalMismatch,\n      product,\n      inExperimentAB4114,\n      inExperimentAB4114b,\n      inExperimentAB4115,\n      inExperimentAB4116,\n      inExperimentAB4790A,\n    } = this.props;\n    const productName = get(product, 'fullTitle', '');\n    const loadingOpacity = isMainReady ? 0 : 1;\n    const visibility = isMainReady ? 'hidden' : 'visible';\n    const display = isMainReady ? 'none' : 'block';\n    if (isMainReady) {\n      // Delay neo script load an additional couple seconds to ensure\n      // initial set of checkout/payment calls are done.\n      // If no initial delay is specified, use zero.\n      setTimeout(loadNeoScript, neoDelay || 0);\n    }\n    return (\n      <div>\n        <section\n          className={'loading-fade ncss-col-sm-12 bg-white z9'}\n          style={{ opacity: loadingOpacity, visibility, display }}\n        >\n          <Loading />\n        </section>\n        {!paypalMismatch &&\n          <div className=\"checkout-placeholder ncss-container fixed-fluid\">\n            <CheckoutBanner />\n            <div\n              id=\"checkout-wrapper\"\n              className={'checkout-container ncss-row'}\n              // Set checkout min-height to view height - (header + footer height)\n              // to keep footer from showing up in the middle of the page.\n              style={{ minHeight: 'calc(100vh - 396px)' }}\n            >\n              <div className=\"ncss-col-sm-12\">\n                <div className=\"ncss-row\">\n                  <OrderProcessing />\n                  <AdyenWrapper dispatch={dispatch} />\n                  <div id=\"adyen-root\" />\n                  <ErrorModal\n                    errors={errors}\n                    isSwoosh={isSwoosh}\n                    productName={productName}\n                    onClose={this.onModalClose}\n                    country={country}\n                    locale={locale}\n                    isFauxpis={isFauxpis}\n                    isSingleItem={isSingleItem}\n                  />\n                  <DynamicDeferredPayment />\n                  <MainSection\n                    fixCtaButton={this.state.fixCtaButton}\n                    layout={layout}\n                    uniteEmailCheck={isGuest}\n                    inExperimentAB4114={inExperimentAB4114}\n                    inExperimentAB4114b={in