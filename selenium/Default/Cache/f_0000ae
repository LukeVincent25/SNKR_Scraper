// When abandoning PPE and changing to another payment type\n    if (isPaypalExpress) {\n      dispatch(convertPaypalExpressToPaypalMark());\n    }\n\n    dispatch({ type: SET_SELECTED_PAYMENT_METHOD, payload: value || CREDIT_DEBIT });\n    dispatch({\n      type: SET_GUID,\n      payload: v4(),\n    });\n    const eventMap = {\n      paymentOptions: TOGGLE_PAYMENT_OPTIONS,\n      newCard: ADD_NEW_CARD,\n      cancel: CANCEL_NEW_CARD,\n    };\n    if (get(this, 'props.dispatch') && eventMap[name]) {\n      const val = value.toLowerCase();\n      dispatch({\n        type: eventMap[name],\n        payload: {\n          [PAYPAL]: val === PAYPAL,\n          [CREDIT_DEBIT]: val === CREDIT_DEBIT,\n          // these options will not be used in ab test,\n          // but retaining in case this component gets promoted\n          [KLARNA]: val === KLARNA,\n          [SOFORT]: val === SOFORT,\n          [COD]: val === COD.toLowerCase(),\n          [IDEAL]: val === IDEAL,\n        },\n      });\n    }\n\n    const thirdPartyPayment = this.props.thirdPartyPayment;\n    if (thirdPartyPayment === 'paypal') {\n      dispatch(fetchPaymentOptions());\n      dispatch({\n        type: SET_PAYMENT_METHOD,\n        payload: this.props.paymentMethods[Object.keys(this.props.paymentMethods)[0]] || {},\n      });\n    }\n  }\n\n  validateBillingAddress() {\n    const {\n      dispatch,\n      isBillingDataInvalid,\n      sameAsShipping,\n      isClickAndCollect,\n      isStoredPaymentSelected,\n    } = this.props;\n    if (!isStoredPaymentSelected && (!sameAsShipping || isClickAndCollect) &&\n     isBillingDataInvalid) {\n      // validate\n      dispatch(touch('genericAddressForm', 'address.firstName', 'address.lastName', 'address.postalCode', 'address.address1', 'address.city'));\n      return false;\n    }\n    return true;\n  }\n\n  processNewPaymentMethod() {\n    const {\n      showNewCard,\n      isCCValid,\n      totalsCoveredByPrepaid,\n      canSubmitCCOrder,\n      canSubmitKlarna,\n      canSubmitSofort,\n      canSubmitCOD,\n      canSubmitIdeal,\n      isApplePay,\n    } = this.props;\n\n    // Note: showNewCard is still true if the user has added a GC that covers total\n    // so we need to also evaluate totalsCoveredByPrepaid\n    if (showNewCard && isCCValid && !totalsCoveredByPrepaid && canSubmitCCOrder) {\n      return this.handleUseCard(!totalsCoveredByPrepaid, false, true);\n    // run klarna, sofort, COD payment preview stuff\n    }\n    if (canSubmitKlarna) {\n      return this.handleKlarnaV2Submit();\n    }\n    if (canSubmitSofort) {\n      return this.handleSofort();\n    }\n    if (canSubmitCOD) {\n      return this.handleCOD();\n    }\n    if (canSubmitIdeal) {\n      return this.handleIdeal();\n    }\n    if (isApplePay) {\n      return this.handleApplePay();\n    }\n    return Promise.resolve();\n  }\n\n  handleCOD() {\n    const { dispatch, address } = this.props;\n\n    return new Promise((resolve) => {\n      if (this.validateBillingAddress()) {\n        const paymentMethod = {\n          type: 'COD',\n          id: v4(),\n          billingAddress: address,\n        };\n        dispatch({\n          type: UNEDIT_PAYMENT_SECTION,\n        });\n        dispatch({\n          type: SET_PAYMENT_METHOD,\n          payload: paymentMethod,\n        });\n        dispatch({\n          type: SET_BILLING_ADDRESS,\n          payload: address,\n        });\n        dispatch({\n          type: SET_PAYMENT_TOKEN,\n          payload: null,\n        });\n\n        dispatch({\n          type: SET_PAYMENT_PROCESSING,\n          payload: false,\n        });\n        resolve(true);\n      } else {\n        resolve(false);\n      }\n    });\n  }\n\n  handleKlarnaV2Submit() {\n    const {\n      isKlarnaBillingSameAsShipping,\n      klarnaShippingAddress,\n      klarnaChecked,\n    } = this.props;\n    if (!klarnaChecked) {\n      return Promise.resolve();\n    }\n    return this.handleUpdateKlarnaSession().then(() => {\n      authorizeKlarna({\n        shippingAddress: klarnaShippingAddress,\n        authCallback: this.handleKlarnaAuthorization,\n        sameAsShipping: isKlarnaBillingSameAsShipping,\n      });\n    });\n  }\n\n  handleUpdateKlarnaSession() {\n    const {\n      dispatch,\n    } = this.props;\n    // if needed, conditions for updating klarna session go here\n    return dispatch(updateKlarnaSession());\n  }\n\n  handleKlarnaAuthorization(res) {\n    const {\n      dispatch,\n    } = this.props;\n    const {\n      approved,\n      authorization_token, // eslint-disable-line\n      finalize_required, //eslint-disable-line\n    } = res;\n\n    if (approved) {\n      const authResponse = {\n        authorization_token,\n        finalize_required,\n        approved,\n      };\n      dispatch({\n        type: SET_ORDER_PROCESSING,\n        payload: true,\n      });\n      return dispatch(klarnaPaymentPreview({}, authResponse))\n        .then(() => dispatch(submitCheckout()));\n    }\n    console.error(res); // eslint-disable-line\n    return Promise.resolve();\n  }\n\n  handleSofort() {\n    const { dispatch, selectedPaymentBillingAddress, returnURL, cancelURL } = this.props;\n    return new Promise((resolve) => {\n      if (this.validateBillingAddress()) {\n        const paymentMethod = {\n          type: 'Sofort',\n          id: v4(),\n          returnURL,\n          cancelURL,\n          billingAddress: selectedPaymentBillingAddress,\n        };\n        dispatch({\n          type: UNEDIT_PAYMENT_SECTION,\n        });\n        dispatch({\n          type: SET_PAYMENT_METHOD,\n          payload: paymentMethod,\n        });\n        dispatch({\n          type: SET_BILLING_ADDRESS,\n          payload: selectedPaymentBillingAddress,\n        });\n        dispatch({\n          type: SET_PAYMENT_TOKEN,\n          payload: null,\n        });\n        dispatch({\n          type: SET_PAYMENT_PROCESSING,\n          payload: false,\n        });\n        resolve(true);\n      } else {\n        resolve(false);\n      }\n    });\n  }\n\n  handleApplePay() {\n    return this.props.dispatch(startFlow());\n  }\n\n  handleIdeal() {\n    const {\n      dispatch,\n      selectedPaymentBillingAddress,\n      returnURL,\n      cancelURL,\n      selectedBank } = this.props;\n    return new Promise((resolve) => {\n      if (this.validateBillingAddress()) {\n        const paymentMethod = {\n          type: 'iDeal',\n          id: v4(),\n          returnURL,\n          cancelURL,\n          bankName: selectedBank,\n          billingAddress: undefined,\n        };\n        dispatch({\n          type: UNEDIT_PAYMENT_SECTION,\n        });\n\n        dispatch({\n          type: SET_PAYMENT_METHOD,\n          payload: paymentMethod,\n        });\n\n        dispatch({\n          type: SET_BILLING_ADDRESS,\n          payload: selectedPaymentBillingAddress,\n        });\n\n        dispatch({\n          type: SET_PAYMENT_TOKEN,\n          payload: null,\n        });\n\n        dispatch({\n          type: SET_PAYMENT_PROCESSING,\n          payload: false,\n        });\n        resolve(true);\n      } else {\n        resolve(false);\n      }\n    });\n  }\n\n  submitOrder() {\n    const {\n      canSubmitPayPal,\n      canSubmitKlarna,\n      canSubmitCCOrder,\n      canSubmitSofort,\n      canSubmitCOD,\n      canSubmitIdeal,\n      isApplePay,\n    } = this.props;\n    const position = { position: 'bottom' };\n    if (canSubmitPayPal) {\n      if (this.props.isPayPal\n        && !this.props.isPaypalExpress\n        && !this.props.totalsCoveredByPrepaid) {\n        return this.props.dispatch(postPaypalMark(position));\n      }\n    } else if (canSubmitCCOrder || canSubmitKlarna || canSubmitSofort || canSubmitCOD || canSubmitIdeal || isApplePay) { // eslint-disable-line max-len\n      return this.handleABOrderClick();\n    }\n    // do something for non CC/PayPal orders\n    return this.props.dispatch(placeOrder({ position, isPaypal: false }));\n  }\n\n  handleABOrderClick() {\n    const {\n      dispatch,\n      totalsCoveredByPrepaid,\n      isBillingDataInvalid,\n      canSubmitKlarna,\n      canSubmitSofort,\n      canSubmitCOD,\n      canSubmitIdeal,\n    } = this.props;\n    this.forceHideKeyboard();\n\n    if (!this.validateBillingAddress()) {\n      return false;\n    }\n\n    if (totalsCoveredByPrepaid && isBillingDataInvalid) {\n      return dispatch({\n        error: true,\n        type: SET_ERRORS,\n        payload: [{\n          crcCode: 'E4C78047',\n        }],\n      });\n    }\n    // klarna, sofort, cod, ideal orders\n    // processNewPaymentMethod sets payment method, and clears out payment preview actions\n    // then we're handling payment preview here, then submitCheckout\n    if (canSubmitKlarna || canSubmitSofort || canSubmitCOD || canSubmitIdeal) {\n      return this.processNewPaymentMethod().then((canSubmitOrder) => {\n        if (canSubmitOrder) {\n          dispatch({\n            type: SET_ORDER_PROCESSING,\n            payload: true,\n          });\n          dispatch(postPaymentPreview(false, true)).then(() => dispatch(submitCheckout()))\n            .catch(() => {\n              dispatch({\n                type: SET_ORDER_PROCESSING,\n                payload: false,\n              });\n            });\n        }\n      });\n    }\n    // this is for credit card or paypal orders-\n    // This flow is abstracted into two functions because processNewPaymentMethod may modify the\n    // current payment method in state and we need placeOrderAB to pick that up.\n    return this.processNewPaymentMethod()\n      .then(() => this.placeOrderAB())\n      // Suppressing unhandled promise rejection errors since there is no error handling on\n      // submitOrder\n      .catch(() => {});\n  }\n\n  placeOrderAB() {\n    const {\n      selectedPaymentBillingAddress,\n      dispatch,\n      isCCValid,\n      isCreditDebit,\n      isGuest,\n      isPaymentReadyToPost,\n      isPaypalExpress,\n      paymentMethod,\n      paymentMethods,\n      validMemberCC,\n      showNewCard,\n      totalsCoveredByPrepaid,\n      canSubmitCCOrder,\n    } = this.props;\n    return new Promise((resolve) => {\n      if (!isGuest) {\n        dispatch({\n          type: SET_BILLING_ADDRESS,\n          payload: selectedPaymentBillingAddress,\n        });\n      }\n      if (isPaymentReadyToPost) {\n        let updatingExpiry = false;\n\n        const updatePaymentMethod = {\n          ...paymentMethod,\n          billingAddress: { ...selectedPaymentBillingAddress },\n        };\n        if (!totalsCoveredByPrepaid) {\n          // setting flag for dispatch, so we can reassign isExpired\n          if (updatePaymentMethod.isExpired) {\n            updatingExpiry = true;\n            updatePaymentMethod.expiryYear = get(this, 'data.ed.y');\n            updatePaymentMethod.expiryMonth = get(this, 'data.ed.m');\n            updatePaymentMethod.isExpired = false;\n          }\n          if (canSubmitCCOrder) {\n          // Since this is a creditcard we'll remove the third party payment here\n            delete updatePaymentMethod.thirdPartyPayment;\n          }\n          // Updating creditcard - no longer need to validate the cvv\n          // if the user returns to payment\n          if (validMemberCC && canSubmitCCOrder) {\n            updatePaymentMethod.validateCVV = false;\n            dispatch({\n              type: SET_VALIDATE_PAYMENT_CVV,\n              payload: updatePaymentMethod,\n            });\n          }\n          // $SR Will need to revisit - Next line -\n          const duplicateId = getDuplicateStoredId(paymentMethod, paymentMethods);\n          if (duplicateId && !isGuest) {\n            updatePaymentMethod.id = duplicateId;\n            dispatch({\n              type: UPDATE_PAYMENT_METHOD,\n              payload: updatePaymentMethod,\n            });\n          }\n        }\n        if (updatingExpiry) {\n          dispatch(updateStoredPayment(updatePaymentMethod)).then(result => resolve(result));\n        } else {\n          resolve(true);\n        }\n      } else if (isCreditDebit) {\n        // trigger validation of payment iframe fields\n        postMessageToChildFrame(IFRAME_MSG, IFRAME_DOMAIN, IFRAME_ELEMENT);\n        resolve(false);\n      } else {\n        this.props.dispatch({\n          error: true,\n          type: SET_ERRORS,\n          payload: [{\n            crcCode: 'B5013F9F', // Credit card information is invalid\n          }],\n        });\n        resolve(false);\n      }\n    }).then((shouldPlaceOrder) => {\n      // We need to explicitly evaluate false because a possible return from updateStoredPayment\n      // is null on success\n      if (shouldPlaceOrder !== false) {\n        if (showNewCard) {\n          // isCCValid only is updates by a new card or a cvv needs validation stored payment\n          // selecting a valid stored card after canceling out of invalid new card or stored\n          // needs validation card will leave this prop out of date\n          if (isCCValid || totalsCoveredByPrepaid) {\n            // we are using the existing isPayPal flag to tell placeOrder\n            // whether to call paymentPreview from placeOrder function.\n            this.props.dispatch(placeOrder({ position: 'bottom', isPayPal: (!isPaypalExpress && !totalsCoveredByPrepaid) }));\n          }\n        } else {\n          this.props.dispatch(placeOrder({ position: 'bottom', isPaypal: !isPaypalExpress }));\n        }\n      }\n    });\n  }\n\n  render() {\n    const {\n      contactInfoValidIfRequired,\n      dispatch,\n      enablePaynowButton,\n      fixCtaButton,\n      isApplePay,\n      isGuest,\n      isMobile,\n      isPayPal,\n      isPaypalExpress,\n      isStoredPaymentSelected,\n      totalsCoveredByPrepaid,\n      hasOnlyDigitalGiftCard,\n    } = this.props;\n    if (document.querySelector('#payment') && !document.querySelector('#payment.ab4790')) {\n      document.querySelector('#payment').className += ' ab4790';\n    }\n\n    con