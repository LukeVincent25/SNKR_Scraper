{emailInUse}\n          disabled={props.emailDisabled || disabled}\n          animateInputLabels\n          isRequired\n          tooltipContent={[{ type: 'message', text: formatMessage(messages.emailTooltip), style: 'tip-left' }]}\n          tooltipId={'emailTooltip'}\n          showIcon\n          trackTooltip={() => { dispatch({ type: TOGGLE_TOOLTIP, payload: 'email' }); }}\n          autoComplete={'email'}\n          showPositiveValidation\n          validationMarker={validationMarker}\n          {...(props.input ? { ...{ input: props.input } } : {})}\n        />\n        {showLoginSection &&\n          <div className=\"d-md-h ncss-col-sm-12 mb2-sm mb4-md va-sm-t\">\n            { loginSection }\n          </div>\n        }\n\n        <FieldWrapper\n          wrapperStyle={'ncss-col-sm-12 ncss-col-md-6  ncss-input-container mod-ncss-input-container  mb2-sm mb0-md va-sm-t'}\n          label={formatMessage(messages.phoneNumber)}\n          id={'phoneNumber'}\n          type={'input'}\n          inputType=\"tel\"\n          validate={inlineValidator('phoneNumber')}\n          disabled={disabled}\n          animateInputLabels\n          isRequired\n          tooltipContent={[{ type: 'message', text: formatMessage(messages.phoneNumberTooltip) }]}\n          tooltipId={'phoneNumTooltip'}\n          showIcon\n          normalize={normalizePhone}\n          trackTooltip={() => { dispatch({ type: TOGGLE_TOOLTIP, payload: 'phoneNumber' }); }}\n          autoComplete={'tel'}\n          showPositiveValidation\n          validationMarker={validationMarker}\n        />\n      </div>\n      {showLoginSection &&\n        <div className=\"d-sm-h d-md-b ncss-col-md-12 mb2-sm mb4-md va-sm-t\">\n          { loginSection }\n        </div>\n      }\n    </div>\n  );\n}\n\nAddressFormComponent.propTypes = {\n  countryOptions: PropTypes.arrayOf(\n    PropTypes.shape({})\n  ),\n  address2Open: PropTypes.bool,\n  allowNickName: PropTypes.bool,\n  disabled: PropTypes.bool,\n  dispatch: Pr