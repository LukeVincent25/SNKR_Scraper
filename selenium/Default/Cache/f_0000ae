pen';\n    }\n\n    return (\n      <section className={sectionClassName}>\n        <div className=\"ncss-row mod-u-full-width m0-sm prl2-sm\">\n          {\n            optionsFormatted.map((option = {}) =>\n              <PaymentOptionCard\n                id={option.id}\n                key={option.id}\n                isSelected={selectedOption === option.id}\n                isDisabled={disabledOptions.includes(option.id)}\n                onSelect={optionId => this.handleChangePaymentOption(optionId)}\n                isBankTransfer={isBankTransfer}\n              />)\n          }\n        </div>\n      </section>\n    );\n  }\n}\n\nPaymentOptionsCN.propTypes = {\n  options: PropTypes.arrayOf(PropTypes.shape({})),\n  disabledOptions: PropTypes.arrayOf(PropTypes.string),\n  selectedOption: PropTypes.string,\n  defaultOption: PropTypes.string,\n  onChange: PropTypes.func,\n  isWechatBrowser: PropTypes.bool,\n  isBankTransfer: PropTypes.bool,\n  allPaymentOptionsCN: PropTypes.arrayOf(PropTypes.shape({})),\n};\n\nPaymentOptionsCN.defaultProps = {\n  defaultOption: defaultsByCountry.cn.defaultSelectedPaymentMethod,\n  isWechatBrowser: false,\n  isBankTransfer: false,\n};\n\nexport default PaymentOptionsCN;\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/payment/PaymentOptionsCN.js","/* eslint-disable react/no-unused-prop-types, jsx-a11y/no-static-element-interactions, jsx-a11y/no-noninteractive-element-interactions, jsx-a11y/no-noninteractive-tabindex, max-len */\nimport React, { Component } from 'react';\nimport PropTypes from 'prop-types';\nimport classnames from 'classnames';\n/**\n * keyboard key codes\n */\nexport const SPACE_KEY_CODE = 32; // space\nexport const ENTER_KEY_CODE = 13; // enter\n\n/**\n * ```\n * import CheckboxToggler from 'checkboxToggler';\n * ```\n *\n * From a standpoint of interaction, this component acts like a collapsable checkbox.\n *\n * As an example, lets say we want to display a product voucher checkbox:\n *\n * ```\n * <CheckboxToggler\n *   id=\"productVoucer\"\n *   name=\"paymentMethod\"\n *   onSelected={(tgt, checked) => { console.log(tgt, checked) }},\n *   labelStyles=\"pl7-sm\"\n *   selected={false}\n *   className=\"mb1-sm\"\n *   displayContentOnChecked={true}\n *  />\n * ```\n *\n */\nexport default class CheckboxToggler extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      selected: this.props.selected,\n    };\n\n    this.toggleCheckedState = this.toggleCheckedState.bind(this);\n    this.handleKeyDown = this.handleKeyDown.bind(this);\n  }\n\n  toggleCheckedState() {\n    // When syncing with the selected prop type the state.selected can get out of sync but this\n    // does not have actual affect on the component other than annoying the dev working with it.\n    const checkedState = this.props.syncWithSelected ? this.props.selected : this.state.selected;\n    this.setState({\n      selected: !checkedState, // toggle state\n    });\n    this.props.onSelected(!checkedState);\n  }\n\n  handleKeyDown(e) {\n    // We need to handle keydown for accessibility\n    const keyCode = e.keyCode;\n    if (keyCode === SPACE_KEY_CODE) {\n      this.toggleCheckedState();\n    }\n  }\n\n  render() {\n    const {\n      name,\n      label,\n      children,\n      id,\n      labelStyle,\n      className,\n      childrenStyle,\n      displayContentOnChecked,\n      syncWithSelected,\n      toolTip,\n    } = this.props;\n\n    const selected = syncWithSelected ? this.props.selected : this.state.selected;\n    const checkboxStyles = classnames('mod-ncss-checkbox-container ncss-checkbox-container', className);\n    const labelStyles = classnames('ncss-label', labelStyle);\n    const showContent = displayContentOnChecked ? selected : !selected; // $SR\n    const childrenStyles = classnames(childrenStyle, !showContent && 'd-sm-h');\n\n    return (\n      <div className={'uikit-checkbox-toggler'}>\n        <div className={checkboxStyles}>\n          <input\n            type=\"checkbox\"\n            name={name}\n            className=\"ncss-checkbox\"\n            id={id}\n            checked={selected}\n            aria-checked={selected}\n            onClick={() => this.toggleCheckedState()}\n            onChange={() => {}} // this is to clear out a warning\n          />\n          <label\n            tabIndex=\"0\"\n            onKeyDown={e => this.handleKeyDown(e)}\n            className={labelStyles}\n            htmlFor={id}\n            aria-label={label}\n          >\n            { label }\n          </label>\n          {toolTip}\n        </div>\n        <div className={childrenStyles}>\n          { children }\n        </div>\n      </div>\n    );\n  }\n}\n\nCheckboxToggler.defaultProps = {\n  syncWithSelected: false,\n};\n\nCheckboxToggler.propTypes = {\n  /**\n   * Unique identifier for checkbox\n   */\n  id: PropTypes.string.isRequired,\n  /*\n   * Group name to identify group of multiple checkboxes\n   */\n  name: PropTypes.string.isRequired,\n  /*\n   * Label for the checkbox\n   */\n  label: PropTypes.string.isRequired,\n  /**\n   * Extra style to be appended to the label\n   */\n  labelStyle: PropTypes.string,\n  /**\n   * Extra style to be appended to the wrapper of the children component\n   */\n  childrenStyle: PropTypes.string,\n  /**\n   * Extra style to be appended to the wrapper\n   */\n  className: PropTypes.string,\n  /**\n   * function that will be called when the checkbox is checked/unchecked\n   */\n  onSelected: PropTypes.func.isRequired,\n  /**\n   * state of initial checkbox, checked or unchecked\n   */\n  selected: PropTypes.bool,\n  /**\n  * ignore internal state management and base checked status on selected proptype\n  */\n  syncWithSelected: PropTypes.bool,\n  /**\n   * DOM elements\n   */\n  children: PropTypes.element,\n  /**\n   * flag that denotes an inverse toggle to display data\n   */\n  displayContentOnChecked: PropTypes.bool,\n\n  toolTip: PropTypes.shape({}),\n};\n\n\n\n