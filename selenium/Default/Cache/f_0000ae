Label: {\n    id: 'checkout.shipping.label.scheduleDelivery',\n    description: 'Specify the delivery date and time',\n    defaultMessage: 'Specify the delivery date and time',\n  },\n});\n\nexport default function ShippingScheduledDeliveriesWrapper(props) {\n  const {\n    isJP,\n    isScheduledDelivery,\n    locale,\n    optionalDates,\n    optionalTimes,\n    selectedDate,\n    selectedTime,\n    toggleIsScheduledDelivery,\n    handleScheduledDeliveryDateChange,\n    handleScheduledDeliveryTimeChange,\n  } = props;\n  return (\n    <div className=\"ncss-row\">\n      <section className=\"js-delivery-time ncss-col-sm-12 prl5-sm\">\n        <div className=\"ncss-checkbox-container\" id=\"scheduleDeliveryTime\">\n          <input\n            type=\"checkbox\"\n            id=\"deliveryTimeToggle\"\n            name=\"deliveryDateTime\"\n            className=\"ncss-checkbox\"\n            checked={isScheduledDelivery}\n            onChange={toggleIsScheduledDelivery}\n          />\n          <label htmlFor=\"deliveryTimeToggle\" className=\"ncss-label pl6-sm\">{formatMessage(messages.scheduleLabel)}</label>\n        </div>\n        {\n          isScheduledDelivery &&\n          <ShippingScheduledDeliveries\n            isJP={isJP}\n            locale={locale}\n            optionalDates={optionalDates}\n            optionalTimes={optionalTimes}\n            selectedDate={selectedDate}\n            selectedTime={selectedTime}\n            onDateChange={handleScheduledDeliveryDateChange}\n            onTimeChange={handleScheduledDeliveryTimeChange}\n          />\n        }\n      </section>\n    </div>\n  );\n}\n\nShippingScheduledDeliveriesWrapper.propTypes = {\n  isJP: PropTypes.bool,\n  isScheduledDelivery: PropTypes.bool,\n  locale: PropTypes.string,\n  optionalDates: PropTypes.arrayOf(PropTypes.string),\n  optionalTimes: PropTypes.arrayOf(PropTypes.shape({\n    startDateTime: PropTypes.string,\n    endDateTime: PropTypes.string,\n  })),\n  selectedDate: PropTypes