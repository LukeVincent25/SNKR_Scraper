de\"\n            type=\"input\"\n            validate={inlineValidator('postalCode')}\n            disabled={disabled}\n            normalize={replaceFullWidthNumerals}\n          />\n          <div className=\"ncss-col-sm-12 ncss-col-md-6 mod-ncss-input-container ncss-input-container mb4-sm va-sm-t no-overflow\">\n            <FieldWrapper\n              wrapperStyle={'d-sm-h mod-ncss-input-container mb2-sm mb4-md va-sm-t'}\n              label={formatMessage(messages.country)}\n              id={'country'}\n              disabled\n            />\n            <div className=\"fakeLabel pr6-m\">\n              {formatMessage(messages.country)}\n              <Tooltip\n                tooltipEnabled\n                tooltipContent={[{\n                  type: 'message',\n                  text: formatMessage(messages.countryTooltip),\n                  style: 'tip-left',\n                }]}\n                tooltipId=\"countryTooltip\"\n                showIcon\n              />\n            </div>\n            <div className={'fakeInput border-medium-grey ncss-col-sm-12 pt2-sm pr4-sm pb2-sm pl4-sm text-color-grey'}>{formatMessage(messages.countryName)}</div>\n          </div>\n          <StateSelector\n            wrapperStyle=\"ncss-col-sm-12 ncss-col-md-6 ncss-select-container mb4-sm va-sm-t\"\n            label={formatMessage(messages.prefecture)}\n            id=\"state\"\n            options={JPStates.ja}\n            validate={inlineValidator('stateJP')}\n            disabled={disabled}\n            optionsLabel={formatMessage(messages.selectPrefectureCta)}\n          />\n          <FieldWrapper\n            wrapperStyle=\"ncss-col-sm-12 ncss-col-md-6 mod-ncss-input-container ncss-input-container mb4-sm va-sm-t\"\n            label={formatMessage(messages.city)}\n            id=\"city\"\n            type=\"input\"\n            validate={inlineValidator('city')}\n            disabled={disabled}\n            normalize={replaceFullWidthNumerals}\n          />\n          <FieldWrapper\n            wrapperStyle=\"ncss-col-sm-12 ncss-col-md-6  ncss-input-container mod-ncss-input-container  mb4-sm va-sm-t\"\n            label={formatMessage(messages.townArea)}\n            id=\"address3\"\n            type=\"input\"\n            validate={inlineValidator('address3')}\n            disabled={disabled}\n            normalize={replaceFullWidthNumerals}\n          />\n          <FieldWrapper\n            wrapperStyle=\"ncss-col-sm-12 ncss-col-md-6  ncss-input-container mod-ncss-input-container  mb4-sm va-sm-t\"\n            label={formatMessage(messages.houseAddress)}\n            id=\"address1\"\n            type=\"input\"\n            validate={inlineValidator('address1')}\n            disabled={disabled}\n            normalize={replaceFullWidthNumerals}\n          />\n          <FieldWrapper\n            wrapperStyle=\"ncss-col-sm-12 mod-ncss-input-container ncss-input-container mb4-sm va-sm-t\"\n            label={formatMessage(messages.apartmentName)}\n            id=\"address2\"\n            type=\"input\"\n            validate={inlineValidator('address2')}\n            disabled={disabled}\n            tooltipEnabled\n            tooltipContent={[{ type: 'message', text: formatMessage(messages.apartmentNameTooltip), style: 'tip-left' }]}\n            tooltipId=\"apartmentName\"\n            showIcon\n            normalize={replaceFullWidthNumerals}\n          />\n        </React.Fragment>\n      }\n      {/* End rendering Japanese */}\n\n      {/* Start rendering English */}\n      {renderingEnglishInJapan &&\n        <React.Fragment>\n          <FieldWrapper\n            wrapperStyle=\"ncss-col-sm-6 ncss-col-md-6 mod-ncss-input-container ncss-input-container mb4-sm va-sm-t\"\n            label={formatMessage(messages.firstName)}\n            id=\"firstName\"\n            type=\"input\"\n            validate={inlineValidator('firstName')}\n            disabled={disabled}\n            normalize={replaceFullWidthNumerals}\n          />\n          <FieldWrapper\n            wrapperStyle=\"ncss-col-sm-6 ncss-col-md-6 mod-ncss-input-container ncss-input-container mb4-sm va-sm-t\"\n            label={formatMessage(messages.lastName)}\n            id=\"lastName\"\n            type=\"input\"\n            validate={inlineValidator('lastName')}\n            disabled={disabled}\n            normalize={replaceFullWidthNumerals}\n          />\n\n          <FieldWrapper\n            wrapperStyle=\"ncss-col-sm-12  ncss-input-container mod-ncss-input-container  mb4-sm va-sm-t\"\n            label={formatMessage(messages.houseAddress)}\n            id=\"address1\"\n            type=\"input\"\n            validate={inlineValidator('address1')}\n            disabled={disabled}\n            normalize={replaceFullWidthNumerals}\n          />\n          <FieldWrapper\n            wrapperStyle=\"ncss-col-sm-12 ncss-col-md-6 mod-ncss-input-container ncss-input-container mb4-sm va-sm-t\"\n            label={formatMessage(messages.apartmentNameEn)}\n            id=\"address2\"\n            type=\"input\"\n            validate={inlineValidator('address2')}\n            disabled={disabled}\n            tooltipEnabled\n            tooltipContent={[{ type: 'message', text: formatMessage(messages.apartmentNameTooltipEn), style: 'tip-left' }]}\n            tooltipId=\"apartmentName\"\n            showIcon\n            normalize={replaceFullWidthNumerals}\n          />\n          <FieldWrapper\n            wrapperStyle=\"ncss-col-sm-12 ncss-col-md-6  ncss-input-container mod-ncss-input-container  mb4-sm va-sm-t\"\n            label={formatMessage(messages.townAreaEn)}\n            id=\"address3\"\n            type=\"input\"\n            validate={inlineValidator('address3JpEn')}\n            disabled={disabled}\n            normalize={replaceFullWidthNumerals}\n          />\n\n          <FieldWrapper\n            wrapperStyle=\"ncss-col-sm-12 ncss-col-md-6 mod-ncss-input-container ncss-input-container mb4-sm va-sm-t\"\n            label={formatMessage(messages.city)}\n            id=\"city\"\n            type=\"input\"\n            validate={inlineValidator('city')}\n            disabled={disabled}\n            normalize={replaceFullWidthNumerals}\n          />\n          <StateSelector\n            wrapperStyle=\"ncss-col-sm-12 ncss-col-md-6 ncss-select-container mb4-sm va-sm-t\"\n            label={formatMessage(messages.prefecture)}\n            id=\"state\"\n            options={JPStates.en}\n            validate={inlineValidator('stateJP')}\n            disabled={disabled}\n            optionsLabel={formatMessage(messages.selectPrefectureCta)}\n          />\n          <div className=\"ncss-col-sm-12 ncss-col-md-6 mod-ncss-input-container ncss-input-container mb4-sm va-sm-t no-overflow\">\n            <FieldWrapper\n              wrapperStyle={'d-sm-h mod-ncss-input-container mb2-sm mb4-md va-sm-t'}\n              label={formatMessage(messages.country)}\n              id={'country'}\n              disabled\n            />\n            <div className=\"fakeLabel pr6-m\">\n              {formatMessage(messages.country)}\n              <Tooltip\n                tooltipEnabled\n                tooltipContent={[{\n                  type: 'message',\n                  text: formatMessage(messages.countryTooltip),\n                  style: 'tip-left',\n                }]}\n                tooltipId=\"countryTooltip\"\n                showIcon\n              />\n            </div>\n            <div className={'fakeInput border-medium-grey ncss-col-sm-12 pt2-sm pr4-sm pb2-sm pl4-sm text-color-grey'}>{formatMessage(messages.countryName)}</div>\n          </div>\n          <FieldWrapper\n            wrapperStyle=\"ncss-col-sm-6 ncss-col-md-6 mod-ncss-input-container ncss-input-container mb4-sm va-sm-t\"\n            label={formatMessage(messages.postCode)}\n            id=\"postalCode\"\n            type=\"input\"\n            validate={inlineValidator('postalCode')}\n            disabled={disabled}\n            normalize={replaceFullWidthNumerals}\n          />\n        </React.Fragment>\n      }\n      {/* End rendering Japanese */}\n\n      {containsOnlyGiftCard &&\n        <div className=\"mb2-sm\">\n          <FieldWrapper\n            wrapperStyle=\"ncss-col-sm-12 ncss-col-md-6  ncss-input-container mod-ncss-input-container  mb4-sm va-sm-t\"\n            label={formatMessage(messages.email)}\n            id=\"email\"\n            type=\"input\"\n            inputType=\"email\"\n            validate={inlineValidator('email')}\n            warn={emailWarn}\n            emailInUse={emailInUse}\n            disabled={disabled}\n            tooltipEnabled\n            tooltipContent={[{ type: 'message', text: formatMessage(messages.emailTooltip), style: 'tip-left' }]}\n            tooltipId={'emailTooltip'}\n            showIcon\n            trackTooltip={() => { dispatch({ type: TOGGLE_TOOLTIP, payload: 'email' }); }}\n            normalize={replaceFullWidthNumerals}\n          />\n          <FieldWrapper\n            wrapperStyle={'ncss-col-sm-12 ncss-col-md-6  ncss-input-container mod-ncss-input-container  mb4-sm va-sm-t'}\n            label={formatMessage(messages.phoneNumber)}\n            id=\"phoneNumber\"\n            type=\"input\"\n            inputType=\"tel\"\n            validate={inlineValidator('phoneNumber')}\n            disabled={disabled}\n            tooltipEnabled\n            tooltipContent={[{ type: 'message', text: formatMessage(messages.phoneNumberTooltip) }]}\n            tooltipId=\"phoneNumTooltip\"\n            showIcon\n            trackTooltip={() => { dispatch({ type: TOGGLE_TOOLTIP, payload: 'phoneNumber' }); }}\n            normalize={replaceFullWidthNumerals}\n          />\n        </div>\n      }\n    </div>\n  );\n}\n\nJPBillingAddressFormComponent.propTypes = {\n  dispatch: PropTypes.func,\n  disabled: PropTypes.bool,\n  country: PropTypes.string,\n  containsOnlyGiftCard: PropTypes.bool,\n  emailInUse: PropTypes.bool,\n  emailWarn: PropTypes.func,\n};\n\nexport default JPBillingAddressFormComponent;\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/JPBillingAddressForm.js","/* eslint-disable react/no-unused-prop-types */\nimport PropTypes from 'prop-types';\nimport React from 'react';\nimport classnames from 'classnames';\nimport FieldWrapper from './FieldWrapper';\nimport StateSelector from './form/StateSelector';\nimport USStates from '../../constants/USStates';\nimport AUStates from '../../constants/AUStates';\nimport CAStates from '../../constants/CAStates';\nimport BRStates from '../../constants/BRStates';\nimport { createInlineValidator } from '../../helpers/validation/index';\nimport defineMessages from '../../helpers/defineMessages';\nimport formatMessage from '../../helpers/formatMessage';\nimport normalizePhone from '../../helpers/validation/phoneValidation';\nimport { GetCountryMessages } from '../../constants/countries';\nimport Tooltip from './Tooltip';\n\n\nlet messages = defineMessages({\n  firstName: {\n    id: 'checkout.shipping.form.label.firstname',\n    description: 'First name input label',\n    defaultMessage: 'First Name',\n  },\n  lastName: {\n    id: 'checkout.shipping.form.label.lastName',\n    description: 'Last name input label',\n    defaultMessage: 'Last Name',\n  },\n  addressNickname: {\n    id: 'checkout.shipping.form.label.addressNick',\n    description: 'Address nickname input label',\n    defaultMessage: 'Address Nickname (optional)',\n  },\n  address: {\n    id: 'checkout.shipping.form.label.address',\n    description: 'Address input label',\n    defaultMessage: 'Address (We do not ship to P.O. boxes)',\n  },\n  billingAddress: {\n    id: 'checkout.payment.form.label.billingAddress',\n    description: 'Address input label',\n    defaultMessage: 'Address',\n  },\n  address2placeHolder: {\n    id: 'checkout.shipping.form.placeholder.address2',\n    description: 'Placeholder Message for address2',\n    defaultMessage: 'Optional - Company, C/O, Apt, Suite, Unit',\n  },\n  city: {\n    id: 'checkout.shipping.form.label.city',\n    description: 'City input label',\n    defaultMessage: 'City',\n  },\n\n  // Dropdown label for animation test (US only)\n  state: {\n    id: 'checkout.shipping.form.label.state',\n    description: 'State input label',\n    defaultMessage: 'State',\n  },\n\n  // Dropdown labels for US, AU, BR, and CA\n  areaAU: {\n    id: 'checkout.billing.form.label.australia.area',\n    description: 'Label for dropdown showing list of Australian states',\n    defaultMessage: 'State',\n  },\n  areaBR: {\n    id: 'checkout.billing.form.label.brazil.area',\n    description: 'Label for dropdown showing list of Brazilian states',\n    defaultMessage: 'State',\n  },\n  areaCA: {\n    id: 'checkout.billing.form.label.canada.area',\n    description: 'Label for dropdown showing list of Canadian provinces',\n    defaultMessage: 'Province',\n  },\n  areaUS: {\n    id: 'checkout.billing.form.label.unitedStates.area',\n    description: 'Label for dropdown showing list of US states',\n    defaultMessage: 'State',\n  },\n\n  // Dropdown option cta labels for US, AU, BR, and CA\n  selectAreaLabelAU: {\n    id: 'checkout.billing.form.label.australia.selectArea',\n    description: 'Select Australian state from a dropdown option call to action',\n    defaultMessage: 'Select state',\n  },\n  selectAreaLabelBR: {\n    id: 'checkout.billing.form.label.brazil.selectArea',\n    description: 'Select Brazilian state from a dropdown option call to action',\n    defaultMessage: 'Select state',\n  },\n  selectAreaLabelCA: {\n    id: 'checkout.billing.form.label.canada.selectArea',\n    description: 'Select Canadian province label from a dropdown option call to action',\n    defaultMessage: 'Select province',\n  },\n  selectAreaLabelUS: {\n    id: 'checkout.billing.form.label.unitedStates.selectArea',\n    description: 'Select US state from a dropdown option call to action',\n    defaultMessage: 'Select state',\n  },\n\n  postalCode: {\n    id: 'checkout.shipping.form.label.zipcode',\n    description: 'Postal code input label',\n    defaultMessage: 'Postal Code',\n  },\n  country: {\n    id: 'checkout.shipping.form.label.country',\n    description: 'Country input label',\n    defaultMessage: 'Country/Region',\n  },\n\n  phoneNumber: {\n    id: 'checkout.shipping.form.label.phoneNumber',\n    description: 'Phone number input label',\n    defaultMessage: 'Phone Number',\n  },\n  phoneNumberTooltip: {\n    id: 'checkout.tooltip.text.phoneNumber',\n    description: 'Phone number tooltip',\n    defaultMessage: 'Please provide a phone number so that we can contact you about your order if needed.',\n  },\n  email: {\n    id: 'checkout.shipping.form.label.email',\n    description: 'Email input label',\n    defaultMessage: 'Email',\n  },\n  emailTooltip: {\n    id: 'checkout.tooltip.text.emailTooltip',\n    description: 'Email tooltip',\n    defaultMessage: 'This is where we\\'ll send your order confirmation.',\n  },\n  requiredFields: {\n    id: 'checkout.payment.requiredFields.note',\n    description: 'Note to user that inputs are required',\n    defaultMessage: 'Required Fields',\n  },\n  countryTooltip: {\n    id: 'checkout.tooltip.text.countryTooltip',\n    description: 'Country tooltip',\n    defaultMessage: 'Changing your billing country/region will affect your payment methods. You can change your billing country/region at the top of this form.',\n  },\n});\n\nconst countryMessages = GetCountryMessages();\n\nfunction countryState(country) {\n  let stateList = [];\n  if (country === 'US') {\n    stateList = USStates;\n  } else if (country === 'AU') {\n    stateList = AUStates;\n  } else if (country === 'BR') {\n    stateList = BRStates;\n  } else if (country === 'CA') {\n    stateList = CAStates;\n  }\n  return stateList;\n}\n\nexport default function BillingFormComponent(props) {\n  const {\n    allowNickName,\n    animateInputLabels,\n    containsOnlyDigitalGiftCard,\n    country,\n    disabled,\n    isBopis,\n  } = props;\n  messages = { ...messages, countryName: countryMessages[country] };\n  const inlineValidator = createInlineValidator({ type: 'address', country: country.toLowerCase() });\n  const disabledCountry = true;\n  const fakeInputClasses = classnames(\n    (animateInputLabels ? 'animatedForm' : ''),\n    'fakeInput border-medium-grey ncss-col-sm-12 pt2-sm pr4-sm pb2-sm pl4-sm'\n  );\n  const animatedSelectOptionLabel = (animateInputLabels ?\n    formatMessage(messages.state) :\n    formatMessage(messages[`selectAreaLabel${country}`])\n  );\n  return (\n    <div className=\"billingForm\">\n      { allowNickName &&\n      <FieldWrapper\n        wrapperStyle=\"mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t\"\n        label={formatMessage(messages.addressNickname)}\n        id={'addressNickname'}\n        type={'input'}\n        disabled={disabled}\n        animateInputLabels={animateInputLabels}\n      />\n      }\n      <FieldWrapper\n        wrapperStyle=\"ncss-col-md-6 ncss-col-sm-12 mod-ncss-input-container mb2-sm mb4-md va-sm-t\"\n        label={formatMessage(messages.firstName)}\n        id={'firstName'}\n        type={'input'}\n        validate={inlineValidator('firstName')}\n        disabled={disabled || isBopis}\n        animateInputLabels={animateInputLabels}\n        autoComplete={'given-name'}\n      />\n      <FieldWrapper\n        wrapperStyle=\"ncss-col-md-6 ncss-col-sm-12 mod-ncss-input-container mb2-sm mb4-md va-sm-t\"\n        label={formatMessage(messages.lastName)}\n        id={'lastName'}\n        type={'input'}\n        validate={inlineValidator('lastName')}\n        disabled={disabled || isBopis}\n        animateInputLabels={animateInputLabels}\n        autoComplete={'family-name'}\n      />\n      <FieldWrapper\n        wrapperStyle=\"mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t\"\n        label={formatMessage(messages.billingAddress)}\n        id={'address1'}\n        type={'input'}\n        validate={inlineValidator('billingAddress1')}\n        disabled={disabled}\n        animateInputLabels={animateInputLabels}\n        autoComplete={'street-address'}\n      />\n      <FieldWrapper\n        wrapperStyle=\"mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t\"\n        id={'address2'}\n        type={'input'}\n        disabled={disabled}\n        placeholder={formatMessage(messages.address2placeHolder)}\n        animateInputLabels={animateInputLabels}\n      />\n      <FieldWrapper\n        wrapperStyle={'ncss-col-md-3 mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t'}\n        label={formatMessage(messages.city)}\n        id={'city'}\n        type={'input'}\n        validate={inlineValidator('city')}\n        disabled={disabled}\n        animateInputLabels={animateInputLabels}\n        autoComplete={'address-level2'}\n        autoCapitalize={'words'}\n      />\n      <StateSelector\n        wrapperStyle={`ncss-col-md-3 mod-ncss-input-container ncss-col-sm-12 ncss-select-container\n          mb2-sm mb4-md va-sm-t`}\n        label={formatMessage(messages[`area${country}`])}\n        id={'state'}\n        type={'select'}\n        options={countryState(country)}\n        validate={inlineValidator(`state${country}`)}\n        optionsLabel={animatedSelectOptionLabel}\n        disabled={disabled}\n        animateInputLabels={animateInputLabels}\n        autoComplete={'address-level1'}\n      />\n      <FieldWrapper\n        wrapperStyle={'ncss-col-md-3 mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t'}\n        label={formatMessage(messages.postalCode)}\n        id={'postalCode'}\n        inputType=\"text\"\n        type={'input'}\n        validate={inlineValidator('postalCode')}\n        disabled={disabled}\n        animateInputLabels={animateInputLabels}\n        autoComplete={'postal-code'}\n      />\n      {disabledCountry ?\n        <div className=\"ncss-col-md-3 ncss-col-sm-12 mb2-sm mb4-md va-sm-t text-color-grey\">\n          <FieldWrapper\n            wrapperStyle={'d-sm-h ncss-col-md-3 mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t text-color-grey'}\n            label={formatMessage(messages.country)}\n            id={'country'}\n            tooltipDisabled\n            tooltipContent={[{ type: 'message', text: formatMessage(messages.countryTooltip), style: 'tip-left' }]}\n            tooltipId={'countryTooltip'}\n            placeholder={formatMessage(messages.country)}\n            disabled={disabledCountry}\n            animateInputLabels={animateInputLabels}\n          />\n          {!animateInputLabels &&\n            <div className=\"fakeLabel pr6-m\">\n              {formatMessage(messages.country)}\n              <Tooltip\n                tooltipContent={[{\n                  type: 'message',\n                  text: formatMessage(messages.countryTooltip),\n                }]}\n                tooltipId=\"countryTooltip\"\n                showIcon\n              />\n            </div>\n          }\n          <div className={fakeInputClasses}>{formatMessage(messages.countryName)}</div>\n        </div>\n        :\n        <FieldWrapper\n          wrapperStyle={'d-sm-h ncss-col-md-3 mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t text-color-grey'}\n          label={formatMessage(messages.country)}\n          id={'country'}\n          tooltipDisabled\n          tooltipContent={[{ type: 'message', text: formatMessage(messages.countryTooltip), style: 'tip-left' }]}\n          tooltipId={'countryTooltip'}\n          placeholder={formatMessage(messages.country)}\n          animateInputLabels={animateInputLabels}\n          autoComplete=\"country-name\"\n        />\n      }\n      {containsOnlyDigitalGiftCard &&\n        <div className=\"mb2-sm\">\n          <FieldWrapper\n            wrapperStyle={'ncss-col-sm-12 ncss-col-md-6  ncss-input-container mod-ncss-input-container  mb2-sm mb0-md va-sm-t'}\n            label={formatMessage(messages.email)}\n            id={'email'}\n            type={'input'}\n            inputType=\"email\"\n            validate={inlineValidator('email')}\n            disabled={disabled}\n            animateInputLabels={animateInputLabels}\n            isRequired\n            showIcon\n            autoComplete={'email'}\n          />\n          <FieldWrapper\n            wrapperStyle={'ncss-col-sm-12 ncss-col-md-6  ncss-input-container mod-ncss-input-container  mb2-sm mb0-md va-sm-t'}\n            label={formatMessage(messages.phoneNumber)}\n            id={'phoneNumber'}\n            type={'input'}\n            inputType=\"tel\"\n            validate={inlineValidator('phoneNumber')}\n            disabled={disabled}\n            animateInputLabels={animateInputLabels}\n            isRequired\n            showIcon\n            normalize={normalizePhone}\n            autoComplete={'tel'}\n          />\n        </div>\n      }\n    </div>\n  );\n}\n\nBillingFormComponent.propTypes = {\n  allowNickName: PropTypes.bool,\n  animateInputLabels: PropTypes.bool,\n  containsOnlyDigitalGiftCard: PropTypes.bool,\n  country: PropTypes.string,\n  disabled: PropTypes.bool,\n  initialValues: PropTypes.shape({}),\n  isBopis: PropTypes.bool,\n  showCountry: PropTypes.bool,\n};\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/BillingForm.js","/* eslint-disable react/no-unused-prop-types, class-methods-use-this */\nimport PropTypes from 'prop-types';\nimport React from 'react';\nimport get from 'lodash.get';\nimport FieldWrapper from './FieldWrapper';\nimport { TOGGLE_TOOLTIP } from '../../state/app';\nimport formatMessage from '../../helpers/formatMessage';\nimport { createInlineValidator } from '../../helpers/validation/index';\nimport { getCountry, getCNStates, getCNCities, getCNDistricts } from '../../helpers/getGlobals';\nimport { messages } from '../../constants/addressFields';\n\nconst addressValidate = (() => {\n  const validatorsMap = new Map();\n  const formatCNAddressFormFiled = (value, allValues, props, name) => {\n    let validValue;\n    switch (name) {\n    case 'address.state':\n      return value || '';\n    case 'address.city':\n      validValue = getCNCities(allValues.address.state).filter(item => item.value === value);\n      return validValue.length ? value : '';\n    case 'address.county':\n      validValue = getCNDistricts(allValues.address.state, allValues.address.city)\n        .filter(item => item.value === value);\n      return validValue.length ? value : '';\n    default:\n      return value || '';\n    }\n  };\n  return (inlineValidator) => {\n    if (validatorsMap.get(inlineValidator)) {\n      return validatorsMap.get(inlineValidator);\n    }\n    validatorsMap.set(inlineValidator, (value, allValues, props, name) => {\n      const validValue = formatCNAddressFormFiled(value, allValues, props, name);\n      return inlineValidator(validValue);\n    });\n    return validatorsMap.get(inlineValidator);\n  };\n})();\n\nexport function AddressFormComponent(props) {\n  const {\n    allowNickName,\n    disabled,\n    dispatch,\n    currentValues,\n  } = props;\n  const currentState = get(currentValues, 'address.state');\n  const currentCity = get(currentValues, 'address.city');\n  const country = getCountry().toLowerCase();\n  const inlineValidator = createInlineValidator({ type: 'address', country });\n\n  return (\n    <div>\n      { allowNickName &&\n      <FieldWrapper\n        wrapperStyle=\"mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t\"\n        label={formatMessage(messages.addressNickname)}\n        id={'addressNickname'}\n        type={'input'}\n        disabled={disabled}\n      />\n      }\n      <FieldWrapper\n        wrapperStyle=\"ncss-col-md-6 ncss-col-sm-12 mod-ncss-input-container mb2-sm mb4-md va-sm-t\"\n        label={formatMessage(messages.lastName)}\n        id={'lastName'}\n        type={'input'}\n        validate={inlineValidator('lastName')}\n        disabled={disabled}\n      />\n      <FieldWrapper\n        wrapperStyle=\"ncss-col-md-6 ncss-col-sm-12 mod-ncss-input-container mb2-sm mb4-md va-sm-t\"\n        label={formatMessage(messages.firstName)}\n        id={'firstName'}\n        type={'input'}\n        validate={inlineValidator('firstName')}\n        disabled={disabled}\n      />\n      <FieldWrapper\n        wrapperStyle={`ncss-col-md-4 mod-ncss-input-container ncss-col-sm-12 ncss-select-container\n          mb2-sm mb4-md va-sm-t`}\n        label={formatMessage(messages.province)}\n        id={'state'}\n        type={'select'}\n        options={getCNStates()}\n        validate={addressValidate(inlineValidator('stateCN'))}\n        disabled={disabled}\n        optionsLabel={formatMessage(messages.selectStateLabel)}\n      />\n      <FieldWrapper\n        wrapperStyle={`ncss-col-md-4 mod-ncss-input-container ncss-col-sm-12 ncss-select-container\n          mb2-sm mb4-md va-sm-t`}\n        label={formatMessage(messages.city)}\n        id={'city'}\n        type={'select'}\n        options={getCNCities(currentState)}\n        validate={addressValidate(inlineValidator('city'))}\n        disabled={disabled}\n        optionsLabel={formatMessage(messages.selectCityLabel)}\n      />\n      <FieldWrapper\n        wrapperStyle={`ncss-col-md-4 mod-ncss-input-container ncss-col-sm-12 ncss-select-container\n          mb2-sm mb4-md va-sm-t`}\n        label={formatMessage(messages.district)}\n        id={'county'}\n        type={'select'}\n        options={getCNDistricts(currentState, currentCity)}\n        validate={addressValidate(inlineValidator('districtCN'))}\n        disabled={disabled}\n        optionsLabel={formatMessage(messages.selectDistrictLabel)}\n      />\n      <FieldWrapper\n        wrapperStyle=\"mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t\"\n        label={formatMessage(messages.address)}\n        id={'address1'}\n        type={'input'}\n        validate={inlineValidator('address1')}\n        disabled={disabled}\n      />\n      <div className=\"pb5-sm\">\n        <FieldWrapper\n          wrapperStyle={'ncss-col-sm-12 ncss-col-md-6  ncss-input-container mod-ncss-input-container  mb2-sm mb0-md va-sm-t'}\n          label={formatMessage(messages.email)}\n          id={'email'}\n          type={'input'}\n          inputType=\"email\"\n          validate={!props.emailDisabled && inlineValidator('email')}\n          disabled={props.emailDisabled || disabled}\n\n          isRequired\n          tooltipEnabled\n          tooltipContent={[{ type: 'message', text: formatMessage(messages.emailTooltip), style: 'tip-left' }]}\n          tooltipId={'emailTooltip'}\n          showIcon\n          trackTooltip={() => { dispatch({ type: TOGGLE_TOOLTIP, payload: 'email' }); }}\n          {...(props.input ? { ...{ input: props.input } } : {})}\n\n        />\n\n        <FieldWrapper\n          wrapperStyle={'ncss-col-sm-12 ncss-col-md-6  ncss-input-container mod-ncss-input-container  va-sm-t'}\n          label={formatMessage(messages.phoneNumber)}\n          id={'phoneNumber'}\n          type={'input'}\n          inputType=\"tel\"\n          validate={inlineValidator('phoneNumber')}\n          disabled={disabled}\n\n          isRequired\n          tooltipEnabled\n          tooltipContent={[{ type: 'message', text: formatMessage(messages.phoneNumberTooltip) }]}\n          tooltipId={'phoneNumTooltip'}\n          showIcon\n          trackTooltip={() => { dispatch({ type: TOGGLE_TOOLTIP, payload: 'phoneNumber' }); }}\n        />\n      </div>\n    </div>\n  );\n}\n\nAddressFormComponent.propTypes = {\n  countryOptions: PropTypes.arrayOf(\n    PropTypes.shape({})\n  ),\n  allowNickName: PropTypes.bool,\n  disabled: PropTypes.bool,\n  dispatch: PropTypes.func,\n  initialValues: PropTypes.shape({}),\n  currentValues: PropTypes.shape({}),\n  shippingAddress: PropTypes.bool,\n  showCountry: PropTypes.bool,\n  emailDisabled: PropTypes.bool,\n  input: PropTypes.shape({}),\n};\nexport default AddressFormComponent;\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/CNAddressForm.js","import PropTypes from 'prop-types';\nimport React from 'react';\nimport JPStates from '../../constants/JPStates';\nimport FieldWrapper from './FieldWrapper';\nimport StateSelector from './form/StateSelector';\nimport { TOGGLE_TOOLTIP } from '../../state/app';\nimport defineMessages from '../../helpers/defineMessages';\nimport formatMessage from '../../helpers/formatMessage';\nimport { createInlineValidator } from '../../helpers/validation/index';\nimport { getCountry, getIsEnglishInJapan } from '../../helpers/getGlobals';\nimport { replaceFullWidthNumerals } from '../../helpers/sanitizers';\n\nconst messages = defineMessages({\n  firstName: {\n    id: 'checkout.shipping.form.label.firstname',\n    description: 'First name input label',\n    defaultMessage: 'First Name',\n  },\n  lastName: {\n    id: 'checkout.shipping.form.label.lastName',\n    description: 'Last name input label',\n    defaultMessage: 'Last Name',\n  },\n  altLastName: {\n    id: 'checkout.shipping.form.label.altLastName',\n    description: 'Phonetic surname',\n    defaultMessage: 'Phonetic (surname)',\n  },\n  altFirstName: {\n    id: 'checkout.shipping.form.label.altFirstName',\n    description: 'Phonetic first name',\n    defaultMessage: 'Phonetic (first name)',\n  },\n  postCode: {\n    id: 'checkout.shipping.form.label.postCode',\n    description: 'Label for post code input',\n    defaultMessage: 'Post Code',\n  },\n  prefecture: {\n    id: 'checkout.shipping.form.label.prefecture',\n    description: 'Prefecture input label',\n    defaultMessage: 'Prefecture',\n  },\n  selectPrefectureCta: {\n    id: 'checkout.shipping.label.selectPrefecture',\n    description: 'Select a Japanese prefecture dropdown call-to-action',\n    defaultMessage: 'Select prefecture',\n  },\n  city: {\n    id: 'checkout.shipping.form.label.city',\n    description: 'City input label',\n    defaultMessage: 'City'