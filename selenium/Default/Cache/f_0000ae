ent\n              options={options}\n              shippingGroups={shippingPriceOffers}\n              orderId={orderId}\n              giftCardItems={giftCardItems}\n              containsOnlyGiftCard={containsOnlyGiftCard}\n            />\n          }\n        </div>\n      </div>);\n  }\n  return null;\n}\n\nFulfillmentOfferingsWrapperComponent.propTypes = {\n  containsOnlyGiftCard: PropTypes.bool,\n  dispatch: PropTypes.func,\n  freeShippingCopy: PropTypes.string,\n  shippingOfferingsProcessing: PropTypes.bool,\n  giftCardItems: PropTypes.shape({}),\n  guestToolTip: PropTypes.shape({}),\n  handleContinueToPayment: PropTypes.func,\n  handleSelectedShippingOption: PropTypes.func,\n  hasGiftCard: PropTypes.bool,\n  inDeliverySection: PropTypes.bool,\n  initialSelectedAddressId: PropTypes.string,\n  isEU: PropTypes.bool,\n  isGuest: PropTypes.bool,\n  isGuestBelowThreshold: PropTypes.bool,\n  isGuestRegistering: PropTypes.bool,\n  isGuestRegistrationProcessing: PropTypes.bool,\n  onClickChangeStore: PropTypes.func,\n  options: PropTypes.arrayOf(PropTypes.shape({})),\n  orderId: PropTypes.string,\n  recipient: PropTypes.shape({}),\n  registrationFormValues: PropTypes.shape({}),\n  selectedPriceOfferId: PropTypes.string,\n  shippingCompleted: PropTypes.bool,\n  shippingCutoffTime: PropTypes.string,\n  shippingGroups: PropTypes.shape({}),\n  shippingPriceOffers: PropTypes.arrayOf(PropTypes.shape({})),\n  showJPCutoffTimeMessaging: PropTypes.bool,\n  showShippingDisclaimer: PropTypes.bool,\n  showShippingMethodPreview: PropTypes.bool,\n  showShippingMethods: PropTypes.bool,\n  shipFulfillmentDetails: PropTypes.shape({}),\n};\n\nexport default FulfillmentOfferingsWrapperComponent;\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/shipping/FulfillmentOfferingsWrapper.js","import PropTypes from 'prop-types';\nimport React from 'react';\nimport { formatDateToLocalHM, getTimeZoneFromLocale } from '../../../helpers/formatDateLocale';\nimport defineMessages from '../../../helpers/defineMessages';\nimport formatMessage from '../../../helpers/formatMessage';\n\nconst messages = defineMessages({\n  scheduleDateLabel: {\n    id: 'checkout.shipping.label.scheduleDeliveryDate',\n    description: 'Specify delivery Date',\n    defaultMessage: 'Delivery Date',\n  },\n  scheduleTimeLabel: {\n    id: 'checkout.shipping.label.scheduleDeliveryTime',\n    description: 'Specify delivery time range',\n    defaultMessage: 'Preferred Delivery Time Range',\n  },\n  morningDeliveryAM: {\n    id: 'JP.checkout.optionalDeliveryTime.am',\n    description: 'Text for morning delivery in drop down list',\n    defaultMessage: 'AM',\n  },\n});\n\nexport default function ShippingScheduledDeliveries(props) {\n  const {\n    isJP,\n    locale,\n    optionalDates,\n    optionalTimes,\n    selectedDate,\n    selectedTime,\n    onDateChange,\n    onTimeChange,\n  } = props;\n  const timeZone = getTimeZoneFromLocale(locale);\n\n  return (\n    <div className=\"js-delivery-selections ncss-form-group mt4-sm\">\n      <div className=\"ncss-select-container pb4-sm\">\n        <label htmlFor=\"deliveryDate\" className=\"ncss-label\">{formatMessage(messages.scheduleDateLabel)}</label>\n        <div className=\"ncss-select\">\n          <select\n            name=\"deliveryDate\"\n            id=\"deliveryDate\"\n            className=\"ncss-select-menu ncss-base pl4-sm pr10-sm u-full-width u-full-height\"\n            value={selectedDate}\n            onBlur={onDateChange}\n          >\n            {optionalDates.map(date => (\n              <option key={date} value={date}>{date}</option>\n            ))}\n          </select>\n          <div className=\"select-arrow g72-arrow-thick-down text-color-black d-sm-b pt2-sm pr4-sm pb2-sm pl4-sm pt3-md pb3-md pt2-lg pb2-lg ta-sm-r\" />\n        </div>\n      </div>\n      <div className=\"ncss-select-container pb4-sm\">\n        <label htmlFor=\"deliveryTime\" className=\"ncss-label\">{formatMessage(messages.scheduleTimeLab