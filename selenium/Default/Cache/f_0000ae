l section',\n    defaultMessage: 'Bank',\n  },\n});\n\nexport class IdealComponent extends Component {\n  constructor(props) {\n    super(props);\n\n    this.handleSubmit = this.handleSubmit.bind(this);\n  }\n\n  handleSubmit() {\n    const { handleIdealSubmit, validateIdealForm } = this.props;\n    validateIdealForm();\n    handleIdealSubmit();\n  }\n\n  render() {\n    const {\n      dispatch,\n      idealBanks,\n      handleBankSelect,\n      selectedBank,\n      showIdealFieldErrors,\n      validateIdealForm,\n    } = this.props;\n\n    return (\n      <div className=\"payment-section ncss-row border-top-light-grey\">\n        <div className=\"ncss-col-sm-12 p5-sm\">\n          <img\n            className=\"d-sm-ib va-sm-m\"\n            src={idealImageBase64}\n            alt={formatMessage(messages.idealAltText)}\n            width=\"21\"\n            height=\"19\"\n          />\n          <p className=\"d-sm-ib va-sm-m ml1-sm\">{formatMessage(messages.idealHeader)}</p>\n          <Tooltip\n            tooltipContent={[{ type: 'message', text: formatMessage(messages.idealToolTipContent) }]}\n            tooltipId={'idealTooltip'}\n            showIcon\n            trackTooltip={() => { dispatch({ type: TOGGLE_TOOLTIP, payload: 'ideal' }); }}\n          />\n        </div>\n        <div className=\"ncss-col-sm-12\">\n          <div className=\"ncss-row\">\n            <div className=\"ncss-col-sm-12 ncss-col-md-6 prl5-sm pb5-sm\">\n              <div className={`mod-ncss-input-container ncss-select-container ${showIdealFieldErrors && !selectedBank ? 'error' : null}`}>\n                <label className=\"ncss-label\" htmlFor=\"idealBankSelect\">{formatMessage(messages.bankLabel)}</label>\n                <div className=\"ncss-select\">\n                  <div className=\"g72-arrow-thick-down text-color-black d-sm-b pt3-sm pr4-sm pb3-sm pl4-sm pt2-lg pb2-lg ta-sm-r\" />\n                  <select id=\"idealBankSelect\" className=\"ncss-select-menu ncss-base pl4-sm