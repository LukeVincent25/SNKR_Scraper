{\n  handleEventTracking: () => {},\n};\n\nAddressValidationModalComponent.propTypes = {\n  address: PropTypes.shape({\n    address1: PropTypes.string,\n    address2: PropTypes.string,\n    address3: PropTypes.string,\n    city: PropTypes.string,\n    county: PropTypes.string,\n    country: PropTypes.string,\n    heading: PropTypes.string,\n    id: PropTypes.string,\n    name: PropTypes.string,\n    postalCode: PropTypes.string,\n    state: PropTypes.string,\n  }),\n  addressUnverified: PropTypes.bool,\n  addressValidationSelection: PropTypes.string,\n  handleAddressValidationSelection: PropTypes.func,\n  handleEventTracking: PropTypes.func,\n  onAccept: PropTypes.func,\n  onCancel: PropTypes.func,\n  selectedAddress: PropTypes.shape({\n    address1: PropTypes.string,\n    address2: PropTypes.string,\n    city: PropTypes.string,\n    postalCode: PropTypes.string,\n  }),\n  suggestedAddress: PropTypes.shape({}),\n};\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/AddressValidationModal.js","import { normalizeGBPostalCode, normalizeJPPostalCode, normalizeNLPostalCode } from './addressUtils';\n\nconst validateFunc = {\n  GB: normalizeGBPostalCode,\n  JP: normalizeJPPostalCode,\n  NL: normalizeNLPostalCode,\n};\nconst normalizePostalCode = (code, country) => {\n  const getValidatedPostalCode = validateFunc[country.toUpperCase()];\n  if (getValidatedPostalCode) {\n    return getValidatedPostalCode(code);\n  }\n  return code;\n};\n\nexport default normalizePostalCode;\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/helpers/normalizePostalCode.js","/* eslint-disable react/prop-types */\nimport PropTypes from 'prop-types';\nimport React from 'react';\nimport { connect } from 'react-redux';\nimport debounce from 'lodash.debounce';\nimport { getCountry } from '@nike/ux-tread-localization';\nimport get from 'lodash.get';\nimport { SvgLoader as Loader } from '@nike/nr-sole-loaders/';\nimport defineMessages from '../../../helpers/defineMessages';\nimport formatMessage from '../../../helpers/formatMessage';\nimport StorePreview from '../StorePreview';\nimport {\n  BOPIS_CHANGE_STORE_CONTINUE,\n  EDIT_PICKUP_LOCATION,\n  fetchCartReviews,\n  fetchFulfillmentOfferingsAndLocations,\n  fetchUserLocation,\n  SET_FULFILLMENT_OFFERINGS,\n  SET_STORE_VIEW,\n  SET_STORES_WITH_AVAILABILITY,\n  UPDATE_PICKUP_LOCATION,\n  CLICK_PICKUP_GEOLOCATION,\n  CLICK_PICKUP_LOCATION_SELECTED,\n  CLICK_PICKUP_LOCATION_SAVED,\n  STORE_VIEW_SEARCH_RADIUS,\n  SET_PICKUP_POINT_LOCATIONS,\n  SET_PUP_FULFILLMENT_OFFERINGS,\n  SET_SELECTED_PUP_LOCATION,\n  fetchFulfillmentOfferings,\n} from '../../../state/app';\nimport { groupStoresByAvailability } from '../../../helpers/shippingUtils';\nimport formatDistance from '../../../helpers/formatDistance';\nimport {\n  getDistanceUnitLabel,\n  getSelectedStoreId,\n  getSelectedPupId,\n  getPupsList,\n  getStoreListWithFulfillmentInfo,\n  getStorePickupLocationId,\n  getStoreViews,\n  getIsUS,\n  getIsEU,\n  getPupFulfillmentOfferings,\n  getSelectedPupOffering,\n} from '../../../selectors';\nimport { LOCATION_TYPE_POSTALCODE, LOCATION_DEFAULT } from '../../../constants/userLocation';\nimport LoadingScreen from '../Loading';\nimport { saveUserLocationType, removeSavedUserLocationTypeIfExists, userHasSelectedLocation } from '../../../state/app/actions/userLocationActionCreators';\nimport { getActiveUserLocation, getActiveUserLocationType, getIsLocationSelectedByUser, getHasBopis, getHasClickAndCollect } from '../../../state/app/selectors';\nimport { createInlineValidator } from '../../../helpers/validation';\nimport PickUpPointsCell from '../PickupPointsCell';\nimport { formatGetByDateRange } from '../../../helpers/formatDate';\nimport { formatPriceOfferExtra } from '../../../helpers/formatPriceOffer';\nimport List from '../List';\nimport normalizePostalCode from '../../../helpers/normalizePostalCode';\nimport {\n  SET_ADDRESS_TYPE,\n  SHIPPING_TYPE_BOPIS,\n  SHIPPING_TYPE_CLICK_AND_COLLECT,\n} from '../../../state/checkout';\nimport { clearPickupTabLocationsDisplayed, setPickupTabLocationsDisplayed } from '../../../state/app/actions/pickupTabLocationsDisplayedActionCreators';\n\nconst messages = defineMessages({\n  mainHeader: {\n    id: 'checkout.shipping.pickup.header',\n    description: 'Header message for BOPIS store selection view',\n    defaultMessage: 'SELECT A STORE FOR PICK UP',\n  },\n  storeSearchLabel: {\n    id: 'checkout.shipping.pickup.searchLabel',\n    description: 'Label for BOPIS store search',\n    defaultMessage: 'Showing stores near:',\n  },\n  submitButtonLabel: {\n    id: 'checkout.shipping.saveAddressButton',\n    description: 'Save this address button',\n    defaultMessage: 'Save & Continue',\n  },\n  defaultMainHeaderEU: {\n    id: 'checkout.shipping.pickup.defaultSearchLabel',\n    description: 'Label for BOPIS and Click and Collect store search',\n    defaultMessage: 'Select a store or click and collect location',\n  },\n  defaultMainHeaderUS: {\n    id: 'checkout.shipping.pickup.US.defaultSearchLabel',\n    description: 'Header label for BOPIS store search',\n    defaultMessage: 'Select a store location',\n  },\n  defaultStoreSearchMessage: {\n    id: 'checkout.shipping.pickup.defaultSearchMessage',\n    description: 'Message displayed on initial display of BOPIS / Click and Collect store search',\n    defaultMessage: 'Enter your zip code to find a pickup location near you.',\n  },\n  noAvailableStoresMessage: {\n    id: 'checkout.v2.shipping.bopis.modal.noAvailableStoresMessage',\n    description: 'Message displayed when no stores are within specified search area',\n    defaultMessage: 'There are no pickup locations within {distance} {distanceUnitLabel} of your location. To complete your order, change your location or ship your items instead.',\n  },\n  noAvailableStoresMessageEMEA: {\n    id: 'checkout.v2.shipping.bopis.noAvailableStoresMessage',\n    description: 'Message displayed when no stores are within specified search area',\n    defaultMessage: 'We couldn\\'t find any stores within {distance} {distanceUnitLabel} of your location.',\n  },\n  noAvailableCollectionPointMessageEMEA: {\n    id: 'checkout.v2.shipping.clickAndCollect.noAvailableStoresMessage',\n    description: 'Message displayed when no collection points are within specified search area',\n    defaultMessage: 'We couldn\\'t find any collection points within {distance} {distanceUnitLabel} of your location.',\n  },\n  loadingMessage: {\n    id: 'checkout.shipping.pickup.loadingText',\n    description: 'Message displayed when waiting for location data to update the UI',\n    defaultMessage: 'Loading',\n  },\n  browserLocation: {\n    id: 'checkout.shipping.pickup.browserLocation',\n    description: 'Message displayed when user gives access to browser geo location',\n    defaultMessage: 'Your Location',\n  },\n  applyButton: {\n    id: 'checkout.shipping.pickup.applyButton',\n    description: 'Label for the text Apply which lets users submit the zipcode search',\n    defaultMessage: 'Apply',\n  },\n  pickupOrdersDescription: {\n    id: 'checkout.shipping.pickup.description',\n    description: 'Message displayed for pickup orders',\n    defaultMessage: 'Same day pickup orders are usually ready within two hours. Valid ID is required to pick up items in-store.',\n  },\n  postalCodePlaceholder: {\n    id: 'checkout.shipping.form.label.zipcode',\n    description: 'Postal code input label',\n    defaultMessage: 'Postal Code',\n  },\n  locateButtonAltText: {\n    id: 'checkout.pickup.form.title.locate',\n    description: 'Hover text for geo location button',\n    defaultMessage: 'Find Stores Near You',\n  },\n  storesHeaderEmea: {\n    id: 'checkout.shipping.pickup.list.header',\n    description: 'Header for the list of stores found in the search area',\n    defaultMessage: 'Nearby Stores',\n  },\n  pupsHeaderEmea: {\n    id: 'checkout.shipping.collectionPoint.list.header',\n    description: 'Header for the list of Click & Collect locations found in the search area',\n    defaultMessage: 'Click & Collect Locations',\n  },\n});\n\nconst PUPS_PAGINATION_INTERVAL = 5;\nconst PUPS_VIEW_MORE_LIMIT = 3;\nconst STORE_PAGINATION_INTERVAL = 5;\nconst STORE_VIEW_MORE_LIMIT = 3;\n\n/**\n * This component displays a list of stores and whether user's items are\n * available for pickup in each store. It keeps local state for current\n * search zip code and returned response data from fulfillment options / store\n * views. After user selects a store and hits continue button, we dispatch the\n * user's store selection and response data to state. Keeping local state makes\n * it simpler to revert back to the initial store selection.\n */\nexport class PickupLocationSelectionComponent extends React.Component {\n  constructor(props) {\n    super(props);\n    this.formatLocationLabel = this.formatLocationLabel.bind(this);\n    this.getBrowserLocation = this.getBrowserLocation.bind(this);\n    this.handleApplyClick = this.handleApplyClick.bind(this);\n    this.handleChangeLocation = this.handleChangeLocation.bind(this);\n    this.handleChangeStore = this.handleChangeStore.bind(this);\n    this.handleChangeZipCode = this.handleChangeZipCode.bind(this);\n    this.handleKeyPress = this.handleKeyPress.bind(this);\n    this.handleZipButtonClick = this.handleZipButtonClick.bind(this);\n    this.onSubmit = this.onSubmit.bind(this);\n    this.setUserLocation = this.setUserLocation.bind(this);\n    this.userInitiatedLocation = this.userInitiatedLocation.bind(this);\n    // eslint-disable-next-line no-underscore-dangle\n    this.search = debounce(this._search, 500);\n    this.state = {\n      error: false,\n      findLocation: false,\n      initialized: false,\n      processing: false,\n      storeListPaginationLocation: 0,\n      pupsListPaginationLocation: 0,\n      zip: '',\n    };\n  }\n\n  componentDidMount() {\n    const { dispatch, isLocationSelectedByUser } = this.props;\n    const { zip } = this.state;\n\n    // if user has entered a zip or clicked the location icon,\n    // render component with saved location upon return\n    if (isLocationSelectedByUser) {\n      return dispatch(fetchUserLocation({ zip })).then(() => {\n        // request location by the zip we have in state\n        this.setUserLocation(true);\n      }, () => {\n        this.setState({\n          initialized: true,\n          showSearchInput: true,\n        });\n        this.setUserLocation();\n      }).finally(() => {\n        dispatch(removeSavedUserLocationTypeIfExists());\n      });\n    }\n\n    // user has not initiated location - coords or postal code\n    // Show postalCode prompt / don't attempt to geo locate\n    this.setState({\n      initialized: true,\n      showSearchInput: true,\n    });\n\n    return this.userInitiatedLocation(false);\n  }\n\n  componentWillUnmount() {\n    const { dispatch, locationType } = this.props;\n    return Promise.all([\n      dispatch(saveUserLocationType(locationType)),\n    ]);\n  }\n\n  onSubmit() {\n    const {\n      dispatch,\n      fulfillmentData,\n      selectedStoreId,\n      storeViews,\n      selectedLocationId,\n      pupsViewsList,\n    } = this.props;\n\n    if (!selectedStoreId && !selectedLocationId) {\n      return;\n    }\n    if (selectedStoreId) {\n      dispatch({\n        type: CLICK_PICKUP_LOCATION_SAVED,\n        payload: selectedStoreId,\n      });\n      if (storeViews) {\n        const selectedStoreInfo = storeViews[selectedStoreId];\n        dispatch({\n          type: UPDATE_PICKUP_LOCATION,\n          payload: selectedStoreInfo,\n        });\n      }\n      if (fulfillmentData) {\n        const { storesWithAvailability, fulfillmentOfferings } = fulfillmentData;\n        dispatch({\n          type: SET_FULFILLMENT_OFFERINGS,\n          payload: fulfillmentOfferings,\n        });\n        dispatch({\n          type: SET_STORES_WITH_AVAILABILITY,\n          payload: storesWithAvailability,\n        });\n      }\n      dispatch({\n        type: BOPIS_CHANGE_STORE_CONTINUE,\n      });\n    } else {\n      // analytics for selected pup?\n      if (pupsViewsList) {\n        const selectedLocation = pupsViewsList.find(\n          (location) => location.id === selectedLocationId\n        );\n        dispatch({\n          type: UPDATE_PICKUP_LOCATION,\n          payload: selectedLocation,\n        });\n      }\n      if (fulfillmentData) {\n        const { fulfillmentOfferings } = fulfillmentData;\n        dispatch({\n          type: SET_FULFILLMENT_OFFERINGS,\n          payload: fulfillmentOfferings,\n        });\n      }\n    }\n    dispatch({\n      type: EDIT_PICKUP_LOCATION,\n      payload: false,\n    });\n  }\n\n  setUserLocation(reset) {\n    if (reset) {\n      this.setState({\n        findLocation: false,\n        showSearchInput: false,\n      });\n    }\n    this.search();\n  }\n\n  getBrowserLocation() {\n    this.setState({\n      findLocation: true,\n    });\n\n    this.props.dispatch({\n      type: CLICK_PICKUP_GEOLOCATION,\n    });\n\n    return this.props.dispatch(fetchUserLocation({\n      forceBrowserLocation: true, // this is the only way to request browser location\n    })).then(() => {\n      this.userInitiatedLocation();\n      this.setUserLocation(true);\n    }, () => {\n      this.userInitiatedLocation(false); // set the UI back to default\n      // If  the user has denied browser based location and we are missing fallback cookies\n      // we will end up in this error handler which will cause the\n      // app to ignore the location icon (gps arrow thingy) click.\n    });\n  }\n\n  getDefaultPostalCode() {\n    const { dispatch } = this.props;\n    dispatch(fetchUserLocation({\n      findAndReturnPostalCode: true,\n    })).then(() => {\n      dispatch(userHasSelectedLocation(true));\n      this.setUserLocation(true);\n    }, () => {\n      this.setUserLocation(false);\n    });\n  }\n\n  // set whether the zip or coordinate location request was user initiated?\n  // sets isLocationSelectedByUser:true when zip code is submitted or geo location is clicked\n  // * sets false if user denies the browser request after clicking the geo location icon\n  userInitiatedLocation(userInitiated = true) {\n    this.props.dispatch(userHasSelectedLocation(userInitiated));\n    if (!userInitiated) {\n      this.getDefaultPostalCode();\n    }\n  }\n\n  /**\n   * Do not call directly. Use this.search, which debounces this function.\n   */\n  // eslint-disable-next-line no-underscore-dangle\n  _search() {\n    const { isLocationSelectedByUser } = this.props;\n    const { zip } = this.state;\n    if (isLocationSelectedByUser) {\n      const { dispatch, activeUserLocation } = this.props;\n      const postalCode = activeUserLocation.postalCode || zip || null;\n      this.setState({\n        searchedZip: postalCode,\n        processing: true,\n      });\n\n      dispatch(clearPickupTabLocationsDisplayed());\n      dispatch({\n        type: SET_SELECTED_PUP_LOCATION,\n      });\n\n\n      return dispatch(fetchFulfillmentOfferingsAndLocations())\n        .then(([fulfillmentData = {}, storeViews, pickupPoints = {}]) => {\n          dispatch({\n            type: SET_STORES_WITH_AVAILABILITY,\n            payload: fulfillmentData.storesWithAvailability,\n          });\n          dispatch({\n            type: SET_PUP_FULFILLMENT_OFFERINGS,\n            payload: fulfillmentData.pupOfferings,\n          });\n          dispatch({\n            type: SET_STORE_VIEW,\n            payload: storeViews,\n          });\n          dispatch({\n            type: SET_PICKUP_POINT_LOCATIONS,\n            payload: pickupPoints,\n          });\n          dispatch(setPickupTabLocationsDisplayed({\n            bopis: !!(Object.keys(fulfillmentData.storesWithAvailability).length),\n            pups: !!(Object.keys(pickupPoints).length),\n          }));\n        }).catch(() => {\n        // no offerings. clear the previous store info.\n          dispatch({\n            type: SET_STORES_WITH_AVAILABILITY,\n            payload: {},\n          });\n          dispatch({\n            type: SET_STORE_VIEW,\n            payload: {},\n          });\n          dispatch(setPickupTabLocationsDisplayed({\n            bopis: false,\n            pups: false,\n          }));\n        })\n        .finally(() => {\n          this.setState({\n            initialized: true,\n            processing: false,\n          });\n        });\n    }\n    return null;\n  }\n\n  handleApplyClick() {\n    const {\n      zip,\n      error,\n    } = this.state;\n    if (zip && !error) {\n      this.userInitiatedLocation();\n      this.props.dispatch(fetchUserLocation({\n        zip,\n      })).finally(() => {\n        this.setUserLocation(true);\n      });\n    }\n  }\n\n\n  handleKeyPress(e) {\n    if (e.key === 'Enter') {\n      this.handleApplyClick();\n    }\n  }\n\n  handleZipButtonClick() {\n    this.setState({\n      showSearchInput: true,\n    });\n  }\n\n  handleChangeZipCode(validatorFn, e) {\n    const zip = e.target.value;\n    // lets validate! take the zip and feed it in\n    const normalizedZip = normalizePostalCode(zip, this.props.country);\n    const invalidMessage = validatorFn(normalizedZip);\n    this.setState({\n      zip: invalidMessage ? zip : normalizedZip,\n      error: invalidMessage,\n    });\n  }\n\n  handleChangeStore(storeId) {\n    this.props.dispatch({\n      type: SET_ADDRESS_TYPE,\n      payload: SHIPPING_TYPE_BOPIS,\n    });\n    this.props.dispatch({\n      type: CLICK_PICKUP_LOCATION_SELECTED,\n      payload: storeId,\n    });\n\n    this.props.dispatch(fetchCartReviews());\n  }\n\n  handleChangeLocation(locationId) {\n    this.props.dispatch({\n      type: SET_ADDRESS_TYPE,\n      payload: SHIPPING_TYPE_CLICK_AND_COLLECT,\n    });\n    this.props.dispatch({\n      type: SET_SELECTED_PUP_LOCATION,\n      payload: locationId,\n    });\n    this.props.dispatch(fetchFulfillmentOfferings()).then((response) => {\n      this.props.dispatch({\n        type: SET_PUP_FULFILLMENT_OFFERINGS,\n        payload: response.pupOfferings,\n      });\n      this.props.dispatch(fetchCartReviews());\n    }, () => {\n      this.props.dispatch({\n        type: SET_PUP_FULFILLMENT_OFFERINGS,\n        payload: [{\n          id: locationId,\n          isUnavailable: true,\n        }],\n      });\n    });\n    this.props.dispatch({\n      type: CLICK_PICKUP_LOCATION_SELECTED,\n    });\n  }\n\n  formatLocationLabel() {\n    const { locationType, activeUserLocation } = this.props;\n\n    return locationType !== LOCATION_TYPE_POSTALCODE\n      ? formatMessage(messages[locationType])\n      : activeUserLocation;\n  }\n\n\n  renderPagination(list, startIndex, maxPagination, listName, callbackListId) {\n    const endIndex = startIndex + maxPagination;\n    const totalListCount = list.length;\n    const displayedListItems = list.slice(startIndex, endIndex);\n    let isPreviousDisabled = {};\n    let isNextDisabled = {};\n    if (startIndex < 1) {\n      isPreviousDisabled = { disabled: true };\n    }\n    if ((endIndex + 1) >= totalListCount) {\n      isNextDisabled = { disabled: true };\n    }\n    return {\n      list: displayedListItems,\n      controlsHtml: (\n        <div className=\"ncss-row pt4-sm\">\n          <div className=\"ncss-col-sm-4\">\n            <button\n              className=\"ncss-btn\"\n              onClick={() => this.onPaginatePrevious(callbackListId)}\n              aria-label={formatMessage(messages.previousAria, { listName })}\n              {...isPreviousDisabled}\n            >\n              <i className=\"g72-arrow-thin-left u-bold\" />\n              <span className=\"pl2-sm\">{formatMessage(messages.previous)}</span>\n            </button>\n          </div>\n          <div className=\"ncss-col-sm-4 ta-sm-c\">\n            <span>{\n              formatMessage(\n                messages.paginationCount,\n                {\n                  firstDisplayedItem: startIndex + 1,\n                  lastDisplayedItem: startIndex + displayedListItems.length,\n                  totalNumberOfItems: totalListCount,\n                }\n              )}\n            </span>\n          </div>\n          <div className=\"ncss-col-sm-4 ta-sm-r\">\n            <button\n              className=\"ncss-btn\"\n              onClick={() => this.onPaginateNext(callbackListId)}\n              aria-label={formatMessage(messages.nextAria, { listName })}\n              {...isNextDisabled}\n            >\n              <span className=\"pr2-sm\">{formatMessage(messages.next)}</span>\n              <i className=\"g72-arrow-thin-right u-bold\" />\n            </button>\n          </div>\n        </div>\n      ),\n    };\n  }\n\n  renderNoAvailabilityMessageHtml() {\n    const { distanceUnitLabel } = this.props;\n    return (\n      <React.Fragment>\n        <div className={'ncss-col-sm-2 ncss-col-md-1 d-sm-ib va-sm-m'}>\n          <div className={'warning-icon'} />\n        </div>\n        <div className={'ncss-col-sm-10 ncss-col-md-11 d-sm-ib va-sm-m pr5-md'}>\n          {\n            formatMessage(\n              messages.noAvailableStoresMessage,\n              {\n                distance: STORE_VIEW_SEARCH_RADIUS,\n                distanceUnitLabel, // Not dynamic, should be dynamic :/\n              }\n            )\n          }\n        </div>\n      </React.Fragment>\n    );\n  }\n\n  renderStorePreview(stores) {\n    const {\n      findLocation,\n      initialized,\n      processing,\n    } = this.state;\n    const {\n      distanceUnitLabel,\n      isLocationSelectedByUser,\n      selectedStoreId,\n      isEU,\n    } = this.props;\n    if (!isLocationSelectedByUser) {\n      return null;\n    }\n    if (!stores.length && initialized && !processing) {\n      return (\n        <div className=\"ncss-row\">\n          {this.renderNoAvailabilityMessageHtml()}\n        </div>\n      );\n    } else if ((!stores.length && !initialized) || findLocation || processing) {\n      return (\n        <div className=\"ta-sm-c\">\n          <LoadingScreen />\n        </div>\n      );\n    }\n\n    const options = stores.map(store => ({\n      id: store.id,\n      label: store.name,\n      content: (<StorePreview {...store} isSelected={store.id === selectedStoreId} />),\n      extra: `${formatDistance(store.distance)} ${distanceUnitLabel}`,\n      alwaysShowContent: true,\n      disabled: !store.availability,\n    }));\n\n    let headerMessage;\n    if (isEU) {\n      headerMessage = formatMessage(messages.storesHeaderEmea);\n    }\n\n    return (\n      <React.Fragment>\n        <List\n          listMessages={{\n            header: headerMessage,\n            noListItems: messages.noAvailableCollectionPointMessageEMEA,\n          }}\n          messageOptions={{\n            ariaListName: {\n              listName: formatMessage(messages.storesHeaderEmea),\n            },\n            noListItems: {\n              distance: STORE_VIEW_SEARCH_RADIUS,\n              distanceUnitLabel,\n            },\n          }}\n          options={options}\n          onSelected={this.handleChangeStore}\n          selectedItemId={selectedStoreId}\n          viewMoreLimit={STORE_VIEW_MORE_LIMIT}\n          paginationInterval={STORE_PAGINATION_INTERVAL}\n        />\n      </React.Fragment>\n    );\n  }\n\n  renderPupsPreview(pups) {\n    const {\n      findLocation,\n      initialized,\n      processing,\n    } = this.state;\n    const {\n      distanceUnitLabel,\n      isLocationSelectedByUser,\n      selectedLocationId,\n      pupOfferings,\n    } = this.props;\n\n    if (!isLocationSelectedByUser) {\n      return null;\n    } else if ((!pups.length && !initialized) || findLocation || processing) {\n      return (\n        <div className=\"ta-sm-c\">\n          <LoadingScreen />\n        </div>\n      );\n    }\n\n    const options = pups.map(location => {\n      const offering = pupOfferings.find(\n        (offer) => offer.id === location.id\n      ) || {};\n      const price = formatPriceOfferExtra(get(offering, 'offering.price.total')) || (<Loader width={32} />);\n      return {\n        id: location.id,\n        label: location.name,\n        content: (<PickUpPointsCell\n          address={location}\n          isSelected={location.id === selectedLocationId}\n          hours={location.operationHours}\n          price={price}\n          name={location.name}\n          estimatedDeliveryDate={offering.maxDate ? formatGetByDateRange(offering.minDate, offering.maxDate) : ''}\n          isUnavailable={offering.isUnavailable}\n        />),\n        extra: `${formatDistance(location.distance)} ${distanceUnitLabel}`,\n        alwaysShowContent: true,\n      };\n    });\n\n    return (\n      <React.Fragment>\n        <List\n          listMessages={{\n            header: messages.pupsHeaderEmea,\n            noListItems: messages.noAvailableCollectionPointMessageEMEA,\n          }}\n          messageOptions={{\n            ariaListName: {\n              listName: formatMessage(messages.pupsHeaderEmea),\n            },\n            noListItems: {\n              distance: STORE_VIEW_SEARCH_RADIUS,\n              distanceUnitLabel,\n            },\n          }}\n          options={options}\n          onSelected={this.handleChangeLocation}\n          selectedItemId={selectedLocationId}\n          viewMoreLimit={PUPS_VIEW_MORE_LIMIT}\n          paginationInterval={PUPS_PAGINATION_INTERVAL}\n        />\n      </React.Fragment>\n    );\n  }\n\n  renderZipSearchField() {\n    const { initialized, showSearchInput, error } = this.state;\n    const { country, isLocationSelectedByUser } = this.props;\n    const locationText = this.formatLocationLabel();\n    const showLabel = (locationText && initialized) || showSearchInput;\n    const validator = createInlineValidator({ type: 'address', country });\n    return (\n      <div className=\"ncss-col-sm-12\">\n        <div className=\"d-sm-ib\">\n          {showLabel &&\n            <label htmlFor=\"store-search\" className=\"fs14-sm\">\n              {(isLocationSelectedByUser && formatMessage(messages.storeSearchLabel))}\n            </label>\n          }\n          {/* Desktop */}\n          {!showSearchInput && initialized &&\n            <button className=\"zip-code-button-desktop ncss-flyout-btn fs14-sm u-underline pl0-sm pb0-sm pt0-sm pr7-sm ml1-sm d-sm-h d-md-ib\" onClick={this.handleZipButtonClick}>{locationText}\n            </button>\n          }\n        </div>\n        {/* Mobile */}\n        <div className=\"d-sm-ib pl2-sm full va-sm-b d-md-h\">\n          {!showSearchInput &&\n          <button className=\"zip-code-button-mobile ncss-flyout-btn bg-white fs14-sm u-underline pb0-sm pt0-sm pl0-sm pr7-sm ml0-sm\" onClick={this.handleZipButtonClick}>\n            {locationText}\n          </button>\n          }\n        </div>\n\n        {showSearchInput &&\n          <div className=\"ncss-row store-search-container\">\n            <div className={`ncss-input-container ncss-col-md-6 ncss-col-sm-10 pr10-sm pr2-md${error ? 'error' : ''}`}>\n              <div className=\"ncss-textarea-container\">\n                <input\n                  id=\"store-search\"\n                  value={this.state.zip}\n                  className=\"ncss-input pt2-sm pr8-sm pb2-sm pl4-sm\"\n                  onChange={(event) => { this.handleChangeZipCode(validator('postalCode'), event); }}\n                  onKeyUp={this.handleKeyPress}\n                  placeholder={formatMessage(messages.postalCodePlaceholder)}\n                  aria-invalid={!!error || undefined}\n                  aria-label={error || undefined}\n                />\n                <span className={'ncss-error-msg'}>\n                  {error}\n                </span>\n                <button\n                  className=\"g72-location fs20-sm pr0-sm u-cursor-pointer\"\n                  alt={formatMessage(messages.locateButtonAltText)}\n                  title={formatMessage(messages.locateButtonAltText)}\n                  onClick={this.getBrowserLocation}\n                  tabIndex={'0'}\n                />\n              </div>\n            </div>\n            {/* Mobile */}\n            <div className=\"ncss-col-sm-2 full va-sm-b d-md-h\">\n              {showSearchInput &&\n              <button className=\"apply-button ncss-btn-white ncss-base pb2-sm fl-sm-r u-underline\" onClick={this.handleApplyClick}>\n                {formatMessage(messages.applyButton)}\n              </button>\n              }\n              {!showSearchInput &&\n              <button className=\"zip-code-button-mobile ncss-flyout-btn bg-white fs14-sm u-underline pb0-sm pt0-sm pl0-sm pr7-sm ml0-sm\" onClick={this.handleZipButtonClick}>\n                {locationText}\n              </button>\n              }\n            </div>\n            <div className=\"ncss-col-md-6 pt2-sm va-sm-t\">\n              <button className=\"ncss-btn-white ncss-base u-underline\" onClick={this.handleApplyClick}>\n                {formatMessage(messages.applyButton)}\n              </button>\n            </div>\n          </div>\n        }\n      </div>\n\n    );\n  }\n\n  render() {\n    const {\n      activeUserLocation,\n      pupsViewsList,\n      country,\n      hasBopis,\n      hasClickAndCollect,\n      isEU,\n      isLocationSelectedByUser,\n      isUS,\n      storesWithFulfillmentInfo,\n      selectedStoreId,\n      selectedLocationId,\n      selectedPupOffering,\n    } = this.props;\n\n    const {\n      initialized,\n      processing,\n    } = this.state;\n\n    const storeList = groupStoresByAvailability(storesWithFulfillmentInfo);\n    const storeContent = hasBopis && this.renderStorePreview(storeList);\n    const locationsContent = hasClickAndCollect && this.renderPupsPreview(pupsViewsList);\n    // should be a selector\n    const shouldDisableContinueButton =\n      (isUS && !selectedStoreId) ||\n      (isEU && (!selectedLocationId && !selectedStoreId)) ||\n      (selectedPupOffering && selectedPupOffering.isUnavailable);\n    const zipSearchField = this.renderZipSearchField({ country });\n    const headerText = (isEU && messages.defaultMainHeaderEU)\n      || (isUS && messages.defaultMainHeaderUS)\n      || messages.mainHeader; /*  I don't think we need this third fallback. I'm adding this\n                                  to avoid breaking the app if bopis loads outside US/EU */\n    // should be a selector\n    const shouldDisplaySubmitButton =\n      (isUS && !!storeList.length) ||\n      (isEU && (!!pupsViewsList.length || !!storeList.length));\n    return (\n      <React.Fragment>\n        <div className=\"bopis-container prl4-sm\">\n          {/* Header */}\n          <div className=\"ncss-row pt6-sm pb0-sm\">\n            <div className=\"ncss-col-sm-12\">\n              <h3 className=\"ncss-brand u-uppercase fs16-sm\">\n                {formatMessage(headerText)}\n              </h3>\n            </div>\n          </div>\n          {/* Search input */}\n          <div className=\"ncss-row pb4-sm\">\n            {zipSearchField}\n          </div>\n          {/* Store results */}\n          {\n            storeContent &&\n            <div className=\"ncss-row pb5-sm\">\n              <div className=\"ncss-col-sm-12 bopis-store-container\">\n                {storeContent}\n              </div>\n            </div>\n          }\n          {/* Click and Collect results */}\n          {locationsContent\n            && initialized\n            && !processing\n            && isLocationSelectedByUser &&\n            <div className=\"ncss-row pb5-sm\">\n              <div className=\"ncss-col-sm-12 bopis-store-container\">\n                {locationsContent}\n              </div>\n            </div>\n          }\n          {!activeUserLocation &&\n          <div className=\"ncss-row pb5-sm\">\n            <div className={'ncss-col-sm-12 d-sm-ib va-sm-m pr5-md'}>\n              {formatMessage(messages.defaultStoreSearchMessage)}\n            </div>\n          </div>\n          }\n          {/* CTA */}\n          {shouldDisplaySubmitButton &&\n            <div className=\"ncss-row pb4-sm mt2-sm\">\n              <div className=\"ncss-col-sm-12 ncss-col-md-6\">\n                <p className=\"fs12-sm lh10-sm pb5-sm pb6-md\">\n                  {formatMessage(messages.pickupOrdersDescription)}\n                </p>\n              </div>\n              <div className=\"va-sm-t ncss-col-sm-12 ncss-col-md-6 ta-sm-r\">\n                <button\n                  onClick={this.onSubmit}\n                  disabled={shouldDisableContinueButton}\n                  className=\"js-next-step mod-ncss-btn ncss-btn-accent ncss-brand pt3-sm prl5-sm pb3-sm pt2-lg pb2-lg d-md-ib u-uppercase u-rounded fs14-sm mod-button-width\"\n                >\n                  {formatMessage(messages.submitButtonLabel)}\n                </button>\n              </div>\n            </div>\n          }\n        </div>\n      </React.Fragment>\n    );\n  }\n}\n\nexport const mapStateToProps = (state) => ({\n  activeUserLocation: getActiveUserLocation(state),\n  bopisPickupLocationId: getStorePickupLocationId(state),\n  country: getCountry(state),\n  distanceUnitLabel: getDistanceUnitLabel(state),\n  hasBopis: getHasBopis(state),\n  hasClickAndCollect: getHasClickAndCollect(state),\n  isEU: getIsEU(state),\n  isLocationSelectedByUser: getIsLocationSelectedByUser(state),\n  isUS: getIsUS(state),\n  locationType: getActiveUserLocationType(state),\n  pupOfferings: getPupFulfillmentOfferings(state),\n  pupsViewsList: getPupsList(state),\n  selectedLocationId: getSelectedPupId(state),\n  selectedPupOffering: getSelectedPupOffering(state),\n  selectedStoreId: getSelectedStoreId(state),\n  storesWithFulfillmentInfo: getStoreListWithFulfillmentInfo(state),\n  storeViews: getStoreViews(state),\n});\n\nPickupLocationSelectionComponent.defaultProps = {\n  activeUserLocation: LOCATION_DEFAULT,\n};\n\nPickupLocationSelectionComponent.propTypes = {\n  bopisPickupLocationId: PropTypes.s