Component {\n  constructor(props) {\n    super(props);\n    this.handleAdyenPostMessages = this.handleAdyenPostMessages.bind(this);\n    this.onChallengeClose = this.onChallengeClose.bind(this);\n    this.onRedirectCancel = this.onRedirectCancel.bind(this);\n  }\n\n  // TODO https://jira.nike.com/browse/CCC-5034\n  // eslint-disable-next-line camelcase\n  UNSAFE_componentWillReceiveProps(nextProps) {\n    if (!this.props.showRedirectDrawer && nextProps.showRedirectDrawer) {\n      // add adyen event listener\n      window.addEventListener('message', this.handleAdyenPostMessages, false);\n    } else if (this.props.showRedirectDrawer && !nextProps.showRedirectDrawer) {\n      window.removeEventListener('message', this.handleAdyenPostMessages, false);\n    }\n  }\n\n  shouldComponentUpdate(nextProps) {\n    // If adyen challenge modal is in use, don't re-render\n    // Not sure if this is necessary\n    if (\n      this.props.challengeModalInUse &&\n      nextProps.challengeModalInUse\n    ) {\n      return false;\n    }\n    return true;\n  }\n\n  componentWillUnmount() {\n    window.removeEventListener('message', this.handleAdyenPostMessages, false);\n  }\n\n  onChallengeClose() {\n    const { dispatch } = this.props;\n    dispatch({\n      type: UPDATE_ACTIVE_ADYEN_EVENT,\n      payload: 'canceled',\n    });\n    this.props.dispatch(clearAdyenChallenge());\n  }\n\n  onRedirectCancel() {\n    const { dispatch } = this.props;\n    dispatch({\n      type: SET_ADYEN_REDIRECT_CANCELED,\n      payload: true,\n    });\n    dispatch({\n      type: CLEAR_ADYEN_REDIRECT_DATA,\n    });\n  }\n\n  handleAdyenPostMessages(e) {\n    // Handle Adyen Redirect postmessage\n    // eslint-disable-next-line no-undef\n    if (window.origin.indexOf(e.origin) > -1) {\n      // we only care if it has MD/PaRes\n      if (e.data) {\n        const MD = e.data.MD;\n        const PaRes = e.data.PaRes;\n        if (MD && PaRes) {\n          this.props.setAdyenData({\n            MD,\n            