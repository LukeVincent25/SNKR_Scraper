ple - 5-10 of 17',\n    defaultMessage: '{firstDisplayedItem}-{lastDisplayedItem} of {totalNumberOfItems}',\n  },\n  next: {\n    id: 'checkout.shipping.location.list.pagination.next',\n    description: 'Label for control used to move a paginated list to the next set of results',\n    defaultMessage: 'Next',\n  },\n  previous: {\n    id: 'checkout.shipping.location.list.pagination.previous',\n    description: 'Label for control used to move a paginated list to the previous set of results',\n    defaultMessage: 'Previous',\n  },\n  nextAria: {\n    id: 'checkout.shipping.location.list.pagination.next.aria',\n    description: 'Accessibility description for control used to move a paginated list to the next set of results',\n    defaultMessage: 'Display the next set of items in the {listName} list',\n  },\n  previousAria: {\n    id: 'checkout.shipping.location.list.pagination.previous.aria',\n    description: 'Accessibility description for control used to move a paginated list to the previous set of results',\n    defaultMessage: 'Display the previous set of items in the {listName} list',\n  },\n  viewMoreAria: {\n    id: 'checkout.shipping.location.list.pagination.viewMore.aria',\n    description: 'Accessibility description for control used to show more items on a list',\n    defaultMessage: 'Display more items in the {listName} list',\n  },\n  viewMoreLabel: {\n    id: 'checkout.shipping.pickup.viewMore',\n    description: 'Label for button that expands the list of stores',\n    defaultMessage: 'View More',\n  },\n};\n\nexport function normalizeMessageOptionCase(msgObj) {\n  return Object.keys(msgObj).reduce(\n    (acc, curr) => {\n      acc[curr] = (msgObj[curr].toLowerCase());\n      return acc;\n    }, {}\n  );\n}\n\nexport function createPaginatedList(list, paginationInterval) {\n  return list.reduce((acc, curr, i) => {\n    if (i && i % paginationInterval === 0) {\n      acc.push([curr]);\n    } else {\n      acc[acc.length - 1].push(curr);\n    }\n    return acc;\n  }, [[]]);\n}\n\nexport function createViewMoreList(list, limit) {\n  return list.slice(0, limit);\n}\n\nexport class ListComponent extends React.Component {\n  constructor(props) {\n    super(props);\n\n    this.onPaginateNext = this.onPaginateNext.bind(this);\n    this.onPaginatePrevious = this.onPaginatePrevious.bind(this);\n    this.onViewMore = this.onViewMore.bind(this);\n\n    this.state = {\n      pageIndex: 0,\n      useViewMore: false,\n      usePagination: false,\n    };\n  }\n\n  componentDidMount() {\n    const { options, paginationInterval, viewMoreLimit } = this.props;\n    const pagedList = paginationInterval ? createPaginatedList(options, paginationInterval) : null;\n    return this.setState({\n      lastPageIndex: pagedList && pagedList.length - 1,\n      totalListItemCount: options.length,\n      usePagination: Boolean(paginationInterval) && options.length > paginationInterval,\n      useViewMore: Boolean(viewMoreLimit) && options.length > viewMoreLimit,\n    });\n  }\n\n  onPaginateNext() {\n    const { pageIndex } = this.state;\n    this.setState({\n      pageIndex: pageIndex + 1,\n    });\n  }\n\n  onPaginatePrevious() {\n    const { pageIndex } = this.state;\n    this.setState({\n      pageIndex: pageIndex - 1 || 0,\n    });\n  }\n\n  onViewMore() {\n    this.setState({\n      useViewMore: false,\n    });\n  }\n\n  renderPaginationControls() {\n    const { messageOptions, paginationInterval } = this.props;\n    const { pageIndex, totalListItemCount, lastPageIndex } = this.state;\n\n    const firstDisplayedItem = (pageIndex * paginationInterval) + 1;\n    const lastDisplayedItem = firstDisplayedItem + paginationInterval - 1;\n\n    let isPreviousDisabled = {};\n    let isNextDisabled = {};\n\n    if (pageIndex === 0) {\n      isPreviousDisabled = { disabled: true };\n    }\n    if (pageIndex === lastPageIndex) {\n      isNextDisabled = { disabled: true };\n    }\n\n    return (\n      <div className=\"ncss-row pt4-sm\">\n        <div className=\"ncss-col-sm-4\">\n          <button\n            className=\"ncss-btn\"\n            onClick={this.onPaginatePrevious}\n            aria-label={\n              formatMessage(\n                messages.previousAria,\n                { ...(normalizeMessageOptionCase(messageOptions.ariaListName)) }\n              )\n            }\n            {...isPreviousDisabled}\n          >\n            <i className=\"g72-arrow-thin-left u-bold\" />\n            <span className=\"pl2-sm\">{formatMessage(messages.previous)}</span>\n          </button>\n        </div>\n        <div className=\"ncss-col-sm-4 ta-sm-c\">\n          <span>{\n            formatMessage(\n              messages.paginationCount,\n              {\n                firstDisplayedItem,\n                lastDisplayedItem,\n                totalNumberOfItems: totalListItemCount,\n              }\n            )}\n          </span>\n        </div>\n        <div className=\"ncss-col-sm-4 ta-sm-r\">\n          <button\n            className=\"ncss-btn\"\n            onClick={this.onPaginateNext}\n            aria-label={\n              formatMessage(\n                messages.nextAria,\n                { ...(normalizeMessageOptionCase(messageOptions.ariaListName)) }\n              )\n            }\n            {...isNextDisabled}\n          >\n            <span className=\"pr2-sm\">{formatMessage(messages.next)}</span>\n            <i className=\"g72-arrow-thin-right u-bold\" />\n          </button>\n        </div>\n      </div>\n    );\n  }\n\n  renderViewMore() {\n    const { messageOptions } = this.props;\n    return (\n      <div className=\"ta-sm-c pt4-sm\">\n        <button\n          data-attr=\"viewMoreButton\"\n          className=\"ncss-btn ncss-flyout-btn\"\n          onClick={this.onViewMore}\n          aria-label={\n            formatMessage(\n              messages.viewMoreAria,\n              { ...(normalizeMessageOptionCase(messageOptions.ariaListName)) }\n            )\n          }\n        >\n          {formatMessage(messages.viewMoreLabel)}\n        </button>\n      </div>\n    );\n  }\n\n  render() {\n    const {\n      listMessages,\n      messageOptions = {},\n      options,\n      onSelected,\n      paginationInterval,\n      selectedItemId,\n      viewMoreLimit,\n    } = this.props;\n\n    const {\n      useViewMore,\n      usePagination,\n    } = this.state;\n\n    let listOptions = options;\n    let paginationControls;\n\n    if (useViewMore) {\n      listOptions = createViewMoreList(options, viewMoreLimit);\n    } else if (usePagination) {\n      const pagedList = createPaginatedList(options, paginationInterval);\n      listOptions = pagedList[this.state.pageIndex];\n      paginationControls = this.renderPaginationControls(listOptions.length);\n    }\n    if (!listOptions.length) {\n      return (listMessages.noListItems &&\n        <div className=\"ncss-row\">\n          {listMessages.header &&\n            <div className=\"ncss-col-sm-12 ncss-brand pb2-sm u-uppercase fs16-sm\">\n              <h3 className=\"fs16-sm\">{formatMessage(listMessages.header)}</h3>\n            </div>\n          }\n          <div className={'ncss-col-sm-12 d-sm-ib va-sm-m pr5-mdx'}>\n            {\n              formatMessage(\n                listMessages.noListItems,\n                { ...(messageOptions.noListItems || {}) },\n              )\n            }\n          </div>\n        </div>\n      );\n    }\n    return (\n      <React.Fragment>\n        {listMessages.header &&\n        <div className=\"ncss-row test\">\n          <div className=\"ncss-col-sm-12 ncss-brand pb2-sm u-uppercase fs16-sm\">\n            <h3 className=\"fs16-sm\">{formatMessage(listMessages.header)}</h3>\n          </div>\n        </div>\n        }\n        <OptionsSelector\n          options={listOptions}\n          onSelected={onSelected}\n          defaultSelected={selectedItemId}\n        />\n        {useViewMore && this.renderViewMore()}\n        {!useViewMore && paginationControls}\n      </React.Fragment>\n    );\n  }\n}\n\nListComponent.propTypes = {\n  defaultSelected: PropTypes.string,\n  listMessages: PropTypes.shape({\n    header: PropTypes.string,\n    noListItems: PropTypes.string,\n  }),\n  messageOptions: PropTypes.shape({\n    ariaListName: PropTypes.shape({\n      listName: PropTypes.string,\n    }),\n  }),\n  options: PropTypes.arrayOf(PropTypes.shape({})),\n  onSelected: PropTypes.func,\n  selectedItemId: PropTypes.string,\n  viewMoreLimit: PropTypes.number,\n  paginationInterval: PropTypes.number,\n};\n\nexport default connect()(ListComponent);\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/List.js","import { SET_PICKUP_TAB_LOCATIONS_DISPLAYED, CLEAR_PICKUP_TAB_LOCATIONS_DISPLAYED } from '../constants';\n\nexport function setPickupTabLocationsDisplayed({ bopis, pups }) {\n  return (dispatch) => {\n    dispatch({\n      type: SET_PICKUP_TAB_LOCATIONS_DISPLAYED,\n      payload: {\n        bopis,\n        pups,\n      },\n    });\n  };\n}\n\nexport function clearPickupTabLocationsDisplayed() {\n  return (dispatch) => {\n    dispatch({\n      type: CLEAR_PICKUP_TAB_LOCATIONS_DISPLAYED,\n    });\n  };\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/state/app/actions/pickupTabLocationsDisplayedActionCreators.js","import PropTypes from 'prop-types';\nimport React from 'react';\nimport PickupRecipient from './PickupRecipient';\nimport PickupLocationSelection from './PickupLocationSelection';\n\nexport default function Pickup(props) {\n  const { handleRecipientSubmit, isEditingPickupLocation = true } = props;\n  const child = isEditingPickupLocation ?\n    (<PickupLocationSelection />) :\n    (<PickupRecipient handleRecipientSubmit={handleRecipientSubmit} />);\n  return child;\n}\n\nPickup.propTypes = {\n  isEditingPickupLocation: PropTypes.bool,\n};\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/shipping/Pickup.js","import React from 'react';\nimport PropTypes from 'prop-types';\n\nimport defineMessage