= disabledOptions.indexOf(option.id) > -1;\n  const hidden = hiddenOptions.indexOf(option.id) > -1;\n  const defaultChecked = selectedOption === option.id;\n  const iconClassNames = `${id}-icn${disabled ? '-disabled' : ''} d-sm-ib va-sm-m mr2-sm ta-sm-c`;\n  const isKlarna = option.id === 'Klarna';\n  const tooltipText = messages[`${id}TooltipContent`] ? formatMessage(messages[`${id}TooltipContent`]) : '';\n  const tooltipContent = [{ type: 'message', text: `${tooltipText}`, style: 'tip-left' }];\n  const containerClasses = 'ncss-col-sm-12 ncss-col-md-6 ncss-col-lg-6 va-sm-t';\n\n  // FYI: - credit card and paypal do not use the label var, everything else does but\n  // Sofort does not get a value set, and Klarna has its own override\n  let paymentLabel;\n  if (Object.keys(messages).indexOf(id) > -1) {\n    paymentLabel = formatMessage(messages[id]);\n  }\n  // TODO: CCC-000 Remove messages.klarna once CCC-3884 is deployed\n  if (isKlarna) {\n    paymentLabel = formatMessage(messages.klarnaPayLater);\n  }\n  if (hidden) {\n    return null;\n  }\n  return (\n    <React.Fragment key={`render-payment-option-${option.id}`}>\n      {/* Visa is available in every country that allows credit cards so we use it to determine\n      this a credit card icon */}\n      {/* \"checked\" attribute is needed when programmatically updating the input's selection status\n      to trigger the ui to show it. defaultChecked only works the first time */}\n      { option.id === 'Visa' &&\n        <div className={containerClasses}>\n          <PaymentRadioInput\n            checked={selectedOption === CREDIT_DEBIT}\n            customClass=\"mb4-sm mb2-md\"\n            defaultChecked={selectedOption === CREDIT_DEBIT}\n            disabled={disabled}\n            paymentType=\"creditDebit\"\n            onChangeHandler={onChangeHandler}\n          >\n            <label className=\"ncss-label pl6-sm\" htmlFor=\"creditDebit\">\n              <i\n                className=\"cc-icn d-sm-ib va-sm-m mr2-sm ta-sm-c\"\n                aria-label={formatMessage(messages.creditCardRadioButtonLabel)}\n              />\n              <span className=\"d-sm-ib va-sm-m\">\n                { formatMessage(messages.creditCardRadioButtonLabel) }\n              </span>\n            </label>\n          </PaymentRadioInput>\n        </div>\n      }\n      { option.id === 'Paypal' &&\n        <div className={containerClasses}>\n          <PaymentRadioInput\n            checked={selectedOption === PAYPAL}\n            customClass=\"mb4-sm mb2-md\"\n            defaultChecked={selectedOption === PAYPAL}\n            disabled={disabled}\n            paymentType={PAYPAL}\n            onChangeHandler={onChangeHandler}\n          >\n            <label className=\"ncss-label pl6-sm\" htmlFor={PAYPAL}>\n              <img\n                className=\"paypal-logo d-sm-ib va-sm-m ta-sm-c\"\n                src={paypalPng}\n                alt=\"PayPal\"\n              />\n            </label>\n          </PaymentRadioInput>\n        </div>\n      }\n      {option.id === APPLE_PAY &&\n        <div className={containerClasses}>\n          <PaymentRadioInput\n            checked={selectedOption === APPLE_PAY}\n            defaultChecked={selectedOption === APPLE_PAY}\n            disabled={disabled}\n            paymentType={APPLE_PAY}\n            onChangeHandler={onChangeHandler}\n          >\n\n            <label className=\"ncss-label pl6-sm\" htmlFor={APPLE_PAY}>\n              <div className=\"applepay-logo\" />\n            </label>\n          </PaymentRadioInput>\n        </div>\n      }\n\n      { option.id !== 'Paypal' && option.id !== 'Visa' && option.id !== 'GiftCard' && option.id !== APPLE_PAY &&\n        <div className={containerClasses}>\n          <PaymentRadioInput\n            checked={defaultChecked}\n            customClass=\"mb4-sm mb2-md\"\n            defaultChecked={defaultChecked}\n            disabled={disabled}\n            paymentType={option.id}\n            onChangeHandler={onChangeHandler}\n          >\n            <label className=\"ncss-label pl6-sm\" htmlFor={option.id}>\n              {getIconContent(option, iconClassNames, paymentLabel)}\n            </label>\n            { disabled &&\n              <Tooltip\n                showIcon\n                tooltipContent={tooltipContent}\n                tooltipId={`${option.id}Tooltip`}\n              />\n            }\n          </PaymentRadioInput>\n        </div>\n      }\n    </React.Fragment>\n  );\n}\n\nexport default function PaymentOptionsComponent(props) {\n  const {\n    disabledOptions,\n    hiddenOptions,\n    onChangeHandler,\n    options,\n    selectedOption,\n  } = props;\n  return (\n    <div className=\"ncss-row\">\n      { options.map(option => renderOption(\n        option,\n        selectedOption,\n        disabledOptions,\n        hiddenOptions,\n        onChangeHandler,\n      )) }\n    </div>\n  );\n}\n\nPaymentOptionsComponent.propTypes = {\n  disabledOptions: PropTypes.arrayOf(PropTypes.string),\n  hiddenOptions: PropTypes.arrayOf(PropTypes.string),\n  onChangeHandler: PropTypes.func,\n  options: PropTypes.arrayOf(PropTypes.shape({})),\n  selectedOption: PropTypes.string,\n};\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/payment/PaymentOptions.js","import PropTypes from 'prop-types';\nimport React from 'react';\nimport classNames from 'classnames';\nimport formatMessage from '../../../helpers/formatMessage';\n\nconst messages = {\n  removeGiftCardButton: {\n    id: 'checkout.payment.removePrepaidCard.button',\n    description: 'Accessibility label for button that removes an applied gift card from an order',\n    defaultMessage: 'Remove Gift Card',\n  },\n  removePromCodeButton: {\n    id: 'checkout.payment.removePromoCode.button',\n    description: 'Accessibility label for button that removes an applied promo code from an order',\n    defaultMessage: 'Remove Promo Code',\n  },\n};\n\nexport default function PrepaidPromoItem(props) {\n  const {\n    type,\n    message,\n    cardValue,\n    onClick,\n  } = props;\n\n  const isGiftCard = type === 'giftCard';\n\n  const cardClassNames = `card-icon d-sm-ib u-max-width ${type === 'promoCode' ? 'promocode-icn' : 'giftcard-icn'}`;\n\n  const rowClassNames = classNames(\n    'gift-card',\n    'ncss-row',\n    'border-top-light-grey',\n    'fs14-sm',\n    'lh12-sm',\n    'prl2-sm',\n    'pt4-sm',\n    'pt2-md',\n    'pb3-sm',\n    'pb2-md',\n  );\n\n  const columnClassNames = classNames(\n    'ncss-col-sm-10',\n    'ncss-col-md-3',\n    'responsive-text-align-lr',\n    'va-sm-t',\n  );\n\n  const buttonClassNames = classNames(\n    'custom-x-button',\n    'text-color-black',\n    'bg-white',\n    'fs24-sm',\n    'pl4-sm',\n    'g72-x-small',\n  );\n\n  return (\n    <li\n      className={rowClassNames}\n    >\n      <div className=\"ncss-col-sm-2 ncss-col-md-9 ta-sm-l va-sm-m\">\n        <span className={cardClassNames} />\n        <span className=\"prepaidMethod mr2-sm text-color-grey d-sm-h d-md-ib\">\n          {message}\n        </span>\n      </div>\n\n      <div className={columnClassNames}>\n        { isGiftCard &&\n          <span className=\"card-value u-bold lh40-sm va-sm-m\">\n            {cardValue}\n          </span> }\n        <button\n          className={buttonClassNames}\n          aria-label={\n            formatMessage(\n              isGiftCard ?\n                messages.removeGiftCardButton :\n                messages.removePromCodeButton\n            )\n          }\n          onClick={onClick}\n        />\n      </div>\n\n      <div className=\"ncss-col-sm-12 d-md-h\">\n        <span className=\"mr2-sm text-color-grey\">\n          {message}\n        </span>\n      </div>\n\n    </li>\n  );\n}\n\nPrepaidPromoItem.propTypes = {\n  message: PropTypes.string,\n  type: PropTypes.string,\n  cardValue: PropTypes.string,\n  onClick: PropTypes.func,\n};\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/payment/PrepaidPromoItem.js","import PropTypes from 'prop-types';\nimport React from 'react';\nimport get from 'lodash.get';\n\nimport formatMessage from '../../../helpers/formatMessage';\nimport {\n  invalidPromo,\n  expiredPromo,\n  promoNotApplied,\n  MEMBERSHIP_REQUIRED,\n  SWOOSH_REQUIRED,\n  SWOOSH_EXCLUDED,\n  ORDER_THRESHOLD_NOT_MET,\n  ORDER_THRESHOLD_EXCEEDED,\n  SHIPPING_NOT_DISCOUNTED,\n  SHIPPING_THRESHOLD_NOT_MET,\n  SHIPPING_THRESHOLD_EXCEEDED,\n} from '../../../constants/promos';\n\nconst messages = {\n  [promoNotApplied]: {\n    id: 'checkout.payment.promocode.not.applied',\n    description: 'Span showing promo code not applied',\n    defaultMessage: '{code} Promo Code is not applicable at this time.',\n  },\n  [invalidPromo]: {\n    id: 'checkout.payment.promocode.invalid',\n    description: 'Span showing promo code invalid',\n    defaultMessage: '{code} Promo Code is invalid at this time. Please try another Promo Code.',\n  },\n  [expiredPromo]: {\n    id: 'checkout.payment.promocode.expired',\n    description: 'Span showing promo code expired',\n    defaultMessage: '{code} Promo Code is expired at this time. Please try another Promo Code.',\n  },\n  [MEMBERSHIP_REQUIRED]: {\n    id: 'checkout.payment.promocode.invalid.memberShipRequired',\n    description: 'An error message shown to the user explaining that their promo code could not be used because they need to be a member',\n    defaultMessage: '{code} Promo Code is for Members only. Join us or sign in to your Nike profile to use it.',\n  },\n  [SWOOSH_REQUIRED]: {\n    id: 'checkout.payment.promocode.invalid.swooshRequired',\n    description: 'An error message shown to the user explaining that their promo code could not be used because they need to be a Swoosh user',\n    defaultMessage: '{code} Promo Code can only be used on swoosh.com. Create or sign in to your Swoosh account to get a discount.',\n  },\n  [SWOOSH_EXCLUDED]: {\n    id: 'checkout.payment.promocode.invalid.swooshExcluded',\n    description: 'An error message shown to the user explaining that their promo code could not be used because they are a Swoosh user',\n    defaultMessage: '{code} Promo Code can’t be used on swoosh.com. Sign in to your Nike profile to get a discount.',\n  },\n  [ORDER_THRESHOLD_NOT_MET]: {\n    id: 'checkout.payment.promocode.invalid.orderThresholdNotMet',\n    description: 'An error message shown to the user explaining that their promo code could not be used because their order did not meet the cost threshold',\n    defaultMessage: '{code} Promo Code doesn’t apply to your order yet. Try adding something to your bag.',\n  },\n  [ORDER_THRESHOLD_EXCEEDED]: {\n    id: 'checkout.payment.promocode.invalid.orderThresholdExceeded',\n    description: 'An error message shown to the user explaining that their promo code could not be used because their order cost exceeded the promo cost threshold',\n    defaultMessage: '{code} Promo Code doesn’t apply to your order yet. Remove something from your bag.',\n  },\n  [SHIPPING_NOT_DISCOUNTED]: {\n    id: 'checkout.payment.promocode.invalid.shippingNotDiscounted',\n    description: 'An error message shown to the user explaining that their promo code could not be used for shipping',\n    defaultMessage: '{code} Promo Code can’t be used for shipping. Please try another one or sign in to your Nike profile for free shipping every time.',\n  },\n  [SHIPPING_THRESHOLD_NOT_MET]: {\n    id: 'checkout.payment.promocode.invalid.shippingThresholdNotMet',\n    description: 'An error message shown to the user explaining that their promo code could not be used because their subtotal is too low for shipping discounts',\n    defaultMessage: 'With {code} Promo Code, your subtotal is too low for shipping discounts. Add something to your bag or remove the promo code.',\n  },\n  [SHIPPING_THRESHOLD_EXCEEDED]: {\n    id: 'checkout.payment.promocode.invalid.shippingThresholdExceeded',\n    description: 'An error message shown to the user explaining that their promo code could not be used because their subtotal is too high for shipping discounts',\n    defaultMessage: 'With {code} Promo Code, your subtotal is too high for shipping discounts. Remove something from your bag or remove the promo code.',\n  },\n};\n\nexport function InvalidPromoCode({ promoObj }) {\n  const displayText = promoObj.displayName ? `\"${promoObj.displayName}\"` : promoObj.code;\n  const failureType = get(promoObj, 'reasons[0].code', promoObj.status);\n\n  const message = messages[failureType];\n\n  return (\n    <p className=\"js-error text-color-error mb2-sm\">{formatMessage(message, { code: displayText })}</p>\n  );\n}\n\nInvalidPromoCode.propTypes = {\n  promoObj: PropTypes.shape({\n    code: PropTypes.string,\n    displayName: PropTypes.string,\n    status: PropTypes.string,\n  }),\n};\n\nexport default InvalidPromoCode;\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/payment/InvalidPromoCode.js","import PropTypes from 'prop-types';\nimport React from 'react';\nimport get from 'lodash.get';\nimport { DELETE_PREPAID_METHOD } from '../../../state/checkout';\nimport { deletePromotionCode } from '../../../state/app';\nimport PrepaidPromoItem from './PrepaidPromoItem';\nimport formatCurrency from '../../../helpers/formatCurrency';\nimport formatMessage from '../../../helpers/formatMessage';\nimport defineMessages from '../../../helpers/defineMessages';\nimport {\n  invalidPromo,\n  expiredPromo,\n  promoNotApplied,\n  promoApplied,\n} from '../../../constants/promos';\nimport InvalidPromoCodeComponent from './InvalidPromoCode';\n\nconst rejectedPromoStatusArray = [invalidPromo, expiredPromo, promoNotApplied];\n\nconst messages = defineMessages({\n  giftCardAppliedText: {\n    id: 'checkout.payment.giftcard.applied',\n    description: 'Span showing what was applied',\n    defaultMessage: 'Gift Card ending in {lastFour} applied',\n  },\n  voucherAppliedText: {\n    id: 'checkout.payment.voucher.applied',\n    description: 'Span showing what was applied',\n    defaultMessage: 'Product Voucher ending in {lastFour} applied',\n  },\n  promoCodeAppliedText: {\n    id: 'checkout.payment.promocode.applied',\n    description: 'Span showing promo code applied',\n    defaultMessage: 'Promo Code {code} applied',\n  },\n});\n\nexport function PrepaidCardList({ prepaidMethods, promotionCodes = {}, dispatch }) {\n  function getPrepaidText(prepaidMethod) {\n    const accountNumber = get(prepaidMethod, ['accountNumber'], '');\n    const length = accountNumber.length;\n    const lastFour = accountNumber.substring(length - 4, length); // $SR\n    return formatMessage(accountNumber.charAt(0) === '2' ?\n      messages.voucherAppliedText : messages.giftCardAppliedText,\n    {\n      lastFour,\n    }); // $SR\n  }\n\n  function getBalance(balance) { // $SR\n    return balance || 0;\n  }\n  // Filter out invalid/not applied codes\n  const appliedCodes =\n    Object.keys(promotionCodes).filter(key => promotionCodes[key].status === promoApplied);\n\n  // Invalid / Not applied promo codes\n  const invalids = Object.keys(promotionCodes).map((key) => {\n    const promoObj = promotionCodes[key];\n    if (rejectedPromoStatusArray.includes(promoObj.status)) {\n      return (\n        <InvalidPromoCodeComponent promoObj={promoObj} />\n      );\n    }\n    return null;\n  }).filter(invalid => !!invalid);\n\n  const anyCodes = !!(Object.keys(prepaidMethods).length || Object.keys(promotionCodes).length);\n  return (\n    anyCodes &&\n      <div className=\"prepaid-promo-list mt4-sm\">\n        {!!invalids.length &&\n          <div className=\"ncss-row mb2-sm\">\n            <div className=\"ncss-col-sm-12 ncss-col-md-10 prl6-sm\">\n              {invalids}\n            </div>\n          </div>\n        }\n        <ul className=\"ncss-container list-of-gc border-light-grey\">\n\n          {/* Promo Codes */}\n          { appliedCodes && appliedCodes.map((promoCode) => {\n            // use display name if available, otherwise use code\n            const promoObj = promotionCodes[promoCode] || {};\n            const displayText = promoObj.displayName || promoObj.code;\n\n            return (\n              <PrepaidPromoItem\n                type=\"promoCode\"\n                message={formatMessage(messages.promoCodeAppliedText, { code: displayText })}\n                key={promoCode}\n                onClick={() => { dispatch(deletePromotionCode(promoCode)); }}\n              />\n            );\n          })}\n\n          {/* Gift Card/Vouchers */}\n          { Object.keys(prepaidMethods).map(prepaidMethodId =>\n            <PrepaidPromoItem\n              type=\"giftCard\"\n              message={getPrepaidText(prepaidMethods[prepaidMethodId])}\n              cardValue={\n                formatCurrency(getBalance(prepaidMethods[prepaidMethodId].balance))\n              }\n              key={prepaidMethodId}\n              onClick={() => dispatch({\n                type: DELETE_PREPAID_METHOD,\n                payload: prepaidMethodId,\n              })}\n            />\n          )}\n        </ul>\n      </div>\n  );\n}\n\nPrepaidCardList.propTypes = {\n  prepaidMethods: PropTypes.shape({}),\n  dispatch: PropTypes.func,\n  promotionCodes: PropTypes.shape({}),\n};\n\n\nexport default PrepaidCardList;\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/payment/PrepaidCardList.js","import React from 'react';\nimport PropTypes from 'prop-types';\nimport { compose } from 'redux';\nimport { reduxForm, reset } from 'redux-form';\nimport { addPaymentMethod, SET_ERRORS } from '../../../state/app';\n\nimport { createInlineValidator } from '../../../helpers/validation/index';\nimport { getCountry } from '../../../helpers/getGlobals';\nimport formatMessage from '../../../helpers/formatMessage';\nimport defineMessages from '../../../helpers/defineMessages';\n\nimport FieldWrapper from '../FieldWrapper';\n\nconst messages = defineMessages({\n  addAnotherPrepaidCard: {\n    id: 'checkout.payment.add.another',\n    description: 'Text for link to add another prepaid card',\n    defaultMessage: 'Add Another',\n  },\n  applyButton: {\n    id: 'checkout.payment.apply.button.label',\n    description: 'Apply button label',\n    defaultMessage: 'APPLY',\n  },\n  codTooltip: {\n    id: 'checkout.payment.prepaidCard.cod.tooltip',\n    description: 'Tooltip explaining gift cards cannot be used with COD payment method',\n    defaultMessage: 'Gift Cards cannot be used with Cash On Delivery payment method.',\n  },\n  duplicateGiftCard: {\n    id: 'C90587A5',\n    description: 'Throw error when user tries to add gift card more than once',\n    defaultMessage: 'You have already added this gift card',\n  },\n  giftCardTooltipHeader: {\n    id: 'checkout.payment.giftcard.tooltip.header',\n    description: 'Tooltip header text',\n    defaultMessage: 'How to find the gift card number',\n  },\n  giftCardTooltipSecondary: {\n    id: 'checkout.payment.giftcarcd.tooltip.secondary',\n    description: 'Explains how many gift cards can be used on a single order',\n    defaultMessage: 'NOTE: You may use up to 10 gift cards on a single order. A new order should be placed if you have subsequent gift cards to redeem, and have already redeemed 10 gift cards on this order.',\n  },\n  giftCardTooltipCardTypeOne: {\n    id: 'checkout.payment.giftcarcd.tooltip.cardTypeOne',\n    description: 'Denotes format of card',\n    defaultMessage: '19-digit card number & 6-digit PIN',\n  },\n  giftCardRemainingText: {\n    id: 'checkout.payment.giftcard.remaining',\n    description: 'Remaining balance text',\n    defaultMessage: 'Remaining balance owed on your order',\n  },\n  prepaidCardInputLabel: {\n    id: 'checkout.payment.prepaidCard.input.label',\n    description: 'Label for gift card or voucher input field',\n    defaultMessage: 'Gift Card or Voucher Number',\n  },\n  prepaidCardSwooshInputLabel: {\n    id: 'checkout.payment.prepaidCard.swoosh.input.label',\n    description: 'Label for gift card swoosh input field',\n    defaultMessage: 'Gift Card Number',\n  },\n  prepaidCardInputAdditionalLabel: {\n    id: 'checkout.payment.prepaidCard.input.add.label',\n    description: 'Label for adding a second or additional gift card or voucher.',\n    defaultMessage: 'Add Another Gift Card or Voucher',\n  },\n  prepaidCardSwooshInputAdditionalLabel: {\n    id: 'checkout.payment.prepaidCard.swoosh.input.add.label',\n    description: 'Label for adding a second or additional swoosh gift card.',\n    defaultMessage: 'Add Another Gift Card',\n  },\n});\n\nfunction addPrepaidCard(dispatch, prepaidCardNumber, pinNumber, address, duplicatePrepaidMethods) {\n  const regexedPrepaidCardNumber = prepaidCardNumber.replace(/\\s+/g, '');\n  if (duplicatePrepaidMethods.length > 0) {\n    dispatch({\n      error: true,\n      type: SET_ERRORS,\n      payload: [{\n        crcCode: 'C90587A5',\n        message: formatMessage(messages.duplicateGiftCard),\n      }],\n    });\n    return;\n  }\n  dispatch(\n    addPaymentMethod({\n      newPayment: {\n        type: 'GiftCard',\n        accountNumber: regexedPrepaidCardNumber,\n        pin: pinNumber,\n        address,\n      },\n      savePayment: true,\n    })\n  );\n  dispatch(reset('prepaidCardForm'));\n}\n\nexport function PrepaidCardForm({\n  address,\n  dispatch,\n  duplicatePrepaidMethods,\n  hasPrepaidMethods,\n  isCod,\n  isGiftCardPaymentAllowed,\n  isPinRequired,\n  isSwoosh,\n  pinNumber,\n  prepaidCardNumber,\n  prepaidData,\n  prepaidFormVisible,\n}) {\n  const inlineValidator = createInlineValidator({ type: 'prepaidcard', country: getCountry().toLowerCase() });\n  const sectionClasses = `ncss-row ncss-form-group gift-card-container ${isPinRequired ? 'show-pin' : ''} ${prepaidFormVisible ? '' : 'd-sm-h'}`;\n  const prepaidDivClasses = `${!isGiftCardPaymentAllowed ? 'mod-disabled' : ''} ncss-col-sm-12 ncss-col-md-10 va-sm-t mb4-sm mb0-md`;\n\n  const toolTipText = isCod\n    ? formatMessage(messages.codTooltip)\n    : (\n      <div>\n        <header className=\"pb6-sm prl2-sm\">\n          <h4 className=\"ncss-brand fs16-sm u-uppercase\">\n            {formatMessage(messages.giftCardTooltipHeader)}\n          </h4>\n        </header>\n        <div className=\"ncss-container pb2-sm\">\n          <div className=\"ncss-row\">\n            <div className=\"ncss-col-sm-12 pt2-sm pr4-md\">\n              <p className=\"fs12-sm lh12-sm pb1-sm ta-sm-l\">\n                {formatMessage(messages.giftCardTooltipCardTypeOne)}\n              </p>\n              <div className=\"giftCardPng\" />\n            </div>\n            <p className=\"fs12-sm lh12-sm\">{formatMessage(messages.giftCardTooltipSecondary)}</p>\n          </div>\n        </div>\n      </div>\n    );\n\n  const tooltipContent = isCod\n    ? [{ type: 'message', text: toolTipText, style: 'tip-left' }]\n    : [{ type: 'giftcard', multiText: toolTipText, style: 'tip-center' }];\n\n  const tooltipCustom = tooltipContent[0].type === 'giftcard' ? 'wide' : '';\n  const addMethodMessage = hasPrepaidMethods ?\n    formatMessage(messages.prepaidCardInputAdditionalLabel)\n    : formatMessage(messages.prepaidCardInputLabel);\n\n  const addSwooshMethodMessage = hasPrepaidMethods ?\n    formatMessage(messages.prepaidCardSwooshInputAdditionalLabel)\n    : formatMessage(messages.prepaidCardSwooshInputLabel);\n\n  const disableButton = (prepaidData && prepaidData.syncErrors) || !isGiftCardPaymentAllowed;\n\n  return (\n    <section className={sectionClasses}>\n      <div className={prepaidDivClasses}>\n        <div className=\"ncss-row\">\n          <FieldWrapper\n            id={'prepaidCardNumber'}\n            label={isSwoosh ? addSwooshMethodMessage : addMethodMessage}\n            type=\"input\"\n            inputType=\"tel\"\n            validate={inlineValidator('prepaidCardNumber')}\n            wrapperStyle=\"ncss-col-md-8 ncss-col-sm-12 ncss-input-container va-sm-t number-input\"\n            tooltipContent={tooltipContent}\n            tooltipEnabled\n            tooltipId={'codTooltip'}\n            tooltipCustom={tooltipCustom}\n            showIcon\n            disabled={!isGiftCardPaymentAllowed}\n          />\n          <FieldWrapper\n            id={'pinNumber'}\n            label=\"PIN\"\n            type=\"input\"\n            inputType=\"tel\"\n            validate={inlineValidator('pinNumber')}\n            wrapperStyle=\"ncss-col-md-4 ncss-col-sm-12 mod-ncss-input-container ncss-input-container va-sm-t pin-input\"\n            disabled={!isGiftCardPaymentAllowed}\n          />\n        </div>\n      </div>\n      <div className=\"ncss-col-sm-12 ncss-col-md-2 mt0-sm mt12-md va-sm-t\">\n        <button\n          onClick={\n            () => {\n              addPrepaidCard(\n                dispatch,\n                prepaidCardNumber,\n                pinNumber,\n                address,\n                duplicatePrepaidMethods,\n              );\n            }\n          }\n          className=\"mod-ncss-btn ncss-btn-black ncss-brand pt3-sm prl5-sm pb3-sm pt2-lg pb2-lg u-uppercase u-full-width u-rounded\"\n          disabled={disableButton}\n        >\n          {formatMessage(messages.applyButton)}\n        </button>\n      </div>\n    </section>\n  );\n}\n\nPrepaidCardForm.propTypes = {\n  address: PropTypes.shape({}),\n  dispatch: PropTypes.func,\n  duplicatePrepaidMethods: PropTypes.array, // eslint-disable-line react/forbid-prop-types\n  hasPrepaidMethods: PropTypes.bool,\n  isCod: PropTypes.bool,\n  isGiftCardPaymentAllowed: PropTypes.bool,\n  isPinRequired: PropTypes.bool,\n  isSwoosh: PropTypes.bool,\n  pinNumber: PropTypes.string,\n  prepaidCardNumber: PropTypes.string,\n  prepaidData: PropTypes.shape({\n    syncErrors: PropTypes.shape({}),\n  }),\n  prepaidFormVisible: PropTypes.bool,\n};\n\nexport default compose(\n  reduxForm({ form: 'prepaidCardForm', initialValues: {} })\n)(PrepaidCardForm);\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/payment/PrepaidCardForm.j