n the root directory of this source tree.\n */\n\nif (process.env.NODE_ENV !== 'production') {\n  var ReactIs = require('react-is');\n\n  // By explicitly using `prop-types` you are opting into new development behavior.\n  // http://fb.me/prop-types-in-prod\n  var throwOnDirectAccess = true;\n  module.exports = require('./factoryWithTypeCheckers')(ReactIs.isElement, throwOnDirectAccess);\n} else {\n  // By explicitly using `prop-types` you are opting into new production behavior.\n  // http://fb.me/prop-types-in-prod\n  module.exports = require('./factoryWithThrowingShims')();\n}\n\n\n\n//////////////////\n// WEBPACK FOOTER\n// ./node_modules/react-transition-group/node_modules/prop-types/index.js\n// module id = 848\n// module chunks = 1 2","/* eslint-disable react/no-unused-prop-types */\nimport React, { Component } from 'react';\nimport PropTypes from 'prop-types';\nimport classnames from 'classnames';\n/**\n *\n * From a standpoint of interaction, this component acts like a fake form selector with\n * a list of inputs of type radio.\n *\n *\n * As an example, lets say we want to display Shipping Methods:\n *\n * ```\n * const options = [\n *   {\n *     id: 'standard',\n *     label: 'Standard (Arrives 06/24/2016)',\n *     extra: 'FREE',\n *     accent: true, // this will display the extra text on the right in black\n *   },\n *   {\n *     id: 'twoDay',\n *     label: 'Two-Day (Arrives 06/22/2016)',\n *     extra: '$15.00',\n *   },\n *   {\n *     id: 'nextDay',\n *     label: 'Next-Day (Arrives 06/21/2016)',\n *     extra: '$25.00',\n *   },\n * ]\n * <OptionsSelector\n *   options={options}\n *   defaultSelected=\"standard\",\n *   onSelected={(selected) => console.log('selected: ', selected)}\n *   styles=\"mb4-sm fs15-sm\"\n *  />\n * ```\n *\n */\nexport default class OptionsSelector extends Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      selectedOption: this.props.defaultSelected,\n    };\n\n    this.handleOnChange = this.handleOnChange.bind(this);\n    this.renderOption = this.renderOption.bind(this);\n  }\n\n  // TODO https://jira.nike.com/browse/CCC-5034\n  // eslint-disable-next-line camelcase\n  UNSAFE_componentWillReceiveProps(nextProps) {\n    this.setState({\n      selectedOption: nextProps.defaultSelected,\n    });\n  }\n\n  /* React is pretty loose with radios apparently, clicking on an already selected radio will\n     still trigger the 'change' event so we check and manually block this. Also a keyboard user\n     will trigger each option when navigating through the list with the arrow keys... Could not\n     find a way around that, no key info available and event type is change not focus\n  */\n  handleOnChange(e) {\n    const { id, value } = e.currentTarget;\n\n    if (id !== this.state.selectedOption) {\n      this.setState({\n        selectedOption: id,\n      });\n      this.props.onSelected(value);\n    }\n  }\n\n  renderOption(option) {\n    const { isGuest, disabled } = this.props;\n    const isChecked = this.state.selectedOption === option.id;\n\n    const optionContainerClassNames = `\n      js-option-container\n      ${option.id}\n      ${isGuest ? '' : ' mod-ncss-radio-container ncss-radio-container'}\n      ${isChecked ? 'bg-white' : 'bg-offwhite'}`;\n    const inputClassNames = isGuest ? 'd-sm-h' : 'ncss-radio';\n    const labelClassNames = `ncss-label label-extra ${isGuest ? 'd-sm-ib' : 'pl4-sm'} pt3-sm pb3-sm pr4-sm pt2-lg pb2-lg`;\n    const labelSpanClassNames = `${isGuest ? 'pl4-sm' : 'pl8-sm'} d-sm-b va-sm-m`;\n    const spanExtraClassNames = `\n      ${option.accent ? option.accentClassNames || 'text-color-black' : ''}\n      span-extra fl-sm-r pt3-sm pr2-sm\n      ${option.disabled ? 'disabled' : ''}`; // $SR\n    const checkedValue = isChecked ? 'checked' : ''; // $SR\n    const disableThisOption = disabled || option.disabled; // $SR\n    const alwaysShowContent = option.alwaysShowContent;\n\n    return (\n      <div key={isGuest ? 'no-id' : option.id} className={`${optionContainerClassNames} ${checkedValue}`}>\n        <input\n          type=\"radio\"\n          name={this.props.name}\n          className={inputClassNames}\n          id={option.id}\n          value={option.id}\n          onChange={this.handleOnChange}\n          checked={checkedValue}\n          disabled={disableThisOption}\n        />\n        <label\n          className={labelClassNames}\n          htmlFor={option.id}\n        >\n          <span className={labelSpanClassNames}>\n            {option.label}\n            {option.toolTip}\n          </span>\n          {/* Content that is always shown is rendered as part of the label */}\n          {alwaysShowContent && option.content}\n        </label>\n        {(checkedValue && !alwaysShowContent) && option.content}\n        <span className={spanExtraClassNames}>\n          {option.extra}\n        </span>\n      </div>\n    );\n  }\n\n  render() {\n    const { options, styles } = this.props;\n    const wrapperStyles = classnames('mod-ncss-form-group ncss-form-group group-up', styles);\n    if (!options || !options.length) return <div />;\n    return (\n      <div className={wrapperStyles}>\n        { options.map(option => this.renderOption(option)) }\n      </div>\n    );\n  }\n}\n\nOptionsSelector.propTypes = {\n  /**\n   * List of options\n   */\n  options: PropTypes.arrayOf(\n    PropTypes.shape({\n      id: PropTypes.string,\n      label: PropTypes.oneOfType([\n        PropTypes.string,\n        PropTypes.element,\n        PropTypes.node,\n        PropTypes.array,\n      ]),\n      extra: PropTypes.oneOfType([\n        PropTypes.string,\n        PropTypes.element,\n        PropTypes.node,\n      ]),\n      disabled: PropTypes.bool,\n      // Always shows content regardless of whether the checkbox is checked\n      alwaysShowContent: PropTypes.bool,\n    })\n  ),\n  /*\n   * ID for default selected\n   */\n  defaultSelected: PropTypes.string,\n  /**\n   * name to identify radio buttons group\n   */\n  name: PropTypes.string,\n  /**\n   * class names to be appended to the wrapper\n   */\n  styles: PropTypes.string,\n  /**\n   * function that will be called when an option is selected\n   */\n  onSelected: PropTypes.func,\n  /**\n   * boolean that will disable all options\n   */\n  disabled: PropTypes.bool,\n  isGuest: PropTypes.bool,\n};\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/OptionsSelector.js","import { removeEmptyValues } from '@nike/ux-tread-dotcom-utils';\n\n/**\n * Takes baseUrl and Parameters to create a single url to request.\n * Cleans empty divs and encodes by default\n *\n * @param {object} params\n * @prop {string} baseUrl - base url of request\n * @prop {object} parameters - Object to be created into URL search parameters\n */\nexport default function getApiUrl({ baseUrl, parameters }) {\n  // clean undefined\n  const cleanParameters = removeEmptyValues(parameters);\n  // Could have used URL if not for IE :/\n  const reducedParameters = Object.keys(cleanParameters).reduce((accumulator = [], key) => {\n    const value = encodeURIComponent(cleanParameters[key]);\n    accumulator.push(`${key}=${value}`);\n    return accumulator;\n  }, []);\n  return `${baseUrl}?${reducedParameters.join('&')}`;\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/packages/ux-tread-address-locate/src/utils/get-api-url.js","import { fetchClient } from '@nike/fetch-client';\nimport getApiUrl from '../utils/get-api-url';\n\nimport {\n  DEFAULT_ADDRESS_API_KEY,\n  DEFAULT_COUNTRIES,\n  DEFAULT_LANGUAGE,\n  DEFAULT_LIMIT,\n  FIND_ERROR_FALLBACK_MESSAGE,\n  INTERACTIVE_FIND,\n  LOQATE_BASE,\n  pageActionName,\n} from '../constants';\n/**\n * Calls Addressey Api with parameters supplied to get verified addresses.\n * See: https://www.loqate.com/resources/support/apis/Capture/Interactive/Find/1.1/\n *\n * @memberof ux-tread-address-locate\n * @param {object} options - named parameters\n * @prop {string} [key] - Addressey API Key\n * @prop {string} text - text to use to search for address (Gets encoded so don't encode)\n * @prop {string} [origin] - A starting location for the search.\n * @prop {array|string} [countries=[us]] - Comma separated list country codes to limit search within\n * @prop {number} [limit=5] - number of results to return\n * @prop {string} [language=en] - language to return results in\n * @prop {string} [container] - Container identification for Building/Apt/Sub Building addresses\n */\nexport default function findAddresses({\n  key = DEFAULT_ADDRESS_API_KEY,\n  text = undefined,\n  origin = undefined,\n  countries = DEFAULT_COUNTRIES,\n  limit = DEFAULT_LIMIT,\n  language = DEFAULT_LANGUAGE,\n  container,\n}) {\n  const baseUrl = `${LOQATE_BASE}${INTERACTIVE_FIND}`;\n  const apiUrl = getApiUrl({\n    baseUrl,\n    parameters: {\n      Key: key,\n      Text: text,\n      Origin: origin,\n      Countries: Array.isArray(countries) ? countries.join(',') : countries,\n      Limit: limit,\n      Language: language,\n      Container: container,\n    },\n  });\n\n  return fetchClient(\n    apiUrl,\n    {},\n    FIND_ERROR_FALLBACK_MESSAGE,\n    { pageActionName },\n  );\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/packages/ux-tread-address-locate/src/api/find-addresses.js","import { fetchClient } from '@nike/fetch-client';\nimport getApiUrl from '../utils/get-api-url';\n\nimport {\n  DEFAULT_ADDRESS_API_KEY,\n  FIND_ERROR_FALLBACK_MESSAGE,\n  INTERACTIVE_RETRIEVE,\n  LOQATE_BASE,\n  pageActionName,\n} from '../constants';\n\n/**\n * Calls Addressey Api with parameters supplied to get verified address.\n * See: https://www.loqate.com/resources/support/apis/Capture/Interactive/Retrieve/1/\n *\n * @memberof ux-tread-address-locate\n * @param {object} options - named parameters\n * @prop {string} [key] - Addressey API Key\n * @prop {bool} [cache=true] - cache response?\n * @prop {string} id - id of the address\n */\nexport default function retrieveAddress({\n  key = DEFAULT_ADDRESS_API_KEY,\n  cache = true,\n  id = undefined,\n  ...rest\n}) {\n  const baseUrl = `${LOQATE_BASE}${INTERACTIVE_RETRIEVE}`;\n  const apiUrl = getApiUrl({\n    baseUrl,\n    parameters: {\n      Key: key,\n      Id: id,\n      cache,\n      // could allow any formatting of fields returned\n      ...rest,\n    },\n  });\n\n  return fetchClient(\n    apiUrl,\n    {},\n    FIND_ERROR_FALLBACK_MESSAGE,\n    { pageActionName },\n  );\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/packages/ux-tread-address-locate/src/api/retrieve-address.js","/*\n  Our vendor Loquate will, in instances where multiple businesses are registered\n  at the same address, return multiple identical address objects (with different id's).\n  Loquate is unable to dedupe the results due to contract restrictions with USPS\n  and so we are forced to do that here.\n*/\nexport default function normalizeFindAddressResponse(responseData) {\n  const exists = {};\n  const response = {\n    Items: responseData.Items.filter((item) => {\n      if (item.Type !== 'Address') {\n        return true;\n      }\n      // item.Text corresponds to street address, item.Description corresponds\n      // to city / postal code. Use both to determine address uniqueness.\n      const itemId = `${item.Text}${item.Description}`;\n      if (!exists[itemId]) {\n        exists[itemId] = true;\n        return true;\n      }\n      return false;\n    }),\n  };\n  return response;\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/packages/address-typeahead/src/normalizers/findAddressNormalizer.js","/* eslint-disable max-len, no-param-reassign */\nimport { COUNTRY_FORM_MAP } from '../constants';\n\nexport default function normalizeRetrieveAddressResponse(responseData) {\n  const formMap = {\n    default: {\n      address1: 'Line1',\n      address2: 'Line2',\n      city: 'City',\n      postalCode: 'PostalCode',\n      state: 'Province',\n    },\n    type2: {\n      address1: 'Line1',\n      address2: 'Line2',\n      city: 'City',\n      postalCode: 'PostalCode',\n    },\n  };\n\n  const returnedAddressData = responseData.Items.shift();\n  const country = returnedAddressData.CountryIso2;\n  const formKey = Object.keys(COUNTRY_FORM_MAP).find(key => COUNTRY_FORM_MAP[key].includes(country));\n  const keyMap = formMap[formKey] || formMap.default;\n\n  return Object.keys(keyMap).reduce((normalized, key) => {\n    normalized[key] = returnedAddressData[keyMap[key]];\n    return normalized;\n  }, {});\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/packages/address-typeahead/src/normalizers/retrieveAddressNormalizer.js","import React from 'react';\nimport PropTypes from 'prop-types';\n\nexport default function SuggestionItemComponent(props) {\n  const {\n    suggestion,\n    onBlur,\n    onClick,\n  } = props;\n\n  return (\n    <li\n      className=\"border-top-medium-grey pt2-sm pb2-sm pl2-sm\"\n      key={suggestion.Id}\n    >\n      <button\n        onBlur={onBlur}\n        onClick={onClick}\n        value={suggestion.Id}\n        data-suggestion-type={suggestion.Type}\n        className=\"ncss-btn d-sm-b pt1-sm pb1-sm\"\n      >\n        {suggestion.Type !== 'Address' ? <i className=\"fl-sm-r g72-arrow-thick-right text-color-grey\" /> : ''}\n        {suggestion.Text}\n        <span className=\"text-color-grey\"> {suggestion.Description}</span>\n      </button>\n    </li>\n  );\n}\n\nSuggestionItemComponent.propTypes = {\n  suggestion: PropTypes.shape({\n    Id: PropTypes.string,\n    Type: PropTypes.string,\n    Text: PropTypes.string,\n    Description: PropTypes.string,\n  }),\n  onBlur: PropTypes.func,\n  onClick: PropTypes.func,\n};\n\n\n\n// WEBPACK FOOTER //\n// ./src/packages/address-typeahead/src/components/SuggestionItem.js","import React from 'react';\nimport { CSSTransition, TransitionGroup } from 'react-transition-group';\nimport PropTypes from 'prop-types';\nimport SuggestionItem from './SuggestionItem';\nimport SafeAnchor from '../../../../checkout/view/components/SafeAnchor';\n\nexport default function SuggestionListComponent(props) {\n 