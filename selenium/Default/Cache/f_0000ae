' : 'd-sm-h'}`;\n  return (\n    <section className={sectionClasses}>\n      <div className=\"ncss-col-sm-12 ncss-col-md-10 va-sm-t mb4-sm mb0-md\">\n        <div className=\"ncss-row\">\n          <FieldWrapper\n            id={'promoCode'}\n            autoComplete={'promo-code'}\n            label={formatMessage(messages.promoCodeInputLabel)}\n            type=\"input\"\n            validate={inlineValidator('promoCode')}\n            wrapperStyle=\"ncss-col-sm-12 ncss-input-container va-sm-t\"\n            tooltipEnabled\n            tooltipContent={[{ type: 'message', text: formatMessage(messages.promoCodeTooltip), style: 'tip-left' }]}\n            tooltipId={'promoCodeTooltip'}\n            showIcon\n            trackTooltip={() => { dispatch({ type: TOGGLE_TOOLTIP, payload: 'promocode' }); }}\n          />\n        </div>\n      </div>\n      <div className=\"ncss-col-sm-12 ncss-col-md-2 mt0-sm mt6-md va-sm-t\">\n        <button\n          onClick={\n            () => addPromotionCode(dispatch, promotionCode)\n          }\n          className=\"qa-promocode mod-ncss-btn ncss-btn-black ncss-brand\n            pt3-sm prl5-sm pb3-sm pt2-lg pb2-lg u-uppercase u-full-width u-rounded\"\n          disabled={!promotionCode}\n        >\n          {formatMessage(messages.applyButton)}\n        </button>\n      </div>\n    </section>\n  );\n}\n\nPromoCodeForm.propTypes = {\n  dispatch: PropTypes.func,\n  prepaidFormVisible: PropTypes.bool,\n  promotionCode: PropTypes.string,\n};\n\nexport default compose(\n  reduxForm({ form: 'promoCodeForm', initialValues: {} })\n)(PromoCodeForm);\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/payment/PromoCodeForm.js","/* eslint no-unused-expressions: 0 */\nimport React from 'react';\nimport PropTypes from 'prop-types';\nimport PrepaidList from './PrepaidCardList';\nimport PrepaidCardFormComponent from './PrepaidCardForm';\nimport PromoCodeFormComponent from './PromoCodeForm';\nimport CheckboxToggler from '../Checkbox