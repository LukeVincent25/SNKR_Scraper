\n  AddressForm and BillingForm based on the Country that has been set in the state.\n  The default currently is the US. */\n  let AddressForm;\n  let BillingForm;\n\n  const euType1 = EU_FORM_TYPE_ONE.test(country);\n  const euType2 = EU_FORM_TYPE_TWO.test(country);\n  const euType3 = EU_FORM_TYPE_THREE.test(country);\n\n  if (country === 'us') {\n    AddressForm = isApoFpo ? MilitaryForm : AddressFormAVS;\n    BillingForm = USBillingForm;\n  } else if (country === 'cn') {\n    AddressForm = CNAddressForm;\n    BillingForm = USBillingForm;\n  } else if (euType1 || euType2 || euType3) {\n    if (euType1) {\n      AddressForm =\n        (inExperimentAB4114 || inExperimentAB4114b) && !initialValues.address.postalCode ?\n          AddressFormAB4114Type1 : euType1AddressForm;\n      BillingForm = euType1BillingForm;\n    }\n    if (euType2 || euType3) {\n      AddressForm =\n        (inExperimentAB4114 || inExperimentAB4114b) && !initialValues.address.postalCode ?\n          AddressFormAB4114Type2 : euType2AddressForm;\n      BillingForm = euType2BillingForm;\n    }\n  } else if (country === 'jp') {\n    AddressForm = JPAddressForm;\n    BillingForm = JPBillingForm;\n  } else {\n    AddressForm = AddressFormAVS;\n    BillingForm = USBillingForm;\n  }\n\n  let form;\n  if (shippingAddress) {\n    form = (\n      <AddressForm\n        {... addressSuggestionProps}\n        address2Open={address2Open}\n        allowNickName={props.allowNickName}\n        animateInputLabels={props.animateInputLabels}\n        country={country.toLowerCase()}\n        currentValues={props.currentValues}\n        disabled={props.disabled}\n        dispatch={props.dispatch}\n        emailInUse={props.emailInUse}\n        emailWarn={props.emailWarn}\n        formName={formName}\n        handleLoginClick={props.handleLoginClick}\n        inExperimentAB4114b={props.inExperimentAB4114b}\n        inExperimentAB4114={props.inExperimentAB4114}\n        initialValues={props.initialValues}\n        isGue