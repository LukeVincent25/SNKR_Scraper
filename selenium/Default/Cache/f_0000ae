        </div>\n\n            </div>\n          </div>\n        </div>\n        <CurrentModes />\n      </div>\n    );\n  }\n}\n\nexport function asyncActions(dispatch) {\n  return function dispatchMetaTags() {\n    dispatch(\n      updatePageTitle('Tunnel')\n    );\n  };\n}\n\nexport function mapStateToProps(state, ownProps) {\n  const cartHref = getLocalizedCartHref(state);\n  const country = getCountry();\n  const currency = getCurrency();\n  const cartItems = getItems(state);\n  const isMobile = get(state, 'app.ui.layout') === 'MOBILE';\n  const isMobileReferrer = getIsMobileReferrer();\n  const isPaypalNavigating = getIsPaypalNavigating(state);\n  const isPaypalPayment = getIsPaypalUser(state);\n  const locale = getLocale();\n  const pathName = get(ownProps, 'location.pathname', '/checkout'); // react-redux-router puts this here\n  const uniteInitialized = getIsUniteInitialized(state);\n  const isMemberLoading = getIsMemberLoading(state);\n  return {\n    cartHref,\n    country,\n    currency,\n    cartItems,\n    isPaypalPayment,\n    isPaypalNavigating,\n    isMemberLoading,\n    isMobile,\n    isMobileReferrer,\n    locale,\n    pathName,\n    uniteInitialized,\n    isCN: getIsCN(state),\n  };\n}\n\nconst mapDispatchToProps = {\n  setLocale: payload => ({ type: SET_LOCALE_DATA, payload }),\n  setLayout: payload => ({\n    type: SET_LAYOUT,\n    payload,\n  }),\n  setCurrency: payload => ({\n    type: SET_CTRY_CURRENCY,\n    payload,\n  }),\n  initializeUnite,\n  fetchNewUserProfile,\n  handleLoginSuccess,\n  getLoginForm,\n  fetchPaymentOptions,\n  postPaypalExpress,\n  sendToCheckout: replace,\n  continueAsGuest: payload => ({\n    type: CONTINUE_AS_GUEST,\n    payload,\n  }),\n  dispatchContinueToPaypal: () => ({\n    type: CONTINUE_TO_PAYPAL,\n  }),\n  setPaypalNavigating: payload => ({\n    type: SET_PAYPAL_NAVIGATING,\n    payload,\n  }),\n  getAvailableCartItemsAndPromotions,\n};\n\nTunnelRouteComponent.propTypes = {\n  authToken: PropTypes.string,\n  