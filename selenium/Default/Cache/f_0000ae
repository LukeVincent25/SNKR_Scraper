nr-sole';\nimport FieldWrapper from '../FieldWrapper';\nimport { getCountry } from '../../../helpers/getGlobals';\nimport { createInlineValidator } from '../../../helpers/validation/index';\nimport { FAPIAO_TYPE_INDIVIDUAL, FAPIAO_TYPE_CORPORATION } from '../../../constants/payment';\nimport { Radio, CheckBox } from '../form/ReduxFormInput';\n\nconst inlineValidator = createInlineValidator({\n  type: 'invoiceInfo', country: getCountry().toLowerCase(),\n});\n\nclass InvoiceInfoForm extends React.PureComponent {\n  constructor(props) {\n    super(props);\n\n    this.individualNameRef = null;\n    this.corporationNameRef = null;\n\n    this.individualNameValidator = (value, allValues) => {\n      const { fapiaoType } = allValues;\n      if (fapiaoType === FAPIAO_TYPE_INDIVIDUAL) {\n        return inlineValidator('individualName')(value, allValues);\n      }\n      return null;\n    };\n\n    this.corporationNameValidator = (value, allValues) => {\n      const { fapiaoType } = allValues;\n      if (fapiaoType === FAPIAO_TYPE_CORPORATION) {\n        return inlineValidator('corporationName')(value, allValues);\n      }\n      return null;\n    };\n\n    this.taxIdValidator = (value, allValues) => {\n      const { fapiaoType } = allValues;\n      if (fapiaoType === FAPIAO_TYPE_CORPORATION) {\n        return inlineValidator('taxId')(value, allValues);\n      }\n      return null;\n    };\n  }\n\n  componentDidUpdate(prevProps) {\n    if (this.props.withFapiao) {\n      let inputToGainFocus = null;\n      if (!prevProps.withFapiao || this.props.fapiaoType !== prevProps.fapiaoType) {\n        inputToGainFocus = this.props.fapiaoType === FAPIAO_TYPE_INDIVIDUAL ?\n          this.individualNameRef : this.corporationNameRef;\n      }\n\n      if (inputToGainFocus) {\n        inputToGainFocus.focus();\n      }\n    }\n  }\n\n  render() {\n    const {\n      withFapiao,\n      fapiaoType,\n      messageFapiao,\n      messageTaxId,\n      messageIndividual,\n      messageIndividualN