) {\n    loadIovation();\n  }\n\n  /**\n   * PLEASE REFACTOR ME\n   * This function takes a string (type) and does different submits off type\n   * @param {string} type - string of type of checkout\n   * @param {string} position - which button was clicked\n   */\n  submitOrder(type, position) {\n    const { dispatch } = this.props;\n    if (type === 'paypal') { // $SR\n      return dispatch(postPaypalMark(position));\n    } else if (type === 'applePay') {\n      return dispatch(startFlow()).then(() => dispatch(placeOrder(position))).catch(() => { });\n    }\n    return dispatch(placeOrder(position));\n  }\n\n  render() {\n    const {\n      dispatch,\n      fixCtaButton,\n      isApplePay,\n      layout,\n      isEnglishInJapan,\n      thirdPartyPayment,\n      totalsCoveredByPrepaid,\n    } = this.props;\n    let placeOrderButton;\n    let placeOrderButtonMobile;\n    const buttonClasses = classnames(\n      'd-lg-ib',\n      'd-sm-h',\n      'fs14-sm',\n      'ncss-brand',\n      'ncss-btn-accent',\n      'pb2-lg',\n      'pb3-sm',\n      'prl5-sm',\n      'pt2-lg',\n      'pt3-sm',\n      'u-uppercase',\n    );\n    const isMobile = layout === 'MOBILE'; // $SR\n    const mobileCtaClasses = `sticky-place-order d-lg-h ${fixCtaButton ? 'fix pt4-sm' : ''}`;\n    const termsClasses = `\n      sticky-terms\n      ${fixCtaButton && isMobile ? 'fix border-top-light-grey ' : ' '}`;\n    let mobileButtonClasses = [\n      'fs14-sm',\n      'ncss-brand',\n      'ncss-btn-accent',\n      'pb2-lg',\n      'pb3-sm',\n      'prl5-sm',\n      'pt2-lg',\n      'pt3-sm',\n      'u-full-width',\n      'u-uppercase'];\n    // lame don't want to have to rewrite all the tests to get this to work correctly\n    // really just passes a string into the submit to loop through and try to submit correctly\n    const creditCardOrApplePay = isApplePay ? 'applePay' : 'creditCard';\n\n    if (thirdPartyPayment === 'paypal' && !totalsCoveredByPrepaid) { // $SR\n      // TODO: CCC-176 need to put