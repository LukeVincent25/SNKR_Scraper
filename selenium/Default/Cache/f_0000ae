n          animateInputLabels\n          showPositiveValidation\n          validationMarker={validationMarker}\n        />\n      }\n      <FieldWrapper\n        wrapperStyle=\"ncss-col-md-6 ncss-col-sm-12 mod-ncss-input-container mb2-sm mb4-md va-sm-t\"\n        label={formatMessage(messages.firstName)}\n        id={'firstName'}\n        type={'input'}\n        validate={inlineValidator('firstName')}\n        disabled={disabled}\n        animateInputLabels\n        autoComplete={'given-name'}\n        showPositiveValidation\n        validationMarker={validationMarker}\n      />\n      <FieldWrapper\n        wrapperStyle=\"ncss-col-md-6 ncss-col-sm-12 mod-ncss-input-container mb2-sm mb4-md va-sm-t\"\n        label={formatMessage(messages.lastName)}\n        id={'lastName'}\n        type={'input'}\n        validate={inlineValidator('lastName')}\n        disabled={disabled}\n        animateInputLabels\n        autoComplete={'family-name'}\n        showPositiveValidation\n        validationMarker={validationMarker}\n      />\n      <FieldWrapper\n        wrapperStyle=\"mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t\"\n        label={formatMessage(messages.address)}\n        id={'address1'}\n        type={'input'}\n        validate={inlineValidator('address1')}\n        disabled={disabled}\n        animateInputLabels\n        autoComplete={'street-address'}\n        showPositiveValidation\n        validationMarker={validationMarker}\n      />\n      {layout === 'DESKTOP' ?\n        <InputAddressLine2Component\n          initialStateOpen={address2Open}\n        />\n        :\n        <FieldWrapper\n          wrapperStyle=\"mod-ncss-input-container ncss-col-sm-12 mb2-sm mb4-md va-sm-t\"\n          id={'address2'}\n          type={'input'}\n          validate={inlineValidator('address2')}\n          disabled={disabled}\n          animateInputLabels\n          showPositiveValidation\n          validationMarker={validationMarker}\n        />\n      }\n      <F