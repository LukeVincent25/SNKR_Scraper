Types.shape({}),\n  paymentToDelete: PropTypes.string,\n  paymentToken: PropTypes.string,\n  pendingPaymentOptions: PropTypes.shape({\n    items: PropTypes.shape({}),\n  }),\n  pinNumber: PropTypes.string,\n  placeOrder: PropTypes.func,\n  prepaidCardNumber: PropTypes.string,\n  prepaidData: PropTypes.shape({}),\n  prepaidFormVisible: PropTypes.bool,\n  prepaidMethods: PropTypes.shape({}),\n  promoData: PropTypes.shape({}),\n  promotionCode: PropTypes.string,\n  promotionCodes: PropTypes.shape({}),\n  recipient: PropTypes.shape({\n    firstName: PropTypes.string,\n    lastName: PropTypes.string,\n  }),\n  sameAsShipping: PropTypes.bool,\n  savePayment: PropTypes.bool,\n  selectedBank: PropTypes.string,\n  selectedKonbiniStore: PropTypes.string,\n  selectedPaymentBillingAddress: PropTypes.shape({}),\n  selectedPaymentId: PropTypes.string,\n  selectedPaymentMethod: PropTypes.string,\n  setInvoiceInfo: PropTypes.func,\n  setUsingWechatBrowser: PropTypes.func,\n  shoppingCountry: PropTypes.string,\n  shouldContinueToOrderReview: PropTypes.bool,\n  shouldFetchKlarnaToken: PropTypes.bool,\n  shouldHandleUseCard: PropTypes.bool,\n  shouldShowContactForm: PropTypes.bool,\n  showBillingAddressPreview: PropTypes.bool,\n  showBillingCountryForm: PropTypes.bool,\n  showBillingCountryModal: PropTypes.bool,\n  showBillingSameAsShippingToggle: PropTypes.bool,\n  showChangeBillingCountryView: PropTypes.bool,\n  showContinueToOrderReview: PropTypes.bool,\n  showIdealFieldErrors: PropTypes.bool,\n  showModal: PropTypes.bool,\n  showNewCard: PropTypes.bool,\n  showNewCardButtons: PropTypes.bool,\n  showPaymentList: PropTypes.bool,\n  showPaymentPreview: PropTypes.bool,\n  storedShippingAddresses: PropTypes.arrayOf(PropTypes.shape({})),\n  thirdPartyPayment: PropTypes.string,\n  thirdPartyPaymentBillingAddress: PropTypes.shape({}),\n  totals: PropTypes.shape({}),\n  totalsAfterPrepaid: PropTypes.number,\n  totalsCoveredByPrepaid: PropTypes.bool,\n  validMemberCC: PropTypes.bool,\n  wasApplePayDefaulted: PropTypes.bool,\n  isEU: PropTypes.bool,\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(PaymentComponent);\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/containers/main/Payment.js","import React from 'react';\nimport PropTypes from 'prop-types';\nimport classnames from 'classnames';\nimport { connect } from 'react-redux';\nimport { touch, isValid } from 'redux-form';\nimport get from 'lodash.get';\nimport { v4 } from 'uuid';\nimport defineMessages from '../../../helpers/defineMessages';\nimport formatMessage from '../../../helpers/formatMessage';\nimport { authorizeKlarna } from '../../../helpers/klarna';\nimport { postMessageToChildFrame } from '../../../helpers/paymentUtils';\nimport { getDuplicateStoredId } from '../../../helpers/storedPayments';\nimport handleUseCard from '../../../helpers/handleUseCard';\nimport {\n  getAddress,\n  getCancelUrl,\n  getCanCheckoutWithBillingAddress,\n  getCanSubmitCCOrder,\n  getCanSubmitCOD,\n  getCanSubmitGiftCardOrder,\n  getCanSubmitIdeal,\n  getCanSubmitKlarnaOrder,\n  getCanSubmitPaypal,\n  getCanSubmitSofort,\n  getConfirmationUrl,\n  getDisplayBillingForm,\n  getEnablePaynowButton,\n  getHasValidContactInfoIfRequired,\n  getIsApplePaySelected,\n  getIsAtMaxCards,\n  getIsBillingDataInvalid,\n  getIsCreditCardValid,\n  getIsCreditDebitSelected,\n  getIsGuestUser,\n  getIsKlarnaBillingSameAsShipping,\n  getIsPaymentReadyToPost,\n  getIsPayPalExpress,\n  getIsPayPalSelected,\n  getIsStoredPaymentSelected,\n  getIsTotalsCoveredByPrepaid,\n  getKlarnaChecked,\n  getKlarnaShippingAddress,\n  getSelectedBank,\n  getSelectedPaymentBillingAddress,\n  getShippingBillingCountryDiffer,\n  getShowNewCard,\n  getThirdPartyPayment,\n  getTotalsAfterPrepaid,\n  getValidMemberCC,\n  getContainsOnlyDigitalGiftCard,\n} from '../../../selectors';\nimport postPaymentPreview from '../../../state/app/actions/postPaymentPreview';\nimport submitCheckout from '../../../state/app/actions/submitCheckout';\nimport { startFlow } from '../../../state/applepay';\nimport { renderCopy } from './OrderReview';\nimport {\n  IFRAME_MSG,\n  IFRAME_DOMAIN,\n  IFRAME_ELEMENT,\n} from './Payment';\n\nimport {\n  ADD_NEW_CARD,\n  CANCEL_NEW_CARD,\n  convertPaypalExpressToPaypalMark,\n  fetchPaymentOptions,\n  getIsClickAndCollectSelected,\n  klarnaPaymentPreview,\n  placeOrder,\n  postPaypalMark,\n  SET_CREDIT_CARD_VALID,\n  SET_ERRORS,\n  SET_GUID,\n  SET_ORDER_PROCESSING,\n  SET_PAYMENT_PROCESSING,\n  SET_SELECTED_PAYMENT_METHOD,\n  TOGGLE_PAYMENT_OPTIONS,\n  UNEDIT_PAYMENT_SECTION,\n  updateKlarnaSession,\n  updateStoredPayment,\n} from '../../../state/app';\nimport {\n  SET_BILLING_ADDRESS,\n  SET_PAYMENT_METHOD,\n  SET_PAYMENT_TOKEN,\n  SET_VALIDATE_PAYMENT_CVV,\n} from '../../../state/checkout';\nimport {\n  UPDATE_PAYMENT_METHOD,\n} from '../../../state/userProfile';\nimport {\n  COD,\n  CREDIT_DEBIT,\n  IDEAL,\n  KLARNA,\n  PAYPAL,\n  SOFORT,\n} from '../../../constants/payment';\n\n\nconst messages = defineMessages({\n  placeOrderBtn: {\n    id: 'checkout.section.placeorder.buttonText',\n    description: 'Place order button',\n    defaultMessage: 'Place Order',\n  },\n  continueToPayPal: {\n    id: 'checkout.section.continueToPaypal.buttonText',\n    description: 'Continue With Paypal Button Text',\n    defaultMessage: 'CONTINUE WITH PAYPAL',\n  },\n});\n\n\nconst placeOrderBtnClasses = classnames(\n  'd-lg-ib',\n  'fs14-sm',\n  'ncss-brand',\n  'ncss-btn-accent',\n  'pb2-lg',\n  'pb3-sm',\n  'prl5-sm',\n  'pt2-lg',\n  'pt3-sm',\n  'u-uppercase',\n);\n\nconst placeOrderBtnWrapperClasses = classnames(\n  'ncss-col-sm-12',\n  'pt5-sm',\n  'pb5-sm',\n  'va-sm-t',\n  'ta-sm-r',\n  'test-desktop-button',\n);\n\nconst placeOrderWrapperClasses = classnames(\n  'ncss-container',\n  'border-light-grey',\n  'bg-white',\n);\n\nconst placeOrderCopyClasses = classnames(\n  'fs12-sm',\n  'lh12-sm',\n  'ta-lg-c',\n  'text-color-grey',\n);\nexport class PlaceOrderAB3576 extends React.Co