 en-gb\n  const agreementLang = language && language.substring(0, 2);\n  // The pattern bellow uses agreement service API to generate the url\n  // For more information about agreement service visit\n  // https://confluence.nike.com/display/PET/What+the+Agreement+Service+is+and+how+to+use+it\n  // example link: http://agreementservice.svs.nike.com/rest/agreement?agreementType=termsOfUse&uxId=com.nike.commerce.nikedotcom.web&country=US&language=en&requestType=redirect\n  let url = [\n    `${BASE_PATH_URL_AGREEMENT_SERVICE}`,\n    `?agreementType=${type}`,\n    `&uxId=${UXID_URL}`,\n    `&country=${country}`,\n    `&language=${agreementLang}`,\n    '&requestType=redirect',\n  ].join('');\n  if (type === AGREEMENT_TYPE_RETURN_POLICY) {\n    url = buildReturnPolicyLink(country, language);\n  }\n  if (type === AGREEMENT_TYPE_PROGRAM_TERMS) {\n    url = [\n      `${BASE_PATH_URL_AGREEMENT_SERVICE}`,\n      `?agreementType=${type}`,\n      `&uxId=${SNKRS_UXID}`,\n      `&country=${country}`,\n      `&language=${agreementLang}`,\n      '&requestType=redirect',\n    ].join('');\n  }\n  return (\n    <a\n      href={url}\n      className={classNames}\n      target=\"_blank\"\n      rel=\"noopener noreferrer\"\n      key={linkKey}\n      onClick={() => {\n        if (type === AGREEMENT_TYPE_TERMS_OF_USE) {\n          dispatch({\n            type: VIEW_TERMS_OF_USE,\n            payload: {},\n          });\n        }\n      }}\n    >{str}\n    </a>\n  );\n}\n\nfunction isEU() {\n  return getMerchGroup(getCountry()) === 'EU';\n}\n\nfunction isJP() {\n  return getMerchGroup(getCountry()) === 'JP';\n}\n\nexport function renderCopy(dispatch, isEnglishInJapan) {\n  const classNames = 'u-underline text-color-grey';\n  let copy;\n  if (isEU()) {\n    copy = messages.orderReviewNoCheckEUCopy;\n  } else if (isJP()) {\n    copy = messages.orderReviewJPCopy;\n  } else {\n    copy = messages.orderReviewCopy;\n  }\n\n  if (isEnglishInJapan) {\n    const tos = formatMessage(messages.termsOfS