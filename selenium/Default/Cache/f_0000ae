';\nimport get from 'lodash.get';\nimport CheckboxToggler from '../CheckboxToggler';\nimport defineMessages from '../../../helpers/defineMessages';\nimport formatMessage from '../../../helpers/formatMessage';\nimport { destroyUniteForms } from '../../../api/unite';\nimport Loading from '../Loading';\nimport {\n  getGuestRegistrationForm,\n  REGISTER_USER,\n} from '../../../state/userProfile';\nimport {\n  TOGGLE_GUEST_REGISTER_CHECKBOX,\n  CLICK_ALREADY_A_MEMBER,\n  GUEST_REGISTER_PROCESSING,\n  SET_ERRORS,\n  SET_SHIPPING_PROCESSING,\n  deleteAllPaymentMethods,\n  UNEDIT_SHIPPING_SECTION,\n} from '../../../state/app';\nimport {\n  COUNTRY_CHINA,\n} from '../../../constants/constants';\nimport {\n  getCountry,\n} from '../../../helpers/getGlobals';\n\nconst messages = defineMessages({\n  guestCheckboxLabel: {\n    id: 'checkout.guest.checkbox.label',\n    description: 'label of guest registration checkbox',\n    defaultMessage: 'Become a NikePlus member for 60-day free returns',\n  },\n  euGuestCheckboxLabel: {\n    id: 'EU.checkout.guest.checkbox.label',\n    description: 'label of guest registration checkbox',\n    defaultMessage: 'Become a NikePlus member for free to receive exclusive benefits.',\n  },\n});\n\nexport class GuestRegistrationComponent extends Component {\n  constructor(props) {\n    super(props);\n    this.dispatchToggleGuestRegister = this.dispatchToggleGuestRegister.bind(this);\n    this.dispatchClickAlreadyAMember = this.dispatchClickAlreadyAMember.bind(this);\n    this.dispatchEmailSignupCheckbox = this.dispatchEmailSignupCheckbox.bind(this);\n    this.dispatchFetchUserProfile = this.dispatchFetchUserProfile.bind(this);\n    this.dispatchRegisterUser = this.dispatchRegisterUser.bind(this);\n    this.handleUniteFailure = this.handleUniteFailure.bind(this);\n    this.setFormFields = this.setFormFields.bind(this);\n    this.alreadyAMemberLink = null;\n    this.successHandler = null;\n    this.failHandler = null;\n  }\n\n  componentDidMount() {\n    // If shown behind a shipping method radio then it will not mount till the method is selected\n    // If shown behind the CheckboxToggler it will mount immediately but will not show\n    // the Unite form till the checkbox is toggled on\n    if (this.props.isShippingMethod) {\n      this.props.dispatch(getGuestRegistrationForm());\n    }\n    if (window && window.nike) {\n      this.alreadyAMemberLink = window.nike.unite.on.join.alreadyAMemberLink.addHandler(\n        this.dispatchClickAlreadyAMember\n      );\n      this.successHandler = window.nike.unite.on.join.success.addHandler(\n        this.dispatchRegisterUser\n      );\n      this.failHandler = window.nike.unite.on.join.fail.addHandler(\n        this.handleUniteFailure\n      );\n    }\n    this.setFormFields();\n  }\n\n  // TODO https://jira.nike.com/browse/CCC-5034\n  // eslint-disable-next-line camelcase\n  UNSAFE_componentWillReceiveProps() {\n    this.setFormFields();\n  }\n\n  componentWillUnmount() {\n    destroyUniteForms();\n    if (window && window.nike) {\n      window.nike.unite.on.join.alreadyAMemberLink.removeHandler(this.alreadyAMemberLink);\n      window.nike.unite.on.join.fail.removeHandler(this.failHandler);\n      window.nike.unite.on.join.success.removeHandler(this.successHandler);\n    }\n  }\n\n  setFormFields() {\n    const {\n      registrationFormValues = {},\n    } = this.props;\n\n    const {\n      firstName,\n      lastName,\n      email,\n    } = (registrationFormValues);\n\n    const data = {\n      firstName: {\n        dataField: 'firstName',\n        value: firstName,\n      },\n      lastName: {\n        dataField: 'lastName',\n        value: lastName,\n      },\n      emailAddress: {\n        dataField: 'emailAddress',\n        value: email,\n      },\n    };\n\n    if (getCountry() === COUNTRY_CHINA) {\n      data.mobileJoinForm = { hiddenFields: { firstName: `${firstName}`, lastName: `${lastName}` } };\n    }\n    if (window && get(window, 'nike.unite.views.join.rendered')) {\n      const formData = window.nike.unite.views.join.getFormData();\n      if (!get(formData, 'emailAddress.value')) {\n        window.nike.unite.views.join.render(data);\n      }\n    }\n  }\n\n  dispatchClickAlreadyAMember() {\n    this.props.dispatch({ type: CLICK_ALREADY_A_MEMBER });\n  }\n\n  dispatchEmailSignupCheckbox() {\n    this.props.dispatch(null);\n  }\n\n  dispatchToggleGuestRegister(checked) {\n    if (checked) {\n      this.props.dispatch(getGuestRegistrationForm());\n      this.setFormFields();\n    } else {\n      destroyUniteForms();\n    }\n    this.props.dispatch({ type: TOGGLE_GUEST_REGISTER_CHECKBOX, payload: checked });\n  }\n\n  dispatchRegisterUser() {\n    const {\n      dispatch,\n      handleContinueToPayment,\n    } = this.props;\n    dispatch({\n      type: REGISTER_USER,\n    });\n    dispatch({\n      type: GUEST_REGISTER_PROCESSING,\n      payload: false,\n    });\n    this.dispatchFetchUserProfile();\n    dispatch({ type: TOGGLE_GUEST_REGISTER_CHECKBOX, payload: false });\n    handleContinueToPayment();\n  }\n\n  dispatchFetchUserProfile() {\n    this.props.dispatch({\n      type: SET_SHIPPING_PROCESSING,\n      payload: true,\n    });\n    this.props.dispatch(deleteAllPaymentMethods());\n    this.props.dispatch({\n      type: UNEDIT_SHIPPING_SECTION,\n    });\n  }\n\n  handleUniteFailure() {\n    this.props.dispatch({ type: TOGGLE_GUEST_REGISTER_CHECKBOX, payload: false });\n    this.props.handleContinueToPayment();\n    this.props.dispatch({\n      error: true,\n      type: SET_ERRORS,\n      payload: [{\n        crcCode: '73AEDBA6',\n      }],\n    });\n    this.props.dispatch({\n      type: GUEST_REGISTER_PROCESSING,\n      payload: false,\n    });\n  }\n\n  render() {\n    const { isShippingMethod, isEU, isGuestRegistrationProcessing } = this.props;\n    const checkboxLabel = isEU ? messages.euGuestCheckboxLabel : messages.guestCheckboxLabel;\n    const visibility = isGuestRegistrationProcessing ? 'hidden' : '';\n    const uniteForm = (\n      <div className={'ncss-col-sm-12 mt2-sm mb5-sm prl4-sm'}>\n        { isGuestRegistrationProcessing && <Loading /> }\n        <div className={`ncss-row ${visibility}`}>\n          <div\n            id=\"nike-unite-join-view\"\n            className=\"nike-unite language_en theme_checkout ncss-col-sm-12\"\n            data-view=\"join\"\n          />\n          <div\n            id=\"nike-unite-login-view\"\n            className=\"nike-unite language_en theme_checkout ncss-col-sm-12 ncss-col-md-6\"\n            data-view=\"login\"\n          />\n          <div\n            id=\"nike-unite-reset-password-view\"\n            className=\"nike-unite language_en theme_checkout ncss-col-sm-12 ncss-col-md-6\"\n            data-view=\"reset\"\n          />\n          <div\n            id=\"nike-unite-confirm-password-reset-view\"\n            className=\"nike-unite language_en theme_checkout ncss-col-sm-12 ncss-col-md-6\"\n            data-view=\"resetConfirm\"\n          />\n          <div\n            id=\"nike-unite-progressive-profile-view\"\n            className=\"nike-unite language_en theme_checkout ncss-col-sm-12 ncss-col-md-6\"\n            data-view=\"progressive\"\n          />\n        </div>\n      </div>\n    );\n    if (isShippingMethod) {\n      return uniteForm;\n    }\n    return (\n      <div className={'ncss-row'}>\n        <CheckboxToggler\n          name={'membership'}\n          label={formatMessage(checkboxLabel)}\n          labelStyle={'ncss-label ncss-brand u-uppercase pl7-sm fs16-sm'}\n          id={'membership'}\n          displayContentOnChecked\n          onSelected={this.dispatchToggleGuestRegister}\n          toolTip={this.props.toolTip}\n        >\n          { uniteForm }\n        </CheckboxToggler>\n      </div>\n    );\n  }\n}\n\nGuestRegistrationComponent.propTypes = {\n  dispatch: PropTypes.func,\n  isShippingMethod: PropTypes.bool,\n  toolTip: PropTypes.shape({}),\n  handleContinueToPayment: PropTypes.func,\n  