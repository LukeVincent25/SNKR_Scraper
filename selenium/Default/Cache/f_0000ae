llettwo',\n    description: 'Guest tooltip bulletpoint two',\n    defaultMessage: 'Priority access to our most coveted products',\n  },\n  shipLabel: {\n    id: 'checkout.delivery.ship',\n    description: 'Label for Ship section',\n    defaultMessage: 'SHIP',\n  },\n  pickupLabel: {\n    id: 'checkout.delivery.pickup',\n    description: 'Label for Pickup section',\n    defaultMessage: 'PICK UP',\n  },\n});\n\nexport class ShippingComponent extends Component {\n  constructor(props, context) {\n    super(props, context);\n    this.setStoredAddressView = this.setStoredAddressView.bind(this);\n    this.setPreviouslySelectedAddress = this.setPreviouslySelectedAddress.bind(this);\n    this.handleApplyShippingAddress = this.handleApplyShippingAddress.bind(this);\n    this.handleShippingContinueClicks = this.handleShippingContinueClicks.bind(this);\n    this.handleApplyAndSaveShippingAddress = this.handleApplyAndSaveShippingAddress.bind(this);\n    this.handleSelectedShippingOption = this.handleSelectedShippingOption.bind(this);\n    this.handleContinueToPayment = this.handleContinueToPayment.bind(this);\n    this.handleContinueToReview = this.handleContinueToReview.bind(this);\n    this.handleApplyStoredShippingAddress = this.handleApplyStoredShippingAddress.bind(this);\n    this.handleEditAddress = this.handleEditAddress.bind(this);\n    this.handleAddressValidationSelection = this.handleAddressValidationSelection.bind(this);\n    this.cancelEdit = this.cancelEdit.bind(this);\n    this.clearForm = this.clearForm.bind(this);\n    this.toggleShippingOptions = this.toggleShippingOptions.bind(this);\n    this.handleApoFpoSubmit = this.handleApoFpoSubmit.bind(this);\n    this.handleApplyShippingAddressGuest = this.handleApplyShippingAddressGuest.bind(this);\n    this.handleClickAndCollect = this.handleClickAndCollect.bind(this);\n    this.handleClickAndCollectEdit = this.handleClickAndCollectEdit.bind(this);\n    this.handlePickupRecipientSubmit =\n      this.handlePickupRecipientSubmit.bind(this);\n    this.handleLoginClick = this.handleLoginClick.bind(this);\n    this.handleForgotPasswordResetRender = this.handleForgotPasswordResetRender.bind(this);\n    this.handleSuccessfulPasswordReset = this.handleSuccessfulPasswordReset.bind(this);\n    this.emailWarn = this.emailWarn.bind(this);\n    this.toggleIsScheduledDelivery = this.toggleIsScheduledDelivery.bind(this);\n    this.handleScheduledDeliveryDateChange = this.handleScheduledDeliveryDateChange.bind(this);\n    this.handleScheduledDeliveryTimeChange = this.handleScheduledDeliveryTimeChange.bind(this);\n    this.handleIsGiftReceipt = this.handleIsGiftReceipt.bind(this);\n    this.handleAddressExtendedDisplay =\n      this.handleAddressExtendedDisplay.bind(this);\n    this.resetForm = this.resetForm.bind(this);\n    this.handleDeliveryTypeToggle = this.handleDeliveryTypeToggle.bind(this);\n\n    this.state = {\n      resetPasswordViewKey: null,\n      resetPasswordSuccessKey: null,\n    };\n  }\n\n  componentDidMount() {\n    const { dispatch, isGuest } = this.props;\n    if (hasWindow()) {\n      if (!isGuest) {\n        dispatch({\n          type: TOTAL_STORED_ADDRESS_COUNT,\n        });\n      }\n\n      getUnite().then((unite) => {\n        if (unite) {\n          this.resetPasswordSuccessKey = unite.on.resetPassword.success.addHandler(this.handleSuccessfulPasswordReset); // eslint-disable-line max-len\n          this.resetPasswordViewKey = unite.on.resetPassword.view.addHandler(this.handleForgotPasswordResetRender); // eslint-disable-line max-len\n        }\n      });\n    }\n  }\n\n  // TODO https://jira.nike.com/browse/CCC-5034\n  // eslint-disable-next-line camelcase\n  UNSAFE_componentWillReceiveProps(nextProps) {\n    const { shippingAddress, fieldErrors } = nextProps;\n\n    if (shippingAddress.addressId && this.props.initialSelectedAddressId === null) {\n      this.props.dispatch({\n        type: SET_INITIALLY_SELECTED_SHIPPING_ADDRESS,\n        payload: shippingAddress.addressId,\n      });\n    }\n    if (this.props && this.props.fieldErrors &&\n        Object.keys(fieldErrors).length > Object.keys(this.props.fieldErrors).length) { // $SR\n      this.props.dispatch(startSubmit('genericAddressForm'));\n    }\n  }\n\n  componentDidUpdate(prevProps) {\n    const fieldErrors = get(prevProps, 'fieldErrors', {});\n    const fieldErrorsLength = get(this, 'props.fieldErrorsLength', 0);\n    const newFieldErrors = get(this, 'props.fieldErrors', {});\n    const addressFieldErrors = get(this, 'props.addressFieldErrors', []);\n    const emailInUse = get(this, 'props.emailInUse');\n    const prevEmailInUse = get(prevProps, 'emailInUse');\n\n    if (\n      !prevProps.isShippingUserSelected &&\n      this.props.isShippingUserSelected\n    ) {\n      this.props.dispatch({\n        type: SET_SHOW_CONDENSED_ADDRESS_LIST,\n        payload: true,\n      });\n    }\n\n    if (Object.keys(fieldErrors).length < fieldErrorsLength) {\n      this.props.dispatch(stopSubmit('genericAddressForm', newFieldErrors));\n      if (addressFieldErrors) {\n        Object.keys(addressFieldErrors).forEach((error) => {\n          this.props.dispatch(touch('genericAddressForm', `address.${error}`));\n        });\n      }\n    }\n\n    if (emailInUse && emailInUse !== prevEmailInUse) {\n      this.props.dispatch({\n        type: LOGIN_PROMPT_SHOWN,\n      });\n    }\n  }\n\n  componentWillUnmount() {\n    getUnite().then((unite) => {\n      if (unite) {\n        unite.on.resetPassword.view.removeHandler(this.resetPasswordViewKey);\n        unite.on.resetPassword.success.removeHandler(this.resetPasswordSuccessKey);\n      }\n    });\n  }\n\n  setPreviouslySelectedAddress() {\n    const { shippingAddress } = this.props;\n    let prevAddressId;\n\n    /* Check to see if there is already an address selected */\n    if (shippingAddress) {\n      prevAddressId = shippingAddress.addressId;\n\n      this.props.dispatch({\n        type: SET_PREVIOUSLY_SELECTED_SHIPPING_ADDRESS,\n        payload: prevAddressId,\n      });\n    }\n  }\n\n  setStoredAddressView(e, view) {\n    if (e) { e.preventDefault(); }\n    this.props.dispatch({ type: SET_STORED_ADDRESSES_VIEW, payload: view });\n  }\n\n  handleApplyShippingAddress(formData, addressId, preferred, toOrderReview) {\n    const { dispatch, isApoFpo, isGuest, country } = this.props;\n    let addressData = formData;\n    /* Preemptively set the shippingAddress & recipient, until service call returns */\n    addressData = {\n      ...formData,\n      address: {\n        ...formData.address,\n        addressId,\n      },\n    };\n    // This needs to happen before we set the shipping\n    // address to keep the pageload events in the right order\n    return Promise.all([\n      dispatch({\n        type: SET_STORED_ADDRESSES_VIEW,\n        payload: true,\n      }),\n      dispatch({\n        type: SET_SHIPPING_ADDRESS,\n        payload: formatAddressFormToStateWithId(addressData, country),\n      }),\n      // Technically not Canceling but easier than making a new action just for canceling\n      dispatch({\n        type: CANCEL_EDIT_ADDRESS,\n      }),\n      dispatch({\n        type: VIEW_SHIPPING_METHODS,\n      }),\n    ]).then(() => {\n      if (isApoFpo && !isGuest) {\n        return this.handleApoFpoSubmit(toOrderReview);\n      } else if (isGuest) {\n        return this.handleApplyShippingAddressGuest(isApoFpo);\n      }\n      // DEFAULT_USER(member) or EMPLOYEE\n      return this.handleApplyAndSaveShippingAddress(addressId, preferred);\n    });\n  }\n\n  // This click handler handles Save/Continue clicks from shipping address save form,\n  // dym suggested address modal and dym address not verifiable modal\n  handleShippingContinueClicks(formData, addressId, preferred, toOrderReview) {\n    const {\n      country,\n      dispatch,\n      isApoFpo,\n      suggestedAddressSelected,\n      suggestedAddress,\n      showAddressValidationModal,\n      typeAheadReturnedBadCharacter,\n    } = this.props;\n    // const in experiment? selected or preferred address? override form data here.\n    let addressData = formData;\n    /* Preemptively set the shippingAddress & recipient, until service call returns */\n    addressData = (suggestedAddressSelected && showAddressValidationModal) ? {\n      ...formData,\n      address: {\n        ...formData.address,\n        ...suggestedAddress,\n        addressId,\n      },\n    } : {\n      ...formData,\n      address: {\n        ...formData.address,\n        addressId,\n      },\n    };\n    // Set the user's entered address to state\n    dispatch({ type: EDIT_ADDRESS });\n    dispatch({\n      type: SET_SHIPPING_ADDRESS,\n      payload: formatAddressFormToStateWithId(addressData, country),\n    });\n    // Don't submit address validation for apoFpo addresses or if TA returned a bad char\n    if (!showAddressValidationModal && !isApoFpo && !typeAheadReturnedBadCharacter) {\n      return dispatch(submitAddressForValidation()).then((modalWillOpen) => {\n        if (modalWillOpen) {\n          return Promise.resolve();\n        }\n        // DYM will not show - move forward in application flow\n        return this.applyAVSShippingAddress(addressData, toOrderReview, addressId, preferred);\n      });\n    }\n    // Modal is currently open, save/continue click comes from modal Save button\n    dispatch({ type: CANCEL_EDIT_ADDRESS });\n    dispatch({\n      type: SET_SHOW_ADDRESS_VALIDATION_MODAL,\n      payload: false,\n    });\n    return this.applyAVSShippingAddress(addressData, toOrderReview, addressId, preferred);\n  }\n\n  /*\n  Used by DYM - Applies a address into state and moves user forward in flow, triggered via\n  button clicks on two different DYM modals.\n  1) Found a better address - Save and Continue - user shown a modal indicating a\n      verified address that differs from what they entered and accepts the new address\n  2) Can't verify entered address - Save and Continue click - user shown a modal indicating\n      their entered address could not be verified and chooses\n      not to Edit Address but to use their unverifiable address\n  */\n  applyAVSShippingAddress(addressData, toOrderReview, addressId, preferred) {\n    const {\n      dispatch,\n      isApoFpo,\n      isGuest,\n      // country,\n    } = this.props;\n\n    // This needs to happen before we set the shipping\n    // address to keep the pageload events in the right order\n    Promise.all([\n      dispatch({\n        type: SET_STORED_ADDRESSES_VIEW,\n        payload: true,\n      }),\n      // Technically not Canceling but easier than making a new action just for canceling\n      dispatch({\n        type: CANCEL_EDIT_ADDRESS,\n      }),\n      dispatch({\n        type: VIEW_SHIPPING_METHODS,\n      }),\n    ]).then(() => {\n      if (isApoFpo && !isGuest) {\n        return this.handleApoFpoSubmit(toOrderReview);\n      } else if (isGuest) {\n        return this.handleApplyShippingAddressGuest(isApoFpo);\n      }\n      // DEFAULT_USER(member) or EMPLOYEE\n      return this.handleApplyAndSaveShippingAddress(addressId, preferred);\n    });\n  }\n\n  handleApplyAndSaveShippingAddress(addressId, preferred) {\n    const { country, dispatch } = this.props;\n    // remove the payment token so user has to re-enter cvv\n    dispatch({\n      type: SET_PAYMENT_TOKEN,\n      payload: '',\n    });\n    // This function is used for both adding a new address and updating an existing\n    // address. The new idn account service has different endpoints for these two\n    // actions.\n    if (addressId) {\n      dispatch(updateIdnAddressData({ addressId, country, preferred }));\n    } else {\n      dispatch(addIdnAddressData({ country, preferred }))\n        .then(addressData => dispatch(updateStoredAddresses(addressData.id)));\n    }\n    dispatch({\n      type: SAVE_ADDRESS,\n    });\n    dispatch({\n      type: OVERRIDE_CANCEL,\n      payload: null,\n    });\n    // always do this\n    return dispatch(fetchFulfillmentOfferings())\n      .then(() => dispatch(setDefaultSelectedPriceOffer()))\n      .then(() => dispatch(fetchCheckoutPreview()));\n  }\n\n  handleApplyShippingAddressGuest(isApoFpo) {\n    const { dispatch } = this.props;\n    dispatch({\n      type: SET_STORED_ADDRESSES_VIEW,\n      payload: true,\n    });\n    dispatch({\n      type: OVERRIDE_CANCEL,\n      payload: null,\n    });\n    if (isApoFpo) {\n      dispatch({\n        type: ADD_CLICK_TRACKER,\n        payload: {\n          clickedApoFpoSubmit: {\n            type: CLICK_SAVE_APO_FPO,\n          },\n        },\n      });\n    }\n    // always do this\n    dispatch({\n      type: SET_SHIPPING_OFFERINGS_PROCESSING,\n      payload: true,\n    });\n    return dispatch(fetchFulfillmentOfferings())\n      .then(() => dispatch(setDefaultSelectedPriceOffer()))\n      // We dispatch set_shipping_offerings_processing early here so that\n      // users with slow connections can see available shipping options as soon as possible\n      .then(() => dispatch({ type: SET_SHIPPING_OFFERINGS_PROCESSING }))\n      .then(() => dispatch(fetchCheckoutPreview()));\n  }\n\n  handleAddressValidationSelection(addressSelection) {\n    return this.props.dispatch({\n      type: SET_ADDRESS_VALIDATION_SELECTION,\n      payload: addressSelection,\n    });\n  }\n\n  handleApoFpoSubmit(toOrderReview) {\n    const { dispatch } = this.props;\n    dispatch({\n      type: ADD_CLICK_TRACKER,\n      payload: {\n        clickedApoFpoSubmit: {\n          type: CLICK_SAVE_APO_FPO,\n        },\n      },\n    });\n    dispatch(reset('genericAddressForm'));\n    return dispatch(fetchFulfillmentOfferings())\n      .then(() => dispatch(setDefaultSelectedPriceOffer()))\n      .then(() => {\n        if (!toOrderReview) {\n          return this.handleContinueToPayment();\n        }\n        return this.handleContinueToReview();\n      });\n  }\n\n  handleAddressExtendedDisplay() {\n    this.props.dispatch({\n      type: SET_SHOW_CONDENSED_ADDRESS_LIST,\n      payload: false,\n    });\n    this.props.dispatch({\n      type: SEE_MORE_ADDRESSES_CLICK,\n    });\n  }\n\n  handleSelectedShippingOption(newShippingOptionStr) {\n    const { dispatch } = this.props;\n    const isGuestReg = (/guest/i).test(newShippingOptionStr);\n\n    if (this.props.isGuestBelowThreshold) {\n      this.props.dispatch({ type: TOGGLE_GUEST_REGISTER_RADIO, payload: isGuestReg });\n    }\n    this.props.dispatch({\n      type: SET_SELECTED_FULFILLMENT_OFFERING_ID,\n      payload: newShippingOptionStr,\n    });\n    dispatch(fetchCheckoutPreview());\n  }\n\n  /* Directs a user click on the continue to payment button\n  to either submit the form and continue or just continue\n\n  To determine which action should be taken we need to know if the user\n  has the form open  or if the user is staring at a some version of a\n  form summary. showStoredAddressView indicates this state.\n\n  showStoredAddressView = true\n  Guest - has completed the form and is viewing the summary\n  Member - has no stored address but has completed the form and is staring at the summary\n  Member - has one stored address and is staring at the summary\n  Member - has multiple stored addresses and staring at the radio\n  list (which will always have an option selected)\n  */\n\n\n  handleContinueToPayment() {\n    const {\n      dispatch,\n      isGuestRegistering,\n      missingContactInformation,\n      shippingCompleted,\n      isClickAndCollectSelected,\n    } = this.props;\n\n    if (isGuestRegistering) {\n      // We want to exit early so user gets registered before we continue on.\n      return dispatch(submitUserRegistration());\n    }\n\n    if (shippingCompleted) {\n      dispatch({\n        type: UNEDIT_SHIPPING_SECTION,\n      });\n      dispatch({\n        type: EDIT_PAYMENT_SECTION,\n      });\n      dispatch({\n        type: SET_PAYMENT_PROCESSING,\n        payload: true,\n      });\n    }\n\n    if (isClickAndCollectSelected) {\n      return Promise.all([\n        dispatch(setShippingCPP()),\n        dispatch({\n          type: CONTINUE_TO_PAYMENT_SUCCESS,\n        }),\n        dispatch(fetchStoredPayments()),\n      ]);\n    }\n\n    if (missingContactInformation) {\n      return this.handleMissingContactInformation();\n    }\n\n    if (!shippingCompleted) {\n      return Promise.all([\n        dispatch({\n          type: OVERRIDE_CANCEL,\n          payload: false,\n        }),\n        dispatch({\n          type: SHOW_ADDRESS_VIEW,\n          payload: false,\n        }),\n      ]);\n    }\n\n    dispatch({\n      type: SET_USER_SELECTED_SHIPPING_OPTIONS,\n      payload: true,\n    });\n    dispatch({\n      type: CONTINUE_TO_PAYMENT_SUCCESS,\n    });\n    return Promise.all([\n      dispatch(fetchPaymentOptions()),\n      dispatch(fetchCheckoutPreview()),\n      dispatch(fetchStoredPayments()),\n    ])\n      .finally(() => {\n        dispatch({\n          type: SET_PAYMENT_PROCESSING,\n          payload: false,\n        });\n      });\n  }\n\n  handleMissingContactInformation() {\n    const {\n      dispatch,\n      email,\n      phoneNumber,\n    } = this.props;\n\n    return Promise.all([\n      dispatch({\n        type: OVERRIDE_CANCEL,\n        payload: true,\n      }),\n      dispatch({\n        type: SET_STORED_ADDRESSES_VIEW,\n        payload: false,\n      }),\n      dispatch({\n        type: EDIT_ADDRESS,\n      }),\n    ]).then(() => {\n      if (!email) {\n        dispatch(touch('genericAddressForm', 'address.email'));\n      }\n\n      if (!phoneNumber) {\n        dispatch(touch('genericAddressForm', 'address.phoneNumber'));\n      }\n    });\n  }\n\n  handleContinueToReview() {\n    const {\n      dispatch,\n      isGuestRegistering,\n      shippingCompleted,\n      missingContactInformation,\n    } = this.props;\n    if (isGuestRegistering) {\n      dispatch(submitUserRegistration());\n    } else {\n      dispatch({\n        type: UNEDIT_SHIPPING_SECTION,\n      });\n    }\n    dispatch({\n      type: UNEDIT_PAYMENT_SECTION,\n    });\n\n    if (missingContactInformation) {\n      // this is a promise...\n      return this.handleMissingContactInformation();\n    }\n\n    if (shippingCompleted) {\n      dispatch({\n        type: OVERRIDE_CANCEL,\n        payload: false,\n      });\n      dispatch({\n        type: SHOW_ADDRESS_VIEW,\n        payload: false,\n      });\n    }\n\n    return dispatch(fetchCheckoutPreview());\n  }\n\n  handleApplyStoredShippingAddress(addressId, value) {\n    const { dispatch } = this.props;\n    const currentAddress = this.props.shippingAddress.addressId;\n    if ((addressId !== currentAddress) || value === 'standard') { // $SR\n      dispatch({\n        type: OVERRIDE_CANCEL,\n        payload: false,\n      });\n    } else {\n      return Promise.resolve();\n    }\n\n    const storedAddresses = this.props.storedShippingAddresses;\n    const selectedAddress = storedAddresses\n      .filter(address => address.addressId === addressId); // $SR\n\n\n    let selectedAddressObj; // $SR\n    if (selectedAddress.length > 0) {\n      selectedAddressObj = Object.assign(...selectedAddress);\n    } else if (storedAddresses.length > 0) {\n      // this fixes toggle from APO - stored address after login\n      selectedAddressObj = Object.assign({}, storedAddresses[0]);\n    } else {\n      selectedAddressObj = {};\n    }\n\n    const updatedShippingAddress = {\n      address: selectedAddressObj,\n    };\n    /* This sets the shippingAddress *AND* recipient */\n    dispatch({\n      type: SET_SHIPPING_ADDRESS,\n      payload: formatAddressFormToStateWithId(updatedShippingAddress),\n    });\n\n    let apiFlow;\n\n    if (storedAddresses.length) {\n      dispatch({\n        type: SET_SHIPPING_OFFERINGS_PROCESSING,\n        payload: true,\n      });\n      apiFlow = dispatch(fetchFulfillmentOfferings())\n        .then(() => dispatch(setDefaultSelectedPriceOffer()))\n        // We dispatch set_shipping_offerings_processing early here so that\n        // users with slow connections can see available shipping options as soon as possible\n        .finally(() => dispatch({ type: SET_SHIPPING_OFFERINGS_PROCESSING }))\n        .then(() => dispatch(updateCheckoutPreview()))\n        .catch(() => { });\n    } else {\n      apiFlow = dispatch(fetchCartReviews());\n    }\n    return apiFlow.then(() => {\n      // Test address object for content before trying to validate it\n      // eslint-disable-next-line max-len\n      if (Object.keys(updatedShippingAddress.address).find(key => !!updatedShippingAddress.address[key])) {\n        return dispatch(validateSelectedShippingAddress());\n      }\n      return Promise.resolve();\n    });\n  }\n\n  cancelEdit(e) {\n    const { dispatch, previouslySelectedAddressId } = this.props;\n\n    this.props.dispatch({\n      type: REMOVE_TEMPORARY_ADDRESS,\n    });\n\n    dispatch({\n      type: CANCEL_EDIT_ADDRESS,\n    });\n\n    if (previouslySelectedAddressId) {\n      /* Use the previouslySelectedAddressId to set the shippingAddress */\n      this.handleApplyStoredShippingAddress(previouslySelectedAddressId);\n\n      /* Clear the previouslySelectedAddressId */\n      dispatch({\n        type: SET_PREVIOUSLY_SELECTED_SHIPPING_ADDRESS,\n        payload: null,\n      });\n    }\n\n    // clear the form\n    dispatch(reset('genericAddressForm'));\n\n    if (!this.props.shippingCompleted) {\n      dispatch({\n        type: OVERRIDE_CANCEL,\n        payload: true,\n      });\n    }\n    dispatch({\n      type: SHOW_ADDRESS_VIEW,\n      payload: true,\n    });\n    /* Switch back to stored Address view state */\n    this.setStoredAddressView(e, true);\n    dispatch({\n      type: CANCEL_UPDATE_NEW_ADDRESS,\n    });\n  }\n\n  handleEditAddress(e, addressId) {\n    if (e) { e.preventDefault(); }\n\n    this.setPreviouslySelectedAddress();\n\n    this.handleApplyStoredShippingAddress(addressId);\n    this.setStoredAddressView(e, false);\n\n    // Since edit address and new address share the same form\n    // we need to clear out any temporary addresses because\n    // they have priority when the form loads\n    this.props.dispatch({\n      type: REMOVE_TEMPORARY_ADDRESS,\n    });\n\n    this.props.dispatch({\n      type: SHOW_ADDRESS_VIEW,\n      payload: false,\n    });\n\n    this.props.dispatch({\n      type: EDIT_ADDRESS,\n    });\n  }\n\n  clearForm(e) {\n    if (e) { e.preventDefault(); }\n    const { dispatch, country } = this.props;\n    this.setPreviouslySelectedAddress();\n    dispatch({\n      type: SET_SHIPPING_ADDRESS,\n      payload: {\n        shippingAddress: {\n          country,\n        },\n        recipient: {},\n      },\n    });\n    dispatch({\n      type: ADD_NEW_ADDRESS,\n    });\n    // need to wait for the clear form to happen before we continue to show the address form\n    return this.resetForm(dispatch).then(() => {\n      dispatch({\n        type: SET_STORED_ADDRESSES_VIEW,\n        payload: false,\n      });\n    });\n  }\n\n  emailWarn(values) {\n    if (this.props.uniteEmailCheck) {\n      this.props.dispatch(checkEmailStatusUnite(values));\n    }\n  }\n\n  toggleShippingOptions(event) {\n    const newShippingOption = get(event, 'target.value');\n    const {\n      dispatch,\n      storedShippingAddresses,\n      initialSelectedAddressId,\n      showApoToggle,\n      hasConsumerPickupPoint,\n      isApoFpo,\n    } = this.props;\n\n    const selectingClickAndCollect = newShippingOption === SHIPPING_TYPE_CLICK_AND_COLLECT;\n\n    if (this.props.addressType === newShippingOption) {\n      event.preventDefault();\n      return;\n    }\n\n    if (\n      this.props.addressType === SHIPPING_TYPE_CLICK_AND_COLLECT &&\n      newShippingOption === SHIPPING_TYPE_STANDARD\n    ) {\n      setQueryString(null);\n    }\n    dispatch({\n      type: SET_ADDRESS_TYPE,\n      payload: newShippingOption,\n    });\n\n    // If this country has Click and Collect and we are moving from one shipping type\n    // to another then reset the shipping related state\n    if (hasConsumerPickupPoint) {\n      if (newShippingOption === SHIPPING_TYPE_STANDARD) {\n        // CCC-2649 fixes field errors in checkout_previews call\n        this.cancelEdit();\n      }\n      dispatch(clearAllClickAndCollectData());\n    }\n\n    if ((showApoToggle && !isApoFpo) || selectingClickAndCollect) {\n      dispatch({\n        type: SET_STORED_ADDRESSES_VIEW,\n        payload: false,\n      });\n      dispatch(reset('genericAddressForm'));\n    } else {\n      const showStored = storedShippingAddresses.length;\n      dispatch({\n        type: SET_STORED_ADDRESSES_VIEW,\n        payload: showStored,\n      });\n      if (initialSelectedAddressId) {\n        /* Use the previouslySelectedAddressId to set the shippingAddress */\n        this.handleApplyStoredShippingAddress(initialSelectedAddressId, newShippingOption);\n      }\n      /* Clear the previouslySelectedAddressId */\n      this.props.dispatch({\n        type: SET_PREVIOUSLY_SELECTED_SHIPPING_ADDRESS,\n        payload: null,\n      });\n    }\n    dispatch(fetchCartReviews());\n  }\n\n  handleClickAndCollect(formdata) {\n    const { dispatch, isJP, isMobile } = this.props;\n    if (isJP) {\n      return dispatch(goToSagawa(formdata, isMobile));\n    }\n    return dispatch(goToUPS(formdata));\n  }\n\n  handleClickAndCollectEdit() {\n    const { dispatch } = this.props;\n    dispatch({\n      type: UPDATE_PICKUP_LOCATION,\n      payload: { name: null },\n    });\n    dispatch(clearAllClickAndCollectData());\n  }\n\n  // Handles the pickup recipient continue button by checking the recipient form has all values set\n  handlePickupRecipientSubmit() {\n    const {\n      dispatch,\n      isPickupRecipientFormValid,\n    } = this.props;\n    if (isPickupRecipientFormValid) {\n      dispatch(setShippingCPP());\n      dispatch({ type: SET_RECIPIENT_FORM_ACTIVE });\n      dispatch({ type: CLICK_PICKUP_CONTINUE_TO_PAYMENT });\n      return Promise.all([\n        dispatch(fetchPaymentOptions()),\n        dispatch(fetchCheckoutPreview()),\n        dispatch(fetchStoredPayments()),\n      ]).then(() => {\n        dispatch({\n          type: UNEDIT_SHIPPING_SECTION,\n        });\n        dispatch({\n          type: EDIT_PAYMENT_SECTION,\n        });\n      });\n    }\n    dispatch({ type: CLICK_COLLECT_CONTINUE_FAIL });\n    dispatch(touch('clickAndCollectForm', 'email', 'firstName', 'lastName', 'phoneNumber'));\n    return Promise.resolve();\n  }\n\n  handleLoginClick(e) {\n    const {\n      addressFormEmail,\n      dispatch,\n    } = this.props;\n    e.preventDefault();\n    dispatch(openLoginModal());\n    if (addressFormEmail) {\n      // emit analytic event\n      dispatch({\n        type: CLICK_LOGIN_PROMPT,\n      });\n      // render email input value\n      getUnite().then((unite) => {\n        unite.views.login.render({\n          emailAddress: { value: addressFormEmail },\n        });\n      });\n    }\n  }\n\n  handleForgotPasswordResetRender() {\n    this.props.dispatch({\n      type: MODAL_FORGOT_PASSWORD,\n      payload: 'forgot password',\n    });\n  }\n\n  handleSuccessfulPasswordReset() {\n    this.props.dispatch({\n      type: MODAL_FORGOT_PASSWORD,\n      payload: 'reset',\n    });\n  }\n\n  toggleIsScheduledDelivery() {\n    this.props.dispatch(toggleIsScheduledDelivery(!this.props.isScheduledDelivery));\n  }\n\n  handleScheduledDeliveryDateChange(event) {\n    this.props.dispatch(setScheduledDeliveryDate(event.target.value));\n  }\n\n  handleScheduledDeliveryTimeChange(event) {\n    const scheduledDeliveryTimeIndex = event.target.value;\n    this.props.dispatch({\n      type: SET_SCHEDULED_DELIVERY_TIME,\n      payload: this.props.scheduledDeliveryOptionalTimes[scheduledDeliveryTimeIndex],\n    });\n  }\n\n  handleIsGiftReceipt() {\n    this.props.dispatch(toggleIsGiftReceipt());\n  }\n\n  // eslint-disable-next-line class-methods-use-this\n  resetForm(dispatch) {\n    return new Promise((resolve) => {\n      dispatch(reset('genericAddressForm'));\n      resolve();\n    });\n  }\n\n  handleDeliveryTypeToggle(e) {\n    const { dispatch } = this.props;\n    const deliveryType = get(e, 'target.dataset.deliveryType', DELIVERY_TYPES.SHIP);\n\n    dispatch({ type: SET_DELIVERY_TYPE, payload: deliveryType });\n\n    if (deliveryType === DELIVERY_TYPES.PICKUP) {\n      dispatch({\n        type: EDIT_PICKUP_LOCATION,\n        payload: true,\n      });\n      dispatch({ type: CLICK_PICKUP_TAB });\n    } else if (deliveryType === DELIVERY_TYPES.SHIP) {\n      dispatch({ type: SET_ADDRESS_TYPE, payload: SHIPPING_TYPE_STANDARD });\n      dispatch({ type: CLICK_SHIP_TAB });\n      dispatch({ type: UPDATE_PICKUP_LOCATION });\n      return dispatch(fetchFulfillmentOfferings())\n        .then(() => dispatch(fetchCartReviews()))\n        .then(() => dispatch(setDefaultSelectedPriceOffer()));\n    }\n    return Promise.resolve();\n  }\n\n  render() {\n    const {\n      addressUnverified,\n      addressValidationSelection,\n      bopisPickupDate,\n      bopisPickupLocation,\n      contactInfo,\n      containsOnlyDigitalGiftCard,\n      dispatch,\n      digitalGiftCardItems,\n      distanceUnitLabel,\n      shippingOfferingsProcessing,\n      hasDigitalGiftCard,\n      inExperimentAB4114,\n      inExperimentAB4114b,\n      inExperimentAB4115,\n      inExperimentAB4116,\n      inDeliverySection,\n      isPickupDeliveryType,\n      isDeliveryTypePickup,\n      isDeliveryTypeShip,\n      isEditingPickupLocation,\n      isGuest,\n      isGuestBelowThreshold,\n      isJerseyIdPresent,\n      isNikeIDPresent,\n      isScheduledDelivery,\n      isSwoosh,\n      isUS,\n      pickupLocation,\n      primaryEmail,\n      processing,\n      pupLocation,\n      recipient,\n      selectedPriceOffer,\n      selectedScheduledDeliveryTime,\n      shippingAddress,\n      shippingAddressForValidationModal,\n      shippingCompleted,\n      shipFulfillmentDetails,\n      shippingPriceOffers,\n      showAddressValidationModal,\n      showCondensedAddressList,\n      showProp65,\n      showScheduledDelivery,\n      showShippingMethods,\n      showShippingMethodsPreview,\n      suggestedAddress,\n      toOrderReview,\n      shouldShowPickupTab,\n    } = this.props;\n\n\n    const applyAddressMethod = isUS || inExperimentAB4114 || inExperimentAB4114b ||\n      inExperimentAB4115 || inExperimentAB4116 ?\n      this.handleShippingContinueClicks : this.handleApplyShippingAddress;\n\n    // This section could use some clean up... Will have to check later to see find better way\n    if (inDeliverySection) {\n      const renderValidationModal = (isUS || inExperimentAB4114 ||\n        inExperimentAB4114b || inExperimentAB4115 || inExperimentAB4116) &&\n        showAddressValidationModal;\n\n      return (\n        <React.Fragment>\n          { processing &&\n            <Loading />\n          }\n          { !processing &&\n            <React.Fragment>\n              <UniteModal className=\"login-modal-exp\" />\n              {renderValidationModal && <AddressValidationModal\n                addressUnverified={addressUnverified}\n                addressValidationSelection={addressValidationSelection}\n                handleAddressValidationSelection={this.handleAddressValidationSelection}\n                handleEventTracking={(event) => {\n                  const { action } = event;\n                  switch (action) {\n                  case 'modalShown':\n                    dispatch({\n                      type: DYM_MODAL_SHOWN,\n                      payload: event.modalType,\n                    });\n                    break;\n                  case 'saveAndContinueClicked':\n                  case 'editAddressClicked':\n                    dispatch({\n                      type: DYM_MODAL_CLICKED,\n                      payload: event,\n                    });\n                    break;\n                  default:\n                    // nothing to see here\n                  }\n                }}\n                isOpen\n                onAccept={() => {\n                  dispatch({\n                    type: SET_AVS_ADDRESS_UNVERIFIED,\n                    payload: false,\n                  });\n                  dispatch(submit('genericAddressForm'));\n                  dispatch({\n                    type: ADDRESS_VALIDATION_SAVE_CONTINUE,\n                    payload: addressValidationSelection,\n                  });\n                }}\n                onCancel={(e) => {\n                  dispatch({\n                    type: SET_AVS_ADDRESS_UNVERIFIED,\n                    payload: false,\n                  });\n                  dispatch({\n                    type: SET_SHOW_ADDRESS_VALIDATION_MODAL,\n                    payload: false,\n                  });\n                  dispatch({\n                    type: ADDRESS_VALIDATION_EDIT_ADDRESS,\n                    payload: addressUnverified ? 'unverified' : 'suggestion',\n                  });\n                  this.handleEditAddress(e, shippingAddress.addressId);\n                }}\n                selectedAddress={shippingAddressForValidationModal}\n                suggestedAddress={suggestedAddress}\n              />}\n              {!shouldShowPickupTab ?\n                <Ship\n                  id={DELIVERY_TYPES.SHIP}\n                  checked={isDeliveryTypeShip}\n                  label={formatMessage(messages.shipLabel)}\n                  handleApplyShippingAddress={applyAddressMethod}\n                  cancelEdit={this.cancelEdit}\n                  clearForm={this.clearForm}\n                  emailWarn={this.emailWarn}\n                  shippingPriceOffers={shippingPriceOffers}\n                  handleAddressExtendedDisplay={this.handleAddressExtendedDisplay}\n                  handleApplyStoredShippingAddress={this.handleApplyStoredShippingAddress}\n                  handleLoginClick={this.handleLoginClick}\n                  showCondensedAddressList={showCondensedAddressList}\n                  handleEditAddress={this.handleEditAddress}\n                  toggleShippingOptions={this.toggleShippingOptions} // TOGGLE OF SHIP TYPE\n                  handleContinueToPayment={this.handleContinueToPayment}\n                  toggleIsScheduledDelivery={this.toggleIsScheduledDelivery}\n                  handleSelectedShippingOption={this.handleSelectedShippingOption}\n                  handleIsGiftReceipt={this.handleIsGiftReceipt}\n                  handleScheduledDeliveryDateChange={this.handleScheduledDeliveryDateChange}\n                  handleScheduledDeliveryTimeChange={this.handleScheduledDeliveryTimeChange}\n                  handleLabelClick={this.handleDeliveryTypeToggle}\n                  isMobile={this.isMobile}\n                  items={this.items}\n                  {...this.props}\n                />\n                :\n                <ShippingTabWrapper>\n                  <Ship\n                    id={DELIVERY_TYPES.SHIP}\n                    checked={isDeliveryTypeShip}\n                    label={formatMessage(messages.shipLabel)}\n                    handleApplyShippingAddress={applyAddressMethod}\n                    cancelEdit={this.cancelEdit}\n                    clearForm={this.clearForm}\n                    emailWarn={this.emailWarn}\n                    shippingPriceOffers={shippingPriceOffers}\n                    handleAddressExtendedDisplay={this.handleAddressExtendedDisplay}\n                    handleApplyStoredShippingAddress={this.handleApplyStoredShippingAddress}\n                    handleLoginClick={this.handleLoginClick}\n                    showCondensedAddressList={showCondensedAddressList}\n                    handleEditAddress={this.handleEditAddress}\n                    toggleShippingOptions={this.toggleShippingOptions} // TOGGLE OF SHIP TYPE\n                    handleContinueToPayment={this.handleContinueToPayment}\n                    toggleIsScheduledDelivery={this.toggleIsScheduledDelivery}\n                    handleSelectedShippingOption={this.handleSelectedShippingOption}\n                    handleIsGiftReceipt={this.handleIsGiftReceipt}\n                    handleScheduledDeliveryDateChange={this.handleScheduledDeliveryDateChange}\n                    handleScheduledDeliveryTimeChange={this.handleScheduledDeliveryTimeChange}\n                    handleLabelClick={this.handleDeliveryTypeToggle}\n                    isMobile={this.isMobile}\n                    items={this.items}\n                    {...this.props}\n                  />\n                  <Pickup\n                    isEditingPickupLocation={isEditingPickupLocation}\n                    id={DELIVERY_TYPES.PICKUP}\n                    checked={isDeliveryTypePickup}\n                    label={formatMessage(messages.pickupLabel)}\n                    handleLabelClick={this.handleDeliveryTypeToggle}\n                    handleRecipientSubmit={this.handlePickupRecipientSubmit}\n                    initialPostalCode={shippingAddress && shippingAddress.postalCode}\n                    {...this.props}\n                  />\n                </ShippingTabWrapper>\n              }\n              {isDeliveryTypeShip &&\n              (showShippingMethods || showShippingMethodsPreview) &&\n              <div className=\"ncss-row\">\n                <div className=\"ncss-col-sm-12 ncss-col-md-12 pb5-sm prl5-sm va-sm-t ta-sm-r\">\n                  <button\n                    onClick={this.handleContinueToPayment}\n                    type=\"button\"\n                    className={'js-next-step continuePaymentBtn mod-ncss-btn ncss-btn-accent ncss-brand mod-button-width pt3-sm prl5-sm pb3-sm pt2-lg pb2-lg u-md-ib u-uppercase u-rounded fs14-sm'}\n                    disabled={shippingOfferingsProcessing}\n                  >\n                    { !toOrderReview ? formatMessage(messages.continueToPayment) :\n                      formatMessage(messages.continueToReviewButton)}\n                  </button>\n                </div>\n              </div>}\n            </React.Fragment>\n          }\n        </React.Fragment>\n      );\n    }\n\n    if (shippingAddress || getContainsOnlyDigitalGiftCard) {\n      return (\n        <div className=\"ncss-row\">\n          <div className=\"ncss-col-sm-12 p5-sm\">\n            { processing &&\n              <Loading />\n            }\n            { !processing &&\n            <ShippingPreview\n              address={shippingAddress}\n              bopisPickupDate={bopisPickupDate}\n              bopisPickupLocation={bopisPickupLocation}\n              contactInfo={contactInfo}\n              containsOnlyDigitalGiftCard={containsOnlyDigitalGiftCard}\n              digitalGiftCardItems={digitalGiftCardItems}\n              distanceUnitLabel={distanceUnitLabel}\n              hasDigitalGiftCard={hasDigitalGiftCard}\n              isPickupDeliveryType={isPickupDeliveryType}\n              isGuest={isGuest}\n              isGuestBelowThreshold={isGuestBelowThreshold}\n              isJerseyIdPresent={isJerseyIdPresent}\n              isNikeIDPresent={isNikeIDPresent}\n              isScheduledDelivery={isScheduledDelivery}\n              isUS={isUS}\n              pickupLocation={pickupLocation}\n              shippingPriceOffers={shippingPriceOffers}\n              recipient={recipient}\n              selectedPriceOffer={selectedPriceOffer}\n              selectedScheduledDeliveryTime={selectedScheduledDeliveryTime}\n              shippingCompleted={shippingCompleted}\n              shouldShowScheduledDelivery={showScheduledDelivery}\n              showPrice\n              showProp65={showProp65}\n              shipFulfillmentDetails={shipFulfillmentDetails}\n              pupLocation={pupLocation}\n              {...((isSwoosh) ? {\n                email: primaryEmail,\n              } : {})}\n            />\n            }\n          </div>\n        </div>\n      );\n    }\n    return null;\n  }\n}\n\nexport function mapStateToProps(state) {\n  const checkoutState = state.checkout;\n  const appState = state.app;\n  const uiState = appState.ui;\n  const isSwoosh = getIsAnyGeoSwooshUser(state);\n  const formSelector = formValueSelector(ADDRESS_FORM_NAME);\n  const addressValidationSelection = state.app.ui.shipping.addressValidationSelection || 'suggestedAddress';\n\n  const isApoFpo = getIsApoFpo(state);\n  return {\n    ...uiState.shipping, // $SR\n    addressFieldErrors: getAddressFieldErrors(state),\n    addressFormEmail: getEmailAddressFromForm(state),\n    addressType: getAddressType(state),\n    addressUnverified: state.app.ui.shipping.addressNotAbleToBeVerified, // $SR\n    addressValidationSelection,\n    availableAddresses: isSwoosh\n      ? getAvailableSwooshAddresses(state)\n      : getAvailableAddresses(state),\n    baseShippingType: getBaseShippingType(state),\n    bopisPickupDate: getBopisPickupDate(state),\n    bopisPickupLocation: getBopisPickupLocation(state),\n    pupLocation: getPupLocationWithFulfillmentInfo(state),\n    contactInfo: checkoutState.contactInfo, // $SR\n    containsOnlyDigitalGiftCard: getContainsOnlyDigitalGiftCard(state),\n    country: checkoutState.country, // $SR\n    currentValues: { address: formSelector(state, 'address') },\n    digitalGiftCardItems: getDigitalGiftCardItems(state),\n    distanceUnitLabel: getDistanceUnitLabel(state),\n    email: getEmailAddress(state),\n    emailInUse: getEmailInUse(state),\n    experimentAttributes: getExperimentAttributes(state),\n    fieldErrors: getShippingFieldErrors(state),\n    fieldErrorsLength: getFieldErrorsLength(state),\n    shippingOfferingsProcessing: getIsShippingOfferingsProcessing(state),\n    fulfillmentOfferings: getFulfillmentOfferings(state),\n    genericAddressForm: state.form.genericAddressForm,\n    hasConsumerPickupPoint: getHasConsumerPickupPoint(state),\n    hasDigitalGiftCard: getContainsADigitalGiftCard(state),\n    inDeliverySection: getIsInDeliverySection(state),\n    initialSelectedAddressId: getInitialSelectedAddressId(state),\n    initialValues: getInitialAddress(state),\n    isApoFpo,\n    isClickAndCollectRecipientSet: getIsRecipientSet(state),\n    isClickAndCollectSelected: getIsClickAndCollectSelected(state),\n    isCN: getIsCN(state),\n    isDeliveryTypePickup: getIsDeliveryTypePickup(state),\n    isDeliveryTypeShip: getIsDeliveryTypeShip(state),\n    isEditingPickupLocation: getIsEditingPickupLocation(state),\n    isEditingStoredAddress: getIsEditingStoredAddress(state),\n    isEU: getIsEU(state),\n    isFauxLoading: getIsFauxLoading(state),\n    isGiftReceipt: getIsGiftReceipt(state),\n    isGuest: getIsGuestUser(state),\n    isGuestAboveThreshold: getIsGuestAboveThreshold(state),\n    isGuestBelowThreshold: getIsGuestBelowThreshold(state),\n    isGuestRegistering: getIsGuestRegistering(state),\n    isGuestRegistrationProcessing: getIsGuestRegistrationProcessing(state),\n    isInlinePresent: getInlinePresent(state),\n    isJerseyIdPresent: getJerseyIdPresent(state),\n    isJP: getIsJP(state),\n    isMobile: getIsMobile(state),\n    isNikeIDPresent: getNikeIdPresent(state),\n    isPickupDeliveryType: getIsPickupDeliveryType(state),\n    isPickupLocationSelected: getIsPickupLocationSelected(state),\n    isPickupRecipientFormValid: isValid('clickAndCollectForm')(state),\n    isRecipientFormActive: getIsRecipientFormActive(state),\n    isScheduledDelivery: getIsScheduledDelivery(state),\n    isSelectingCPP: getIsSelectingCPP(state),\n    isShippingUserSelected: getIsShippingUserSelected(state),\n    isSwoosh,\n    isUS: getIsUS(state),\n    items: getItems(state),\n    missingContactInformation: getMissingContactInformation(state),\n    orderId: getOrderId(state),\n    phoneNumber: getPhoneNumber(state),\n    pickupLocation: getPickupLocation(state),\n    preferred: getIsPreferred(state),\n    primaryEmail: getSwooshPrimaryEmail(state),\n    processing: getIsShippingProcessing(state),\n    registrationFormValues: getRegistrationFormValues(state),\n    scheduledDeliveryOptionalDates: getScheduledDeliveryOptionalDates(state),\n    scheduledDeliveryOptionalTimes: getScheduledDeliveryOptionalTimes(state),\n    selectedPriceOffer: getSelectedPriceOffer(state),\n    selectedScheduledDeliveryDate: getSelectedScheduledDeliveryDate(state),\n    selectedScheduledDeliveryTime: getSelectedScheduledDeliveryTime(state),\n    shipFulfillmentDetails: getShipFulfillmentDetails(state),\n    shippingAddress: state.checkout.shippingAddress, // $SR\n    shippingAddressForValidationModal: getShippingAddressForValidationModal(state),\n    shippingCompleted: getIsShippingAddressComplete(state),\n    shippingCutoffTime: getShippingCutoffTime(state),\n    shippingPriceOffers: getFormattedShippingPriceOffers(state),\n    shippingTotal: getFormattedFulfillmentCostTotal(state),\n    showAddressValidationModal: getShowAddressValidationModal(state),\n    showApoToggle: getShowApoToggle(state),\n    showCondensedAddressList: getShowCondensedAddressList(state),\n    showGiftReceipt: getShouldShowGiftReceipt(state),\n    showGuestRegisterAsCheckbox: getShowGuestRegisterAsCheckbox(state),\n    showJPCutoffTimeMessaging: getShowJPCutoffTimeMessaging(state),\n    showLoginSection: getShowLoginSection(state),\n    showProp65: getShowProp65(state),\n    showScheduledDelivery: getShouldShowScheduledDelivery(state),\n    showShippingDisclaimer: getShowShippingDisclaimer(state),\n    showShippingMethods: getShouldShowShippingMethods(state),\n    showShippingMethodsPreview: getShowShippingMethodsPreview(state),\n    showStoredAddressesView: getShowStoredAddressesView(state),\n    storedShippingAddresses: getStoredShippingAddresses(state),\n    storesWithFulfillmentInfo: getStoreListWithFulfillmentInfo(state),\n    storeViews: getStoreViews(state),\n    suggestedAddress: state.checkout.suggestedAddress, // $SR\n    suggestedAddressSelected: state.app.ui.shipping.addressValidationSelection === 'suggestedAddress', // $SR\n    toOrderReview: getToOrderReview(state),\n    typeAheadReturnedBadCharacter: getTypeAheadHasBadCharacter(state),\n    shouldShowPickupTab: getShouldShowPickupTab(state),\n  };\n}\n\nShippingComponent.propTypes = {\n  addressUnverified: PropTypes.bool,\n  addressFormEmail: PropTypes.string,\n  addressType: PropTypes.string,\n  addressValidationSelection: PropTypes.string,\n  bopisPickupDate: PropTypes.string,\n  clickAndCollectRecipient: PropTypes.shape({}),\n  clickAndCollectRecipientFields: PropTypes.string,\n  contactInfo: PropTypes.shape({}),\n  containsOnlyDigitalGiftCard: PropTypes.bool,\n  country: PropTypes.string,\n  disabled: PropTypes.bool,\n  dispatch: PropTypes.func,\n  distanceUnitLabel: PropTypes.string,\n  email: PropTypes.string,\n  emailInUse: PropTypes.bool,\n  bopisPickupLocation: PropTypes.shape({\n    id: PropTypes.string,\n  }),\n  fieldErrors: PropTypes.oneOfType([\n    PropTypes.array,\n    PropTypes.shape({}),\n  ]),\n  digitalGiftCardItems: PropTypes.shape({}),\n  hasConsumerPickupPoint: PropTypes.bool,\n  hasDigitalGiftCard: PropTypes.bool,\n  isFauxLoading: PropTypes.bool,\n  inExperimentAB4114: PropTypes.bool,\n  inExperimentAB4114b: PropTypes.bool,\n  inExperimentAB4115: PropTypes.bool,\n  inExperimentAB4116: PropTypes.bool,\n  initialSelectedAddressId: PropTypes.string,\n  inDeliverySection: PropTypes.bool,\n  isApoFpo: PropTypes.bool,\n  isPickupDeliveryType: PropTypes.bool,\n  isPickupRecipientFormValid: PropTypes.bool,\n  isClickAndCollectRecipientSet: PropTypes.bool,\n  isClickAndCollectSelected: PropTypes.bool,\n  isDeliveryTypePickup: PropTypes.bool,\n  isDeliveryTypeShip: PropTypes.bool,\n  isEditingPickupLocation: PropTypes.bool,\n  isGuest: PropTypes.bool,\n  isGuestBelowThreshold: PropTypes.bool,\n  isGuestRegistering: PropTypes.bool,\n  isJerseyIdPresent: PropTypes.bool,\n  isJP: PropTypes.bool,\n  isUS: PropTypes.bool,\n  isMobile: PropTypes.bool,\n  showCondensedAddressList: PropTypes.bool,\n  isNikeIDPresent: PropTypes.bool,\n