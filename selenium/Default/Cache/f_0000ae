 to add the needed url/call\n  render() {\n    // for redirect we should post a form\n    if (this.props.redirectUrl) {\n      const children = (\n        <React.Fragment>\n          <iframe\n            name={this.iframeName}\n            sandbox=\"allow-scripts allow-forms allow-same-origin\"\n            className=\"mt1 u-full-width full-screen-height\"\n            src={this.props.redirectUrl}\n            frameBorder=\"0\"\n            scrolling=\"no\"\n            title={this.iframeName}\n          />\n          <BankForm\n            iframeName={this.iframeName}\n            redirectUrl={this.props.redirectUrl}\n            PaReq={this.props.PaReq}\n            MD={this.props.MD}\n            url={this.props.url}\n          />\n        </React.Fragment>\n      );\n      return ReactDOM.createPortal(\n        (\n          <PopUpDrawer\n            handleClose={this.props.handleClose}\n            header={formatMessage(messages.adyenUITitle)}\n            isOpen\n            showTitle\n          >\n            {children}\n          </PopUpDrawer>\n        ),\n        this.adyenRoot);\n    }\n    if (this.props.container) {\n      return ReactDOM.createPortal(\n        <PopUpDrawer\n          handleClose={this.props.handleClose}\n          header={formatMessage(messages.adyenUITitle)}\n          isOpen\n          showTitle\n        >\n          {this.props.container}\n        </PopUpDrawer>,\n        this.adyenRoot\n      );\n    }\n    return null;\n  }\n}\n\nAdyenDrawerPopup.propTypes = {\n  container: PropTypes.node,\n  handleClose: PropTypes.func,\n  MD: PropTypes.string,\n  PaReq: PropTypes.string,\n  redirectUrl: PropTypes.string,\n  url: PropTypes.string,\n};\n\nexport default AdyenDrawerPopup;\n\n\n\n// WEBPACK FOOTER //\n// ./src/checkout/view/components/payment/Adyen/AdyenDrawer.js","import PropTypes from 'prop-types';\nimport React from 'react';\nimport { SET_ADYEN_CHALLENGE_MODAL_IN_USE } from '../../../../state/app';\n\nexport class AdyenChallengeCo