`;\n      let label;\n      if (isCod || cardType === 'COD') {\n        label = formatMessage(messages.codLabel);\n      } else if (isKonbini) {\n        label = formatMessage(messages.konbiniLabel);\n      } else {\n        label = cardType;\n      }\n      const paymentMethodElement = (\n        <p className={cardClassList}>\n          { country !== COUNTRY_CHINA && <span className=\"ml2-sm d-sm-ib va-sm-m\">{label}</span> }\n        </p>\n      );\n      paymentMethodRender = (\n        <React.Fragment>\n          { paymentMethodElement }\n          { isKonbini && (\n            <React.Fragment>\n              <p className=\"u-bold\">{formatMessage(messages.konbiniSecureInventory)}</p>\n              <h5 className=\"mt4-sm\">{formatMessage(messages.konbiniDue)}</h5>\n              <p className=\"u-bold\">{formatMessage(messages.konbiniAutomaticCancel)}</p>\n              <p className=\"u-bold\">{formatMessage(messages.konbiniShppingAfterPayment)}</p>\n              { isOrderConfirmationPage && <KonbiniConfirmButton /> }\n            </React.Fragment>\n          )}\n        </React.Fragment>\n      );\n      addressRender = (isCod || cardType === 'COD' || isSofort || isIdeal || isKonbini) ? (\n        <div>\n          <h3>{formatMessage(messages.billingDetailsTitle)}</h3>\n          <AddressPreview {...billingAddress} billingPreview />\n        </div>\n      ) : null;\n    }\n  }\n  return (\n    <div className=\"paymentPreview-container payment-list\">\n      <section className=\"mb4-sm\">\n        <h3 className=\"paymentPreview-header\">\n          { formatMessage(messages.paymentMethodsTitle) }\n        </h3>\n        { paymentMethodRender }\n      </section>\n      <section>\n        { prepaidMethods &&\n          Object.keys(prepaidMethods).length > 0 &&\n          Object.keys(prepaidMethods).map((prepaidMethod, i) => { // $SR\n            const accountNumber = get(prepaidMethods, [prepaidMethod, 'accountNumber'], ''); // $SR\n            const lastFour = accountNumber.slice(\n              accountNumber.length - 4,\n              accountNumber.length\n            ); // $SR\n            return (\n              <div className=\"gift-card-applied\" key={prepaidMethod}>\n                <span className={`d-sm-ib u-max-width pb4-sm text-color-grey giftcard-icn gc-preview-${i}`}>\n                  {formatMessage(messages.giftCardPreviewLabel)}\n                  { lastFour }\n                </span>\n              </div>\n            );\n          })\n        }\n\n        { appliedCodes && appliedCodes.map((promoCode) => {\n          const displayText = get(promotionCodes[promoCode], 'displayName', promoCode);\n          return (\n            <div className=\"promo-code-applied\" key={promoCode}>\n              <span className=\"d-sm-ib u-max-width pb4-sm text-color-grey promocode-icn\">\n                {formatMessage(messages.promoCodePreviewLabel, { code: displayText })}\n              </span>\n            </div>\n          );\n        })\n        }\n\n        { withBillingAddress && addressRender }\n      </section>\n\n      {\n        withFapiao &&\n          <section className=\"mt4-sm\">\n            <h4>{ formatMessage(messages.fapiaoTitle) }</h4>\n            <p className=\"text-color-grey\">\n              { formatMessage(messages.fapiaoTypeLabel) }\n              { formatMessage(messages.electronicFapiaoText) }\n            </p>\n            <p className=\"text-color-grey\">\n              { formatMessage(messages.fapiaoDetailLabel) }\n              { fapiaoDetail }\n            </p>\n          </section>\n      }\n    </div>\n  );\n}\n\nPaymentPreviewComponent.propTypes = {\n  country: PropTypes.string,\n  withBillingAddress: PropTypes.bool,\n  billingAddress: PropTypes.shape({}),\n  contactInfo: PropTypes.shape({\n    email: PropTypes.string,\n  }),\n  inExperimentAB4790A: PropTypes.bool,\n  isCod: PropTypes.bool,\n  isSofort: PropTypes.bool,\n  isIdeal: PropTypes.bool,\n  isKonbini: PropTypes.bool,\n  paymentM