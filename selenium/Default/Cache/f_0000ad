n    // Due to different formatting rules, Japan loctool value takes a deliveryRange\n    // param instead of minTime/maxTime\n    let deliveryRange;\n    if (isAM(minTime, maxTime)) {\n      deliveryRange = 'AM';\n    } else {\n      deliveryRange = `${minTime} - ${maxTime}`;\n    }\n    return formatMessage(messages.arriveBetweenJP, {\n      date,\n      deliveryRange,\n    });\n  }\n  return formatMessage(messages.arriveBetween, {\n    date,\n    minTime,\n    maxTime,\n  });\n}\n\nexport function FulfillmentOfferingsPreviewComponent(props) {\n  const {\n    inDeliverySection,\n    renderingJapanese,\n    isOrderConfirmation,\n    isScheduledDelivery,\n    selectedScheduledDeliveryTime,\n    shippingCompleted,\n    shouldShowScheduledDelivery,\n    shipFulfillmentDetails,\n    isMobile,\n  } = props;\n\n  const brandStyling = 'ncss-brand fs16-sm u-uppercase';\n\n  if (shippingCompleted || isOrderConfirmation) {\n    const shippingMethodProps = formatPriceOffer(shipFulfillmentDetails, isMobile);\n\n    // Delivery date information\n    let shippingDate;\n    if (shouldShowScheduledDelivery && isScheduledDelivery) {\n      shippingDate =\n        formatScheduledDeliveryShippingDate(selectedScheduledDeliveryTime, renderingJapanese);\n    }\n    return (\n      <div className=\"shippingContainer\">\n        <h3 className={`shippingHeader ${inDeliverySection ? brandStyling : ''}`}>{formatMessage(messages.shippingSpeedLabel)}</h3>\n        <p className=\"shipping-preview-method text-color-grey method-label\">\n          {shippingMethodProps.extra}\n          <br />\n          <span>\n            {!isMobile ? `${shippingMethodProps.label}` : `${formatMessage(messages.arrivesLabel)} ${shippingMethodProps.label}`}\n          </span>\n        </p>\n        {shippingDate &&\n          <p className=\"shipping-preview-date u-bold\">\n            {shippingDate}\n          </p>\n        }\n      </div>\n    );\n  }\n  return null;\n}\n\nFulfillmentOfferingsPreviewComponent.prop