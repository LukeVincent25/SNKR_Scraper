   col    ) ) count++;\n\t\t\t    if (qrCode.isDark(row + 1, col    ) ) count++;\n\t\t\t    if (qrCode.isDark(row,     col + 1) ) count++;\n\t\t\t    if (qrCode.isDark(row + 1, col + 1) ) count++;\n\t\t\t    if (count == 0 || count == 4) {\n\t\t\t\t    lostPoint += 3;\n\t\t\t    }\n\t\t    }\n\t    }\n\n\t    // LEVEL3\n\n\t    for (var row = 0; row < moduleCount; row++) {\n\t\t    for (var col = 0; col < moduleCount - 6; col++) {\n\t\t\t    if (qrCode.isDark(row, col)\n\t\t\t\t\t    && !qrCode.isDark(row, col + 1)\n\t\t\t\t\t    &&  qrCode.isDark(row, col + 2)\n\t\t\t\t\t    &&  qrCode.isDark(row, col + 3)\n\t\t\t\t\t    &&  qrCode.isDark(row, col + 4)\n\t\t\t\t\t    && !qrCode.isDark(row, col + 5)\n\t\t\t\t\t    &&  qrCode.isDark(row, col + 6) ) {\n\t\t\t\t    lostPoint += 40;\n\t\t\t    }\n\t\t    }\n\t    }\n\n\t    for (var col = 0; col < moduleCount; col++) {\n\t\t    for (var row = 0; row < moduleCount - 6; row++) {\n\t\t\t    if (qrCode.isDark(row, col)\n\t\t\t\t\t    && !qrCode.isDark(row + 1, col)\n\t\t\t\t\t    &&  qrCode.isDark(row + 2, col)\n\t\t\t\t\t    &&  qrCode.isDark(row + 3, col)\n\t\t\t\t\t    &&  qrCode.isDark(row + 4, col)\n\t\t\t\t\t    && !qrCode.isDark(row + 5, col)\n\t\t\t\t\t    &&  qrCode.isDark(row + 6, col) ) {\n\t\t\t\t    lostPoint += 40;\n\t\t\t    }\n\t\t    }\n\t    }\n\n\t    // LEVEL4\n\t    \n\t    var darkCount = 0;\n\n\t    for (var col = 0; col < moduleCount; col++) {\n\t\t    for (var row = 0; row < moduleCount; row++) {\n\t\t\t    if (qrCode.isDark(row, col) ) {\n\t\t\t\t    darkCount++;\n\t\t\t    }\n\t\t    }\n\t    }\n\t    \n\t    var ratio = Math.abs(100 * darkCount / moduleCount / moduleCount - 50) / 5;\n\t    lostPoint += ratio * 10;\n\n\t    return lostPoint;\t\t\n    }\n};\n\nmodule.exports = QRUtil;\n\n\n/***/ })\n\n});\n\n\n// WEBPACK FOOTER //\n// 0.873d7cb6c8185263ea0d.e7633b8f7.chunk.js","/* eslint-disable react/no-array-index-key */\nimport React from 'react';\nimport PropTypes from 'prop-types';\nimport SafeAnchor from './SafeAnchor';\n\n/**\n * Multiple can be open at once *\n *\n * ** TODO **\n * - CCC-425: add ids to tooltips so that a component could have multiple\n */\n\nexport default class Tooltip extends React.Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      tooltipVisible: false,\n    };\n\n    this.toggleTooltip = this.toggleTooltip.bind(this);\n    this.tooltipListener = this.tooltipListener.bind(this);\n  }\n\n  componentDidMount() {\n    document.body.addEventListener('click', this.tooltipListener, false);\n    window.addEventListener('blur', this.tooltipListener, false);\n  }\n\n  componentWillUnmount() {\n    document.body.removeEventListener('click', this.tooltipListener, false);\n    window.removeEventListener('blur', this.tooltipListener, false);\n  }\n\n  getParentClass() {\n    let className = 'tooltip d-sm-ib';\n    if (this.props.tooltipCustom === 'wide') {\n      className = `wide-tooltip ${className}`;\n    }\n    return className;\n  }\n\n  tooltipListener() {\n    const previousState = this.state.tooltipVisible;\n    /* Close tooltip if clicking anywhere on the\n     page, except another tooltip glyph toggler */\n    if (previousState === false) {\n      return;\n    }\n\n    /* change to updater funtion */\n    this.setState({\n      tooltipVisible: false,\n    });\n  }\n\n  toggleTooltip(e) {\n    if (this.state.tooltipVisible && this.props.trackTooltip) { // $SR\n      this.props.trackTooltip();\n    }\n\n    const previousState = this.state.tooltipVisible;\n\n    this.setState({\n      tooltipVisible: !previousState,\n    });\n\n    if (e) { e.preventDefault(); }\n  }\n\n  formattedTooltipContent() {\n    const tooltipContent = this.props.tooltipContent.map((content, i, contentArray) => {\n      /* No Margin bottom on last element */\n      const tooltipClasses = (contentArray.length - 1 === i) ? '' : 'mb2-sm'; // $SR\n\n      if (content.type === 'message') {\n        return <p key={`tooltip-message-${i}`} aria-label={content.text} aria-live=\"assertive\" className={tooltipClasses}>{content.text}</p>;\n      }\n\n      if (content.type === 'message-link') {\n        return (\n          <p key={`tooltip-messagelink-${i}`} aria-label={content.text} aria-live=\"assertive\" className={tooltipClasses}>\n            {content.text}&nbsp;\n            <SafeAnchor\n              href={content.href}\n              className=\"text-color-white u-underline\"\n              aria-label={content.ariaLabel}\n              target={content.target}\n            >\n              {content.link}\n            </SafeAnchor>\n          </p>\n        );\n      }\n\n      if (content.type === 'message-ccv') {\n        return (\n          <div className=\"ncss-container pl4-md\" key={`tooltip-message-${i}`}>\n            <div className=\"fs14-md\"><strong>{ content.title }</strong></div>\n            <div className=\"ncss-col-sm-12 ncss-col-md-6 pl0-md\">\n              <div className=\"pt4-md\">\n                { content.typeOne }\n              </div>\n              <p className=\"fs10-md\" aria-live=\"assertive\">\n                { content.typeOneText }\n                <img src={content.typeOneImg} alt=\"visaCard\" className=\"pt4-md\" />\n              </p>\n            </div>\n            <div className=\"ncss-col-sm-12 ncss-col-md-6 pr0-md\">\n              <div className=\"pt4-md\">\n                { content.typeTwo }\n              </div>\n              <p className=\"fs10-md\" aria-live=\"assertive\">\n                { content.typeTwoText }\n                <img src={content.typeTwoImg} alt=\"amexCard\" className=\"pt4-md\" />\n              </p>\n            </div>\n          </div>\n        );\n      }\n\n      if (content.type === 'list') {\n        return (\n          <div className={'fs12-sm lh12-sm'}>\n            <p>{content.message}</p>\n            <ul className={'custom-list-style pl6-sm'}>\n              {content.list.map(msg => <li key={`tooltip-${msg}`}>{msg}</li>)